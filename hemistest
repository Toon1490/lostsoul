	TweenAnimate(RH,{C0=cf(1.1,0.1,-0.7)*angles(math.rad(-10),math.rad(-10),math.rad(-5))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
