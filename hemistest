						local eg = TweenAnimate(RootJoint,{C0=cf(0+0*Sine,-2.4+0*Sine,0+0*Sine)*angles(math.rad(92.5+-2.5*Sine),math.rad(0+0*Sine),math.rad(0+0*Sine))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
						TweenAnimate(LH,{C0=cf(-0.8+0*Sine,-0.8+0*Sine,-0.2+0*Sine)*angles(math.rad(-15.8+2.3*Sine),math.rad(4.7+0.9*Sine),math.rad(-20.1+0.2*Sine))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
						TweenAnimate(RH,{C0=cf(1+0*Sine,-0.9+0*Sine,0+0*Sine)*angles(math.rad(-7.5+2.5*Sine),math.rad(-5+-0.4*Sine),math.rad(9.6+-0.2*Sine))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
						TweenAnimate(LW,{C0=cf(-1.3+0*Sine,0.9+0*Sine,0+0*Sine)*angles(math.rad(-71.6+0.5*Sine),math.rad(23.9+2.2*Sine),math.rad(-76.7+1*Sine))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
						TweenAnimate(RW,{C0=cf(1.4+-0*Sine,0.8+0*Sine,-0+0*Sine)*angles(math.rad(-59.6+1.3*Sine),math.rad(16.1+-2*Sine),math.rad(57.1+0.7*Sine))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
						TweenAnimate(Torso.Neck,{C0 =cf(0+0*Sine,0+0*Sine,0.1+-0*Sine)*angles(math.rad(-2.5+7.5*Sine),math.rad(0+0*Sine),math.rad(0+0*Sine))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					end
				end

			elseif torvel>2 and torvel<32 and (hitfloor~=nil or moves["gliding"]) then
				Anim="Walk"
				moves["jumped"] = 0
				if getModeName() == "Indolent" then -- indolent
					local Alpha = .1
					TweenAnimate(RootJoint,{C0=cf(0,0 + .1 * math.cos(sine/34),0)*angles(math.rad(-sine*root.Velocity.magnitude/5),math.rad(0),math.rad(0))*RootCF},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LH,{C0=cf(-1,-1 - .1 * math.cos(sine/34),0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RH,{C0=cf(1,-1 - .1 * math.cos(sine/34),0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LW,{C0=cf(-1.2,1 - .1 * math.cos(sine/30),0)*angles(math.rad(0),math.rad(0),math.rad(-80))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RW,{C0=cf(1.2,1 - .1 * math.cos(sine/30),0)*angles(math.rad(0),math.rad(0),math.rad(80))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(Torso.Neck,{C0 =cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(0))*necko},Alpha,'Linear','Out',0,false,0)
				elseif (typeof(Mode) == typeof({"bruh"}) and Mode["newdata"] == true and Mode["flying"]) then
					TweenAnimate(RootJoint,{C0=RootCF*cf(0 - 0.15 * math.cos(sine / 47),0,(tonumber(Mode["flying"]) or 2) + (tonumber(Mode["flying"]) or 2)/2 * math.cos(sine / 45))*angles(math.rad(20*Vec.Z),math.rad(-16*Vec.X - root.RotVelocity.Y),math.rad(2*Vec.X - root.RotVelocity.Y *4.5 + 3 * math.cos(sine / 47)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LH,{C0=cf(-1,-1,0)*angles(math.rad(-20 - 8 * math.sin(sine/37)*Vec.Z),math.rad(0),math.rad(0))*angles(math.rad(0 - 4 * math.cos(sine/46)*Vec.Z),math.rad(-90),math.rad(6*Vec.X))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RH,{C0=cf(1,-0.3,-0.7)*angles(math.rad(-20 - 7 * math.cos(sine/46)*Vec.Z),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(5*Vec.X))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LW,{C0=cf(-1.5,0.7,0.1)*angles(math.rad(-11.1*Vec.Z),math.rad(8.5),math.rad(-23.5- 5 * math.cos(sine/42)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RW,{C0=cf(1.5,0.7,0)*angles(math.rad(-11.1*Vec.Z),math.rad(-11.1),math.rad(17.2- 6 * math.cos(sine/45)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(Torso.Neck,{C0 =necko*cf(0,0,0)*angles(math.rad(-20*Vec.Z),math.rad(5*Vec.X),math.rad(2*Vec.X))},Alpha,'Linear','Out',0,false,0)
				else
					local Alpha,Easing,Direction,Repeat,Reverse,Delay = .1,'Linear','Out',0,false,0
					TweenAnimate(RootJoint,{C0=cf(0,.07-.05*math.cos(sine/7),0)*angles(math.rad(-15*Vec.Z),math.rad(-7*math.sin(sine/14)),math.rad(-13*Vec.X))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(LH,{C0=cf(-1,-1-.13*math.sin(sine/14),-.05+.15*math.sin(sine/14)*Vec.Z)*angles(math.rad(30*math.cos(sine/14)*Vec.Z),math.rad(-2*math.cos(sine/14)),math.rad(20*math.cos(sine/14)*Vec.X))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(RH,{C0=cf(1,-1+.13*math.sin(sine/14),-.05-.15*math.sin(sine/14)*Vec.Z)*angles(math.rad(-30*math.cos(sine/14)*Vec.Z),math.rad(2*math.cos(sine/14)),math.rad(-20*math.cos(sine/14)*Vec.X))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(LW,{C0=cf(-1.5,0.5,0)*angles(math.rad(-30*math.cos(sine/14)*Vec.Z),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(RW,{C0=cf(1.5,0.5,0)*angles(math.rad(30*math.cos(sine/14)*Vec.Z),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(Torso.Neck,{C0 =necko*cf(0,0,0)*angles(math.rad(2),math.rad(0),math.rad(7*math.sin(sine/14)-40*Vec.X))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				end		
			elseif torvel>=32 and (hitfloor~=nil or moves["gliding"]) then
				Anim="Run"
				moves["jumped"] = 0
				if getModeName() == "Indolent" then -- indolent
					local Alpha = .1
					TweenAnimate(RootJoint,{C0=cf(0,0 + .1 * math.cos(sine/34),0)*angles(math.rad(-sine*root.Velocity.magnitude/3),math.rad(0),math.rad(0))*RootCF},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LH,{C0=cf(-1,-1 - .1 * math.cos(sine/34),0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RH,{C0=cf(1,-1 - .1 * math.cos(sine/34),0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LW,{C0=cf(-1.2,1 - .1 * math.cos(sine/30),0)*angles(math.rad(0),math.rad(0),math.rad(-80))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RW,{C0=cf(1.2,1 - .1 * math.cos(sine/30),0)*angles(math.rad(0),math.rad(0),math.rad(80))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(Torso.Neck,{C0 =cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(0))*necko},Alpha,'Linear','Out',0,false,0)
				elseif (typeof(Mode) == typeof({"bruh"}) and Mode["newdata"] == true and Mode["flying"]) or Mode == 1 or Mode == 3 or Mode == 12 or Mode == 12.5 or Mode == 13 or Mode == 14 or Mode == 16 or Mode == 17 or Mode == 8 or Mode == 21  or Mode == 995--[[or Mode == 70051]] or Mode == "You are pretty useless." or Mode == 24 or Mode == 15 or Mode == 28  or Mode == 29 or Mode == 55 or Mode == 56 or Mode == 57 or Mode == 103 or Mode == 101 or Mode == 500 or (typeof(Mode) == typeof({"bruh"}) and (Mode["idle"] == 2 or Mode["idle"] == 4)) then -- Flying
					local Alpha = .1
					local Mode = Mode
					if typeof(Mode) ~= typeof({"bruh"}) then
						Mode = {flying=2}
					end
					TweenAnimate(RootJoint,{C0=RootCF*cf(0 - 0.15 * math.cos(sine / 47),0,(tonumber(Mode["flying"]) or 2) + (tonumber(Mode["flying"]) or 2)/2 * math.cos(sine / 45))*angles(math.rad(55*Vec.Z),math.rad(-16*Vec.X - root.RotVelocity.Y),math.rad(2*Vec.X - root.RotVelocity.Y *4.5 + 3 * math.cos(sine / 47)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LH,{C0=cf(-1,-1,0)*angles(math.rad(-20 - 8 * math.sin(sine/37)*Vec.Z),math.rad(0),math.rad(0))*angles(math.rad(0 - 4 * math.cos(sine/46)*Vec.Z),math.rad(-90),math.rad(6*Vec.X))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RH,{C0=cf(1,-0.3,-0.7)*angles(math.rad(-20 - 7 * math.cos(sine/46)*Vec.Z),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(5*Vec.X))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LW,{C0=cf(-1.5,0.7,0.1)*angles(math.rad(-16.1*Vec.Z),math.rad(8.5),math.rad(-23.5- 5 * math.cos(sine/42)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RW,{C0=cf(1.5,0.7,0)*angles(math.rad(-16.1*Vec.Z),math.rad(-11.1),math.rad(17.2- 6 * math.cos(sine/45)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(Torso.Neck,{C0 =necko*cf(0,0,0)*angles(math.rad(-55*Vec.Z),math.rad(5*Vec.X),math.rad(2*Vec.X))},Alpha,'Linear','Out',0,false,0)

				elseif Mode == 50 then -- lol upside down maelstrom flying IS NECESSARY
					local Alpha = .1
					TweenAnimate(RootJoint,{C0=RootCF*cf(0 - 0.15 * math.cos(sine / 47),0,2.3 + 1 * math.cos(sine / 45))*angles(math.rad(240),math.rad(0 - root.RotVelocity.Y),math.rad(0 - root.RotVelocity.Y *4.5 + 3 * math.cos(sine / 47)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LH,{C0=cf(-1,-1,0)*angles(math.rad(0 - 8 * math.sin(sine/37)),math.rad(0),math.rad(0))*angles(math.rad(0 - 4 * math.cos(sine/46)),math.rad(-90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RH,{C0=cf(1,-0.3,-0.7)*angles(math.rad(-13.8 - 7 * math.cos(sine/46)),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LW,{C0=cf(-1.5,0.7,0.1)*angles(math.rad(-3.1),math.rad(8.5),math.rad(-23.5- 5 * math.cos(sine/42)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RW,{C0=cf(1.5,0.7,0)*angles(math.rad(-16.1),math.rad(-11.1),math.rad(17.2- 6 * math.cos(sine/45)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(Torso.Neck,{C0 =cf(0,0.2,-0.6)*angles(math.rad(35.2),math.rad(0),math.rad(0))*necko},Alpha,'Linear','Out',0,false,0)

				elseif Mode == 2 then
					local Alpha = .1
					TweenAnimate(RootJoint,{C0=RootCF*cf(0 - .15 * math.cos(sine / 47),0,0.2 + 1 * math.cos(sine / 45))*angles(math.rad(70),math.rad(0 - root.RotVelocity.Y),math.rad(0 - root.RotVelocity.Y *4.5 + 3 * math.cos(sine / 47)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LH,{C0=cf(-1,-1,0)*angles(math.rad(0 - 8 * math.sin(sine/37)),math.rad(0),math.rad(0))*angles(math.rad(0 - 4 * math.cos(sine/46)),math.rad(-90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RH,{C0=cf(1,-0.3,-0.7)*angles(math.rad(-13.8 - 7 * math.cos(sine/46)),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LW,{C0=cf(-1.5,0.7,0.1)*angles(math.rad(-3.1),math.rad(8.5),math.rad(-23.5- 5 * math.cos(sine/42)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RW,{C0=cf(1.5,0.7,0)*angles(math.rad(-16.1),math.rad(-11.1),math.rad(17.2- 6 * math.cos(sine/45)))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(Torso.Neck,{C0 =cf(0,0.2,-0.6)*angles(math.rad(35.2),math.rad(0),math.rad(0))*necko},Alpha,'Linear','Out',0,false,0)
				else
					local Alpha,Easing,Direction,Repeat,Reverse,Delay = .1,'Linear','Out',0,false,0
					TweenAnimate(RootJoint,{C0=cf(0,.11-.08*math.cos(sine/3),0)*angles(math.rad(-15*Vec.Z),math.rad(-10*math.sin(sine/6)),math.rad(-13*Vec.X))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(LH,{C0=cf(-1,-1-.37*math.sin(sine/6),-.07+.23*math.sin(sine/6)*Vec.Z)*angles(math.rad(60*math.cos(sine/6)*Vec.Z),math.rad(-2*math.cos(sine/6)),math.rad(20*math.cos(sine/6)*Vec.X))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(RH,{C0=cf(1,-1+.37*math.sin(sine/6),-.07-.23*math.sin(sine/6)*Vec.Z)*angles(math.rad(-60*math.cos(sine/6)*Vec.Z),math.rad(2*math.cos(sine/6)),math.rad(-20*math.cos(sine/6)*Vec.X))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(LW,{C0=cf(-1.5,0.5,0)*angles(math.rad(-50*math.cos(sine/6)*Vec.Z),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(RW,{C0=cf(1.5,0.5,0)*angles(math.rad(50*math.cos(sine/6)*Vec.Z),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(Torso.Neck,{C0 =necko*cf(0,0,0)*angles(math.rad(2),math.rad(0),math.rad(10*math.sin(sine/6)-40*Vec.X))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				end
			end
			modet1B.UIGradient.Offset = Vector2.new(0, math.sin(sine/64)*2)
			modet2B.UIGradient.Offset = Vector2.new(0, math.cos(-sine/32))
		end
	end) 
end)
