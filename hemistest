local converts = [[CREDITS TO EVERY DEVS OF HEMISPHERIC GLITCHER
(read "Credits" tab)

Converted version by Auxphe_ra

I also optimized it by removing ground effects if reduce effects is enabled
and a option called "GUI ScreenInsets". That will change the UI layout (ScreenInsets) after you press Activate.

CAUTION : if you wanna use this in a FD or CR game, avoid using these wings because it has union parts, which others cannot see it:
	+ Default

- 30/06/2025

]]

local ohno = Instance.new("ColorCorrectionEffect",workspace.CurrentCamera)

function LocalTweenyThing(object,Aniim,time,easingstyle,easingdirection,repet,reverse,deelay)
	if object == "camera" then
		object = workspace.CurrentCamera
	end
	local infos=TweenInfo.new(time or (20/60),Enum.EasingStyle[easingstyle or "Linear"],Enum.EasingDirection[easingdirection or "InOut"],repet or 0,reverse or false,deelay or 0)
	local tween=game:GetService("TweenService"):Create(object,infos,Aniim)
	tween:Play()
	return tween
end

function LocalProperties(Object,Property,Edit)
	if Object == "camera" then
		Object = workspace.CurrentCamera
	elseif Object == "player" then
		Object = game.Players.LocalPlayer
	end
	if Edit ~= nil then
		Object[Property] = Edit
	end
	return Object[Property]
end

local beans = false

function Funmy(Object,Anim)
	beans = true
	coroutine.resume(coroutine.create(function()
		LocalTweenyThing(workspace.CurrentCamera,{FieldOfView=40},4,"Back","In")
		local hek = LocalTweenyThing(ohno,{Brightness=1.2,Contrast=.5},4,"Quint","In")
		hek.Completed:connect(function()
			LocalTweenyThing(ohno,{Brightness=0,Contrast=0},1,"Quint","Out")
			local e = LocalTweenyThing(workspace.CurrentCamera,{FieldOfView=70},1,"Quad","Out")
			e.Completed:connect(function()
				beans = false
			end)
		end)
	end))
end

checkboxes = {}

local function setupGui(datas,isDev)
	local gui = hemis.LocalScript.this:Clone()
	checkboxes = {
		debugg=datas:FindFirstChild("debug") or false,
		csd=datas:FindFirstChild("csd") or true,
		music=datas:FindFirstChild("music") or true,
		ldm=datas:FindFirstChild("ldm") or false,
		chare=datas:FindFirstChild("chare") or "Avatar",
		exec=datas:FindFirstChild("exec") or true,
		colroes=datas:FindFirstChild("colroes") or false,
		wing=datas:FindFirstChild("wing") or "Default",
		gsize=datas:FindFirstChild("gsize") or 'CoreUISafeInsets',
	}
	local function checkbox(name,index,options)
		local eg = gui.frameo.mainFrame.settings.ScrollingFrame.temp:Clone()
		eg.TextLabel.Text = name
		if not options then
			if checkboxes[index] == true then
				eg.TextButton.Text = "X"
			else
				eg.TextButton.Text = ""
			end
			eg.TextButton.MouseButton1Click:connect(function()
				if checkboxes[index] == true then
					checkboxes[index] = false
					eg.TextButton.Text = ""
				else
					checkboxes[index] = true
					eg.TextButton.Text = "X"
				end
			end)
		else
			eg.TextButton.Text = checkboxes[index]
			eg.TextLabel.Size = UDim2.new(0.55,-6,0,30)
			eg.TextButton.Size = UDim2.new(0,68,0,34)
			local selected = table.find(options,checkboxes[index]) or 1
			eg.TextButton.MouseButton1Click:connect(function()
				selected = selected + 1
				if selected > #options then
					selected = 1
				end
				checkboxes[index] = options[selected]
				eg.TextButton.Text = options[selected]
			end)
		end
		eg.Position = UDim2.new(0,0,0,40*(#gui.frameo.mainFrame.settings.ScrollingFrame:GetChildren()-1))
		eg.Visible = true
		gui.frameo.mainFrame.settings.ScrollingFrame.CanvasSize = UDim2.new(0,0,0,40*#gui.frameo.mainFrame.settings.ScrollingFrame:GetChildren())
		eg.Parent = gui.frameo.mainFrame.settings.ScrollingFrame
	end
	checkbox("Music (L)","music")
	checkbox("Colorshift (0)","csd")
	checkbox("Reduce Effects (9)","ldm")
	checkbox("Wing Style (8)","wing",{"Default","Stratospheric","Star","Old","Thin","Tiny Old","None"})
	checkbox("Show Hitboxes (6)","debugg")
	checkbox("Executions","exec")
	checkbox("Weirder Colors","colroes")
	checkbox("GUI ScreenInsets", "gsize", {"CoreUISafeInsets","DeviceSafeInsets","None"})
	local option = {"Avatar","Cytus","Emi","RCE Cytus"}
	if game.Players.LocalPlayer.Name == "Zombieza4569" or game.Players.LocalPlayer.Name == "Zombieza4569" then
		table.insert(option,"Vida")
		table.insert(option,"Moai")
		table.insert(option,"Aero") -- for testing purposes
		table.insert(option,"Emi") -- also for testing purposes
	elseif game.Players.LocalPlayer.Name == "Zombieza4569" then
		table.insert(option,"Vida")
	elseif game.Players.LocalPlayer.Name == "Zombieza4569" then
		table.insert(option,"Aero")
	end
	if isDev then
		table.insert(option,"Phantasma")
	end
	checkbox("Character","chare",option)
	LocalTweenyThing(gui.frameo.Frame.UIGradient,{Rotation=-90},3,"Sine","InOut",-1,false,0)
	gui.frameo.mainFrame.text1.ScrollingFrame.TextLabel.Text = changelog
	gui.frameo.mainFrame.text1.changelog.MouseButton1Click:connect(function()
		gui.frameo.mainFrame.text1.ScrollingFrame.TextLabel.Text = changelog
	end)
	gui.frameo.mainFrame.text1.lore.MouseButton1Click:connect(function()
		gui.frameo.mainFrame.text1.ScrollingFrame.TextLabel.Text = lore
	end)
	gui.frameo.mainFrame.text1.credits.MouseButton1Click:connect(function()
		gui.frameo.mainFrame.text1.ScrollingFrame.TextLabel.Text = credits
	end)
	gui.frameo.mainFrame.activate.MouseButton1Click:connect(function()
		gui.frameo.mainFrame.activate.Visible = false
		end)
	if gui.frameo.mainFrame.text1:FindFirstChild("conv") then
		gui.frameo.mainFrame.text1.conv.MouseButton1Click:Connect(function()
			gui.frameo.mainFrame.text1.ScrollingFrame.TextLabel.Text = converts
		end)
	end
	gui.Parent = game.Players.LocalPlayer.PlayerGui
	return gui
end

function Funmeye(datas,isDev) -- intro and start gui
	local music = Instance.new("Sound",game.Players.LocalPlayer.Character)
	music.Volume = -.1
	music.Looped = true
	music.TimePosition = 36
	music.SoundId = "rbxassetid://4953966210"
	music:Play()
	LocalTweenyThing(music,{Volume=0.1},2,"Quint","In")
	local gui = setupGui(datas,isDev)
	local keyHeck
	game.Players.LocalPlayer:GetMouse().KeyDown:connect(function(k)
		if k == keyHeck and k ~= "w" and k ~= "a" and k ~= "s" and k ~= "d" then -- alternate activation method
			gui.frameo.mainFrame.activate.Visible = false
		end
		keyHeck = k
	end)
	gui.frameo.Position = UDim2.new(0,0,-0.5,0)
	LocalTweenyThing(gui.frameo,{Position=UDim2.new(0,0,0,0)},0.5,"Back","Out")
	repeat game:GetService("RunService").Heartbeat:wait() until gui.frameo.mainFrame.activate.Visible == false
	checkboxes["starter"] = keyHeck
	music:Destroy()
	local twen = LocalTweenyThing(gui.frameo,{Position=UDim2.new(0,0,1,0)},0.5,"Sine","In")
	twen.Completed:connect(function()
		gui.Enabled = false
	end)
	return checkboxes
end

local Player = players.LocalPlayer

local Char = Player.Character

repeat wait() until Char:FindFirstChild("cl1val") and Char:FindFirstChild("cl2val") and Char.Head:FindFirstChild("bgutest") and Char:FindFirstChild("musicval") and Char:FindFirstChild("musicobj")
local ui = hemis.LocalScript.ui:Clone()
ui.Parent = Player.PlayerGui

local modet,cl1val,cl2val,musicval,musicobj = Char.Head:FindFirstChild("bgutest"),Char:FindFirstChild("cl1val"),Char:FindFirstChild("cl2val"),Char:FindFirstChild("musicval"),Char:FindFirstChild("musicobj")

local input = function(io,a)
	if a then return end
	local io = {KeyCode=io.KeyCode,UserInputType=io.UserInputType,UserInputState=io.UserInputState}
	Event(io)
end
userInputService.InputBegan:Connect(input)
userInputService.InputEnded:Connect(input)
local Changed = false
local Mouse = Player:GetMouse()
local h,t = Mouse.Hit,Mouse.Target

local sine = 0

game:GetService("RunService").RenderStepped:connect(function()
	if h~=Mouse.Hit or t~=Mouse.Target then
		Event({isMouse=true,Target=Mouse.Target,Hit=Mouse.Hit})
		h,t=Mouse.Hit,Mouse.Target
	end

	-- stupid ui
	pcall(function() -- so i don't crash the script with the stupid ui
		ui.main.modename.Text = modet.Namer.Text
		ui.main.music.Text = musicval.Value
		for i,v in pairs(ui.main:GetDescendants()) do
			if v.Name == "cl1" or v.Name == "cl1s" then
				v.ImageColor3 = cl1val.Value
			elseif v.Name == "cl2" or v.Name == "cl2s" then
				v.ImageColor3 = cl2val.Value
			elseif v.Name == "cl3" or v.Name == "cl3s" then
				v.ImageColor3 = cl1val.Value:lerp(cl2val.Value,0.5)
			end
			if string.sub(v.Name,4) == "s" then
				v.Rotation = v.Rotation - 4
				if modet.Namer.Text == " MAELSTROM " then
					v.Rotation = v.Rotation + 28
				end
			end
		end
		ui.main.spinnycircle.Rotation = ui.main.spinnycircle.Rotation + 2
		if not beans then
			workspace.CurrentCamera.FieldOfView = 70
			ohno.Contrast = 0
			ohno.Saturation = 0
			ohno.Brightness = 0
			ohno.TintColor = Color3.new(1,1,1) -- laggi
			--[[if modet.Namer.Text == " MAELSTROM " then
				ui.main.spinnycircle.Rotation = ui.main.spinnycircle.Rotation - 14
				workspace.CurrentCamera.FieldOfView = 70 - (musicobj.Value.PlaybackLoudness/50)
				ohno.Contrast = musicobj.Value.PlaybackLoudness/750
				ohno.Saturation = musicobj.Value.PlaybackLoudness/750
				ohno.TintColor = Color3.new(1,1-musicobj.Value.PlaybackLoudness/1000,1-musicobj.Value.PlaybackLoudness/1000)
			elseif modet.Namer.Text == " RYTHMIC " then
			workspace.CurrentCamera.FieldOfView = 70 - musicobj.Value.PlaybackLoudness/50
				ohno.Brightness = musicobj.Value.PlaybackLoudness/2000
				ohno.TintColor = Color3.fromHSV(sine/45 - math.floor(sine/45),musicobj.Value.PlaybackLoudness/2000,1)
			end]]
		end
		local beans,bean,be = Color3.toHSV(cl1val.Value)
		local beans2,bean2,be2 = Color3.toHSV(cl2val.Value)
		if be > 0.8 and bean < 0.3 then
			ui.main.modename.TextColor3 = Color3.new(0,0,0)
			ui.main.music.TextColor3 = Color3.new(0,0,0)
			ui.main.divider.BackgroundColor3 = Color3.new(0,0,0)
		else
			ui.main.modename.TextColor3 = Color3.new(1,1,1)
			ui.main.music.TextColor3 = Color3.new(1,1,1)
			ui.main.divider.BackgroundColor3 = Color3.new(1,1,1)
		end
		if be2 > 0.8 and bean2 < 0.3 then
			ui.main.TextLabel.TextColor3 = Color3.new(0,0,0)
			ui.main.TextLabel2.TextColor3 = Color3.new(0,0,0)
		else
			ui.main.TextLabel.TextColor3 = Color3.new(1,1,1)
			ui.main.TextLabel2.TextColor3 = Color3.new(1,1,1)
		end
		sine = sine + 1
	end)
end)




end)



















local egg = spat

-- Artificial Heartbeat --
local ArtificialHB = Instance.new("BindableEvent")
ArtificialHB.Name = "Heartbeat"

local tf = 0
local allowframeloss = false
local tossremainder = false
local lastframe = tick()
local frame = 1/60
ArtificialHB:Fire()

game:GetService("RunService").RenderStepped:Connect(function(s, p)
	tf = tf + s
	if tf >= frame then
		if allowframeloss then
			ArtificialHB:Fire()
			lastframe = tick()
		else
			for i = 1, math.floor(tf / frame) do
				ArtificialHB:Fire()
			end
			lastframe = tick()
		end
		if tossremainder then
			tf = 0
		else
			tf = tf - frame * math.floor(tf / frame)
		end
	end
end)

function swait(num)
	if num == 0 or num == nil then
		ArtificialHB.Event:wait()
	else
		for i = 0, num do
			ArtificialHB.Event:wait()
		end
	end
end
-- End of Artificial Heartbeat --

function Whatever(func,...)
	egg[func](swait,...)
end



















































