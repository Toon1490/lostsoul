function emi(char)
	local corruption = resources.characters.Emi:Clone()
	local m = Instance.new("Model",char)
	m.Name = "Emi"
	local mc1 = Instance.new("Model",m)
	mc1.Name = "mrc1"
	local mc2 = Instance.new("Model",m)
	mc2.Name = "mrc2"
	local acc = Instance.new("Model",m)
	acc.Name = "acc"
	for i,v in pairs(char:GetChildren()) do
		if v:IsA("BasePart") then
			v.Transparency = 1
		elseif v.ClassName == "Accessory" then
			v:Destroy()
		end
	end
	local o = corruption.hed:Clone()
	CreateWeld(char.Head,o.it,char.Head,0,-0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = corruption.tors:Clone()
	CreateWeld(char.Torso,o.it,char.Torso,0,-0.05,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = corruption.larm:Clone()
	CreateWeld(char["Left Arm"],o.it,char["Left Arm"],0,-0.05,0,math.rad(0),math.rad(180),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = corruption.rarm:Clone()
	CreateWeld(char["Right Arm"],o.it,char["Right Arm"],0,-0.05,0,math.rad(0),math.rad(180),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = corruption.lleg:Clone()
	CreateWeld(char["Left Leg"],o.it,char["Left Leg"],0,0.25,0,math.rad(0),math.rad(-90),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = corruption.rleg:Clone()
	CreateWeld(char["Right Leg"],o.it,char["Right Leg"],0,.25,0,math.rad(0),math.rad(90),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	for i,v in pairs(m:GetDescendants()) do
		if v:IsA("BasePart") then
			pcall(function()
				v.Massless = true
				v.CanCollide = false
				v.UsePartColor = true
			end)
			if v.Name == "mrc1" then
				v.Parent = mc1
			else
				v.Parent = m
			end
		end
	end
end

local clone = {}

function moaim(char,it)
	local moai = (it or resources.characters.Moai):Clone()
	if char:FindFirstChild("Body Colors") then char:FindFirstChild("Body Colors"):Destroy() end -- body colors dumpy
	wait() -- this gives me pain
	for i,v in pairs(char:GetChildren()) do
		if v.ClassName ~= "Part" and v.ClassName ~= "Humanoid" and v.ClassName ~= "Script" and v.ClassName ~= "LocalScript" and v.ClassName ~= "RemoteEvent" and v.ClassName ~= "RemoteFunction" then
			v:Destroy()
		elseif v.ClassName == "Part" then
			if v:FindFirstChildOfClass("Decal") then
				v:FindFirstChildOfClass("Decal"):Destroy()
			end
			if moai:FindFirstChild(v.Name) then
				local it = moai:FindFirstChild(v.Name)
				v.Color = it.Color
				for f,h in pairs(it:GetChildren()) do
					local eg = h:Clone()
					if eg:IsA("BasePart") then
						eg.CFrame = v.CFrame * (it.CFrame:ToObjectSpace(h.CFrame))
					end
					if eg:FindFirstChildOfClass("WeldConstraint") then
						local wel = eg:FindFirstChildOfClass("WeldConstraint")
						if wel.Part0 == it then
							wel.Part0 = v
						elseif wel.Part1 == it then
							wel.Part1 = v
						end
					end
					eg.Parent = v
					if eg.Name == "glower" then eg.Parent = workspace table.insert(clone,eg) end
				end
			end
		end
	end
	for i,v in pairs(moai:GetChildren()) do
		if v.ClassName == "CharacterMesh" or v.ClassName == "Shirt" or v.ClassName == "Pants" or v.ClassName == "ShirtGraphic" then
			v:Clone().Parent = char
		end
	end
end

-- end moai's stupid effects

