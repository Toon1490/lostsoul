		TweenAnimate(RW,{C0=cf(1.3,0.4,0.5)*angles(math.rad(109.8),math.rad(-3.2),math.rad(101.3))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0,0.1,-0.4)*angles(math.rad(25),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
