				Transparency = 0.5,
				Material="Neon",
				CFrame = root.CFrame
			},{ -- decal start properties
				Color3 = Color3.new((MAINRUINCOLOR.R*50)^2.5,(MAINRUINCOLOR.G*50)^2.5,(MAINRUINCOLOR.B*50)^2.5),
				Transparency = .5
			},{ -- part end properties
				Size=Vector3.new(15,15,15),
				Transparency = 1,
				Color=MAINRUINCOLOR2,
				CFrame = root.CFrame
			},{ -- decal end properties
				Transparency = 1
			},20/60,"Elastic","InOut")
			for i = 1,3 do
				weirdeff("VBLASTMESH2",{ -- part start properties
					Color=MAINRUINCOLOR,
					Transparency = .05,
					Material="Neon",
					CFrame = targetted.CFrame*randr()
				},{ -- mesh start properties
					VertexColor = Vector3.new(MAINRUINCOLOR2.R*5,MAINRUINCOLOR2.G*5,MAINRUINCOLOR2.B*5),
