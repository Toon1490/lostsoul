--[[Made by Aero, KnottyEeveez and moai]]--

-- Changelog [3/26/2020] -- 

pcalle = pcall

local caughterrors = {}

function pcallo(dd)
	local eje,errorr = pcalle(dd)
	if not eje and not caughterrors[errorr] then
		print(errorr)
		caughterrors[errorr] = true
	end
	return eje,errorr
end

pcall = pcallo

Developers = {} -- =trues are for Developers[plr.Name] (checking if player is a dev)

local Player = players.LocalPlayer
local Mouse,mouse,UserInputService,ContextActionService
--do
	local CAS = {Actions={}}
	local fakeEvent = function()
		local t = {_fakeEvent=true}
		t.Connect = function(self,f)self.Function=f end
		t.connect = t.Connect
		return t
	end
	local m = {Target=nil,Hit=CFrame.new(),KeyUp=fakeEvent(),KeyDown=fakeEvent(),Button1Up=fakeEvent(),Button1Down=fakeEvent()}
	local UIS = {InputBegan=fakeEvent(),InputEnded=fakeEvent()}
	function CAS:BindAction(name,fun,touch,...)
		CAS.Actions[name] = {Name=name,Function=fun,Keys={...}}
	end
	function CAS:UnbindAction(name)
		CAS.Actions[name] = nil
	end
	local function te(self,ev,...)
		local t = m[ev]
		if t and t._fakeEvent and t.Function then
			t.Function(...)
		end
	end
	m.TrigEvent = te
	UIS.TrigEvent = te
	Mouse,mouse,UserInputService,ContextActionService = m,m,UIS,CAS
--end

function Event(io)
	if io.isMouse then
		m.Target = io.Target
		m.Hit = io.Hit
	elseif io.UserInputType == Enum.UserInputType.MouseButton1 then
		if io.UserInputState == Enum.UserInputState.Begin then
			m:TrigEvent("Button1Down")
		else
			m:TrigEvent("Button1Up")
		end
	else
		for n,t in pairs(CAS.Actions) do
			for _,k in pairs(t.Keys) do
				if k==io.KeyCode then
					t.Function(t.Name,io.UserInputState,io)
				end
			end
		end
		if io.UserInputState == Enum.UserInputState.Begin then
			m:TrigEvent("KeyDown",io.KeyCode.Name:lower())
			UIS:TrigEvent("InputBegan",io,false)
		else
			m:TrigEvent("KeyUp",io.KeyCode.Name:lower())
			UIS:TrigEvent("InputEnded",io,false)
		end
	end
end

-- Labelling What is What. -- //

Pl = players.LocalPlayer
Player = Pl
Plr = Pl
C = Pl.Character
Hum = C.Humanoid
CFR = CFrame.new

function CreateWeld(parent,part0,part1,C1X,C1Y,C1Z,C1Xa,C1Ya,C1Za,C0X,C0Y,C0Z,C0Xa,C0Ya,C0Za)
	local weld = Instance.new("Motor6D")
	weld.Parent = parent
	weld.Part0 = part0
	weld.Part1 = part1
	weld.C1 = CFrame.new(C1X,C1Y,C1Z)*CFrame.Angles(C1Xa,C1Ya,C1Za)
	weld.C0 = CFrame.new(C0X,C0Y,C0Z)*CFrame.Angles(C0Xa,C0Ya,C0Za)
	return weld
end

function CreateWeldS(parent,part0,part1,C0,C1)
	local weld = Instance.new("Motor6D")
	weld.Part0 = part0
	weld.Part1 = part1
	weld.C1 = C1 or CFrame.new()
	weld.C0 = C0 or CFrame.new()
	weld.Parent = parent
	return weld
end

-- moai's stupid effects and variables

local paradiseIsland = resources:WaitForChild("paradise")

local effectstuff = resources:WaitForChild("effectstuff")

local NS = NumberSequence.new
local NSK = NumberSequenceKeypoint.new
local CS = ColorSequence.new
local CSK = ColorSequenceKeypoint.new
local NR = NumberRange.new

function weirdeff(name,startprop,startprop1,endprop,endprop1,timer,easing,easing2,repet,reverse,del,deb,server)
	if not server then
		Whatever("weirdeff",name,startprop,startprop1,endprop,endprop1,timer,easing,easing2,repet,reverse,del,deb)
	else
		local e = effectstuff[name]:Clone()
		for i,v in pairs(e:GetChildren()) do
			--if v:IsA("Decal") then
			for d,g in pairs(startprop1) do
				v[d] = g
			end
			--end
		end
		for d,g in pairs(startprop) do
			e[d] = g
		end
		e.Anchored = true
		e.CanCollide = false
		e.Massless = true
		e.Name = "Effect"
		e.Parent = workspace
		TweenAnimate(e,endprop,timer,easing,easing2,repet,reverse,del)
		for i,v in pairs(e:GetChildren()) do
			TweenAnimate(v,endprop1,timer,easing,easing2,repet,reverse,del)
		end
		if not deb then
			coroutine.resume(coroutine.create(function()
				swait((del or 0)*60)
				swait((timer*((repet or 0)+1))*60)
				if reverse then
					swait((timer*((repet or 0)+1))*60)
				end
				e.Transparency = 1
				swait((1)*60)
				e:Destroy()
			end))
		end
		return e
	end
end

function particle(pos,num,props,attPos) -- particles don't really need to be client side
	--Whatever("particle",pos,num,props,attPos)
	local part = pos
	if typeof(pos) == typeof(CFrame.new()) then
		part = Instance.new("Part")
		part.Anchored = true
		part.CFrame = pos
		part.Transparency = 1
		part.CanCollide = false
		part.Parent = workspace
	end
	local at = Instance.new("Attachment",part)
	at.Position = attPos or Vector3.new(0,0,0)
	local eff = Instance.new("ParticleEmitter",at)
	eff.Enabled = false
	eff.Speed = NumberRange.new(0)
	for i,v in pairs(props) do
		eff[i] = v
	end
	if num ~= 0 then
		coroutine.resume(coroutine.create(function()
			eff:Emit(num)
			wait(10)
			eff:Destroy()
		end))
	else
		eff.Enabled = true
		return eff
	end
end

function cytus(char)
	local cy = resources.characters.Cytus:Clone()
	local m = Instance.new("Folder",char)
	m.Name = "Cytus"
	local mc1 = Instance.new("Folder",m)
	mc1.Name = "mrc1"
	local mc2 = Instance.new("Folder",m)
	mc2.Name = "mrc2"
	local acc = Instance.new("Folder",m)
	acc.Name = "acc"
	for i,v in pairs(char:GetChildren()) do
		if v:IsA("BasePart") then
			v.Transparency = 1
		elseif v.ClassName == "Accessory" then
			v:Destroy()
		end
	end
	local o = cy.hed:Clone()
	CreateWeld(char.Head,o.it,char.Head,0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = cy.tors:Clone()
	CreateWeld(char.Torso,o.it,char.Torso,0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = cy.larm:Clone()
	CreateWeld(char["Left Arm"],o.it,char["Left Arm"],0,0,0,math.rad(0),math.rad(180),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = cy.rarm:Clone()
	CreateWeld(char["Right Arm"],o.it,char["Right Arm"],0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = cy.lleg:Clone()
	CreateWeld(char["Left Leg"],o.it,char["Left Leg"],0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = cy.rleg:Clone()
	CreateWeld(char["Right Leg"],o.it,char["Right Leg"],0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	for i,v in pairs(m:GetDescendants()) do
		if v:IsA("BasePart") then
			pcall(function()
				v.Massless = true
				v.CanCollide = false
				v.UsePartColor = true
			end)
			if v.Name == "mrc1" then
				v.Parent = mc1
			elseif v.Name == "mrc2" then
				v.Parent = mc2
			elseif v.Name == "acc" then
				v.Parent = acc
			end
		end
	end
end

local rcCol = {mc1={},mc2={},lc1={},lc2={},acc={}}

function RCEcytus(char)
	local cy = resources.characters.RCECytus:Clone()
	local m = Instance.new("Folder",char)
	m.Name = "RCECytus"
	for i,v in pairs(char:GetChildren()) do
		if v:IsA("BasePart") then
			v.Transparency = 1
		elseif v.ClassName == "Accessory" then
			v:Destroy()
		end
	end
	local o = cy.head:Clone()
	CreateWeld(char.Head,o.it,char.Head,0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = cy.tors:Clone()
	CreateWeld(char.Torso,o.it,char.Torso,0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = cy.larm:Clone()
	CreateWeld(char["Left Arm"],o.it,char["Left Arm"],0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = cy.rarm:Clone()
	CreateWeld(char["Right Arm"],o.it,char["Right Arm"],0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = cy.lleg:Clone()
	CreateWeld(char["Left Leg"],o.it,char["Left Leg"],0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = cy.rleg:Clone()
	CreateWeld(char["Right Leg"],o.it,char["Right Leg"],0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	for i,v in pairs(m:GetDescendants()) do
		if v:IsA("BasePart") then
			pcall(function()
				v.Massless = true
				v.CanCollide = false
				v.UsePartColor = true
			end)
			if v.Name == "neon" or v.Name == "hair" or v.Name == "outears" then
				table.insert(rcCol["mc1"],v)
			elseif v.Name == "misc" or v.Name == "innears" or v.Name == "shoe" then
				table.insert(rcCol["mc2"],v)
			elseif v.Name == "dress" or v.Name == "misc2" then
				table.insert(rcCol["acc"],v)
			elseif v.Name == "stok" then
				table.insert(rcCol["lc1"],v)
			elseif v.Name == "jacket" then
				table.insert(rcCol["lc2"],v)
			end
		end
	end
end

function vida(char)
	local corruption = resources.characters.Vida:Clone()
	local m = Instance.new("Model",char)
	m.Name = "Cytus"
	local mc1 = Instance.new("Model",m)
	mc1.Name = "mrc1"
	local mc2 = Instance.new("Model",m)
	mc2.Name = "mrc2"
	local acc = Instance.new("Model",m)
	acc.Name = "acc"
	for i,v in pairs(char:GetChildren()) do
		if v:IsA("BasePart") then
			v.Transparency = 1
		elseif v.ClassName == "Accessory" then
			v:Destroy()
		end
	end
	local o = corruption.hed:Clone()
	CreateWeld(char.Head,o.it,char.Head,0,0.05,0,math.rad(0),math.rad(-90),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = corruption.tors:Clone()
	CreateWeld(char.Torso,o.it,char.Torso,0,0,0,math.rad(-90),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(90))
	o.Parent = m
	local o = corruption.larm:Clone()
	CreateWeld(char["Left Arm"],o.it,char["Left Arm"],0,0,0,math.rad(0),math.rad(-90),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = corruption.rarm:Clone()
	CreateWeld(char["Right Arm"],o.it,char["Right Arm"],0,0,0,math.rad(0),math.rad(-90),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = corruption.lleg:Clone()
	CreateWeld(char["Left Leg"],o.it,char["Left Leg"],0,0,0,math.rad(0),math.rad(-90),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	local o = corruption.rleg:Clone()
	CreateWeld(char["Right Leg"],o.it,char["Right Leg"],0,0,0,math.rad(0),math.rad(-90),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
	o.Parent = m
	for i,v in pairs(m:GetDescendants()) do
		if v:IsA("BasePart") then
			pcall(function()
				v.Massless = true
				v.CanCollide = false
				v.UsePartColor = true
			end)
			if v.Name == "mrc1" then
				v.Parent = mc1
			elseif v.Name == "mrc2" then
				v.Parent = mc2
			elseif v.Name == "acc" then
				v.Parent = acc
			end
		end
	end
end

