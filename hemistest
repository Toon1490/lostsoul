		weirdeff("sphere",{ -- part start properties
			Size=Vector3.new(0,0,0),
			Color=cl2,
			Transparency = 0.5,
			Material="Neon",
			CFrame = root.CFrame*CFrame.new(0,-3,0)
		},{ -- decal start properties, but sphere has no decals
		},{ -- part end properties
			Size=Vector3.new(86,0,86)*(size or 1),
			Transparency = 0
