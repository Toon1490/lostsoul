			e:Destroy()
			e2:Destroy()
			local e3 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(28,28,1.5),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*angles(0,0,math.rad(179))*cf(0,0,.15)
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(23,23,1.5),
				CFrame = cfe*angles(0,0,math.rad(358))*cf(0,0,.15)
			},{ -- decal end properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = 1
			},15/60,"Sine","Out")
			local e4 = weirdeff("woosh2",{ -- part start properties
				Size=Vector3.new(28,28,1.5),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*angles(0,0,math.rad(-179))*cf(0,0,-.15)
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(23,23,1.5),
				CFrame = cfe*angles(0,0,math.rad(-358))*cf(0,0,-.15)
			},{ -- decal end properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = 1
			},15/60,"Sine","Out")
		end))
	end
	death.Color = Color3.new(1,0,0)
	swait(stats["ASPD"]*5)
	death.Color = Color3.new(0,1,0)
	swait(stats["ASPD"]*10)
	attack,idlesoff = false,false
	root.Anchored = false
	swait(60)
	death:Destroy()
end

function AeroMad()
	attack,idlesoff = true,true
	root.Anchored = true
	local cc = Instance.new("ColorCorrectionEffect",game.Lighting)
	game:service'Debris':AddItem(cc,5)
	cc.Contrast = -2
	dialog(getName(),{"No."},"player",{1},"")
	CreateSound5(4196111215,char,4,1)
	-- ~-~ AEROMAD 1 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*10/60,'Elastic','Out',0,false,0
	local eg = TweenAnimate(RootJoint,{C0=cf(0,2,0)*angles(math.rad(0),math.rad(-45),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1,-1,-0.1)*angles(math.rad(-9.8),math.rad(1.7),math.rad(-9.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(1.1,-0.5,-1)*angles(math.rad(-29.9),math.rad(2.5),math.rad(-4.3))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1.6,0.1,-0.5)*angles(math.rad(77.8),math.rad(18.3),math.rad(-28.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(1.1,1,-0.8)*angles(math.rad(118.9),math.rad(-40.1),math.rad(-65.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0.3,0.2,0.2)*angles(math.rad(-29.7),math.rad(31.3),math.rad(21.5))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(stats["ASPD"]*5)
	cc.Contrast = 0
	CreateSound5(4648407351,root,3,1.5)
	swait(Alpha*60 - 5)
	CreateSound5(4648415285,root,5,0.9)
	CreateSound5(4575206546,root,7,0.6)
	for i = 1,3 do
		weirdeff("VBLASTMESH2",{ -- part start properties
			Size=Vector3.new(10,0,10),
			Color=MAINRUINCOLOR,
			Transparency = .05,
			Material="Neon",
			CFrame = root.CFrame*CFrame.new(0,50,0)
		},{ -- mesh start properties
			VertexColor = Vector3.new(MAINRUINCOLOR2.R*5,MAINRUINCOLOR2.G*5,MAINRUINCOLOR2.B*5),
			Scale = Vector3.new(0,5,0)
		},{ -- part end properties
			Transparency = 1,
			CFrame = root.CFrame*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)*CFrame.new(0,50,0)
		},{ -- mesh end properties
			Scale = Vector3.new(5,2.5,5)*(math.random(25,100)/100)
		},math.random(40,160)/80,"Linear","Out")
	end
	weirdeff("Explosion2",{ -- part start properties
		Size=Vector3.new(50,50,50),
		Color=MAINRUINCOLOR2,
		Transparency = -2,
		Material="Neon",
		CFrame = root.CFrame*CFrame.new(0,25,0)
	},{ -- decal start properties
		Color3 = Color3.new(MAINRUINCOLOR.R*10,MAINRUINCOLOR.G*10,MAINRUINCOLOR.B*10),
		Transparency = -1
	},{ -- part end properties
		Size=Vector3.new(150,350,150),
		Transparency = 1,
		CFrame = root.CFrame*CFrame.new(0,175,0)
	},{ -- decal end properties
		Transparency = 1
	},.5,"Elastic","InOut")
	TweenAnimate(root,{CFrame=root.CFrame*cf(0,500,0)},stats["ASPD"]*9/60,"Elastic","InOut")
	swait(stats["ASPD"]*10)
	CreateSound5(4755396368,char,2,.9)
	CreateSound5(3570968988,char,2,.8)
	-- ~-~ AEROMAD 2 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*40/60,'Quad','InOut',0,false,0
	local eg = TweenAnimate(RootJoint,{C0=cf(0,2,0.2)*angles(math.rad(25),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1,-0.9,0)*angles(math.rad(-25),math.rad(20),math.rad(-10))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(0.9,-0.6,-0.9)*angles(math.rad(-39.9),math.rad(-7.4),math.rad(8.4))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-0.7,0.7,-0.8)*angles(math.rad(58.9),math.rad(54.7),math.rad(119.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(0.7,0.7,-0.8)*angles(math.rad(57.1),math.rad(-58.7),math.rad(-115.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0,0,-0.2)*angles(math.rad(10),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	local col = Color3.new(1,.1,.1)
	for i = 1,12 do
		coroutine.resume(coroutine.create(function()
			swait(stats["ASPD"]*i*4)
			local cfe = root.CFrame*randr()
			local e2 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(5,5,1.5),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,11,.15)
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = 1,
			},{ -- part end properties
				Size=Vector3.new(28,28,1.5),
				CFrame = cfe*cf(0,11,.15)*angles(0,0,math.rad(179))
			},{ -- decal end properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = -1
			},10/60,"Sine","In",nil,nil,nil,nil,true)
			local e = weirdeff("woosh2",{ -- part start properties
				Size=Vector3.new(5,5,1.5),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,11,-.15)
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = 1,
			},{ -- part end properties
				Size=Vector3.new(28,28,1.5),
				CFrame = cfe*cf(0,11,-.15)*angles(0,0,math.rad(-179))
			},{ -- decal end properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = -1
			},10/60,"Sine","In",nil,nil,nil,nil,true)
			swait(10)
			e:Destroy()
			e2:Destroy()
			local e3 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(28,28,1.5),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,11,.15)*angles(0,0,math.rad(179))
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(23,23,1.5),
				CFrame = cfe*cf(0,11,.15)*angles(0,0,math.rad(358))
			},{ -- decal end properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = 1
			},10/60,"Sine","Out")
			local e4 = weirdeff("woosh2",{ -- part start properties
				Size=Vector3.new(28,28,1.5),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,11,.15)*angles(0,0,math.rad(-179))
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(23,23,1.5),
				CFrame = cfe*cf(0,11,.15)*angles(0,0,math.rad(-358))
			},{ -- decal end properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = 1
			},10/60,"Sine","Out")
		end))
	end
	local e2 = weirdeff("InvertedSphere",{ -- part start properties
		Size=Vector3.new(0,0,0),
		Color=Color3.new(0,0,0),
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame
	},{ -- decal start properties
	},{ -- part end properties
		Size=Vector3.new(1000,1000,1000),
	},{ -- decal end properties
	},Alpha,"Quint","In",0,true,0)
	local e2 = weirdeff("InvertedSphere",{ -- part start properties
		Size=Vector3.new(0,0,0),
		Color=Color3.new(1,0,0),
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame
	},{ -- decal start properties
	},{ -- part end properties
		Size=Vector3.new(1200,1200,1200),
	},{ -- decal end properties
	},Alpha,"Quad","In",0,true,0)
	TweenAnimate(cc,{Contrast=.5,Brightness=-.25,TintColor=Color3.new(1,.5,.5)},Alpha,"Quint","In")
	swait(Alpha*60)
	local e2 = weirdeff("sphere",{ -- part start properties
		Size=Vector3.new(500,500,500),
		Color=MAINRUINCOLOR,
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame
	},{ -- decal start properties
	},{ -- part end properties
		Size=Vector3.new(2048,2048,2048),
		Transparency = 1,
	},{ -- decal end properties
	},3,"Sine","Out",0,false,0)
	local e2 = weirdeff("SpikeBall",{ -- part start properties
		Size=Vector3.new(750,750,750),
		Color=Color3.new(0,0,0),
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame
	},{ -- decal start properties
		Color3=Color3.new(20,0,0),
	},{ -- part end properties
		Size=Vector3.new(2048,2048,2048),
		Transparency = 1,
	},{ -- decal end properties
		Transparency = 1,
	},1,"Elastic","Out",0,false,0)
	cc.Contrast = 3
	cc.Brightness = 1
	cc.TintColor = Color3.new(1,0,0)
	cc.Saturation = 1
	for i = 1,15 do
		coroutine.resume(coroutine.create(function()
			swait(stats["ASPD"]*i*3)
			local cfe = root.CFrame*randr()
			local e2 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(1500,1500,500),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,500,50)
			},{ -- decal start properties
				Color3=Color3.new(20,0,0),
				Transparency = 1,
			},{ -- part end properties
				Size=Vector3.new(2048,2048,500),
				CFrame = cfe*cf(0,500,50)*angles(0,0,math.rad(179))
			},{ -- decal end properties
				Transparency = -1
			},20/60,"Sine","In",nil,nil,nil,nil,true)
			local e = weirdeff("woosh2",{ -- part start properties
				Size=Vector3.new(1500,1500,500),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,500,-50)
			},{ -- decal start properties
				Color3=Color3.new(0,0,0),
				Transparency = 1,
			},{ -- part end properties
				Size=Vector3.new(2048,2048,500),
				CFrame = cfe*cf(0,500,-50)*angles(0,0,math.rad(-179))
			},{ -- decal end properties
				Transparency = -1
			},20/60,"Sine","In",nil,nil,nil,nil,true)
			swait(20)
			e:Destroy()
			e2:Destroy()
			local e3 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(2048,2048,500),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,500,50)*angles(0,0,math.rad(179))
			},{ -- decal start properties
				Color3=Color3.new(20,0,0),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(1000,1000,500),
				CFrame = cfe*cf(0,500,50)*angles(0,0,math.rad(358))
			},{ -- decal end properties
				Transparency = 1
			},20/60,"Sine","Out")
			local e4 = weirdeff("woosh2",{ -- part start properties
				Size=Vector3.new(2048,2048,500),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,500,-50)*angles(0,0,math.rad(-179))
			},{ -- decal start properties
				Color3=Color3.new(0,0,0),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(1000,1000,500),
				CFrame = cfe*cf(0,500,-50)*angles(0,0,math.rad(-358))
			},{ -- decal end properties
				Transparency = 1
			},20/60,"Sine","Out")
		end))
	end
	for i = 1,8 do
		weirdeff("VBLASTMESH2",{ -- part start properties
			Size=Vector3.new(10,0,10),
			Color=MAINRUINCOLOR2,
			Transparency = 1,
			Material="Neon",
			CFrame = root.CFrame*randr()
		},{ -- mesh start properties
			VertexColor = Vector3.new(0,0,0),
			Scale = Vector3.new(1,0,1)*(math.random(75*i,150*i)/3)
		},{ -- part end properties
			Transparency = .05
		},{ -- mesh end properties
			Scale = Vector3.new(0,0,0)
		},math.random(60,140)/150,"Sine","Out",0,false,i/60)
		weirdeff("VBLASTMESH2",{ -- part start properties
			Size=Vector3.new(10,0,10),
			Color=MAINRUINCOLOR2,
			Transparency = .05,
			Material="Neon",
			CFrame = root.CFrame*randr()
		},{ -- mesh start properties
			VertexColor = Vector3.new(MAINRUINCOLOR.R*20,MAINRUINCOLOR.G*20,MAINRUINCOLOR.B*20),
			Scale = Vector3.new(0,0,0),
		},{ -- part end properties
			Transparency = 1
		},{ -- mesh end properties
			Scale = Vector3.new(1,0,1)*(math.random(150*i,300*i)/3)
		},math.random(60,140)/50,"Sine","Out",0,false,i/60)
	end
	CreateSound5(3737497113,char,1,.5)
	CreateSound5(3570970614,char,4,1.5)
	CreateSound5(3404036141,char,5,.7)
	CreateSound5(239000203,char,5,1)
	for i,v in pairs(workspace:GetDescendants()) do
		if v:FindFirstChildOfClass("Humanoid") and v ~= char then
			v:FindFirstChildOfClass("Humanoid").Health = -9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999
		end
	end
	swait(2)
	CreateSound5(1413550336,char,2,1.1)
	cc.Brightness = -1
	TweenAnimate(cc,{Contrast=0,Brightness=0,TintColor=Color3.new(1,1,1),Saturation=0},1,"Sine","Out")
	-- ~-~ AEROMAD 3 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*30/60,'Quint','Out',0,false,0
	local eg = TweenAnimate(RootJoint,{C0=cf(0,1.8,0.4)*angles(math.rad(50),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1,-1,-0.1)*angles(math.rad(-35.1),math.rad(12.2),math.rad(-17.3))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(0.9,-0.8,-1)*angles(math.rad(-59.9),math.rad(-17.1),math.rad(18.2))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1,0.3,1)*angles(math.rad(115.9),math.rad(-10),math.rad(-84.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(1,0.3,1)*angles(math.rad(115.9),math.rad(10),math.rad(84.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0,0.3,-0.6)*angles(math.rad(35),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	swait(stats["ASPD"]*30)
	attack,idlesoff = false,false
	root.Anchored = false
	swait(30)
	cc:Destroy()
end

function AeroSad() -- heavily nerfed aeromad()
	attack,idlesoff = true,true
	root.Anchored = true
	local cc = Instance.new("ColorCorrectionEffect",game.Lighting)
	game:service'Debris':AddItem(cc,5)
	cc.Contrast = -2
	CreateSound5(4196111215,char,4,0.7)
	-- ~-~ AEROMAD 1 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*10/60,'Elastic','Out',0,false,0
	local eg = TweenAnimate(RootJoint,{C0=cf(0,2,0)*angles(math.rad(0),math.rad(-45),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1,-1,-0.1)*angles(math.rad(-9.8),math.rad(1.7),math.rad(-9.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(1.1,-0.5,-1)*angles(math.rad(-29.9),math.rad(2.5),math.rad(-4.3))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1.6,0.1,-0.5)*angles(math.rad(77.8),math.rad(18.3),math.rad(-28.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(1.1,1,-0.8)*angles(math.rad(118.9),math.rad(-40.1),math.rad(-65.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0.3,0.2,0.2)*angles(math.rad(-29.7),math.rad(31.3),math.rad(21.5))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(stats["ASPD"]*5)
	cc.Contrast = 0
	CreateSound5(4648407351,root,3,1.5)
	swait(Alpha*60 - 5)
	CreateSound5(4648415285,root,5,0.9)
	CreateSound5(4575206546,root,7,0.6)
	for i = 1,3 do
		weirdeff("VBLASTMESH2",{ -- part start properties
			Size=Vector3.new(10,0,10),
			Color=MAINRUINCOLOR,
			Transparency = .05,
			Material="Neon",
			CFrame = root.CFrame*CFrame.new(0,50,0)
		},{ -- mesh start properties
			VertexColor = Vector3.new(MAINRUINCOLOR2.R*5,MAINRUINCOLOR2.G*5,MAINRUINCOLOR2.B*5),
			Scale = Vector3.new(0,5,0)
		},{ -- part end properties
			Transparency = 1,
			CFrame = root.CFrame*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)*CFrame.new(0,50,0)
		},{ -- mesh end properties
			Scale = Vector3.new(5,2.5,5)*(math.random(25,100)/100)
		},math.random(40,160)/80,"Linear","Out")
	end
	weirdeff("Explosion2",{ -- part start properties
		Size=Vector3.new(50,50,50),
		Color=MAINRUINCOLOR2,
		Transparency = -2,
		Material="Neon",
		CFrame = root.CFrame*CFrame.new(0,25,0)
	},{ -- decal start properties
		Color3 = Color3.new(MAINRUINCOLOR.R*10,MAINRUINCOLOR.G*10,MAINRUINCOLOR.B*10),
		Transparency = -1
	},{ -- part end properties
		Size=Vector3.new(150,350,150),
		Transparency = 1,
		CFrame = root.CFrame*CFrame.new(0,175,0)
	},{ -- decal end properties
		Transparency = 1
	},.5,"Elastic","InOut")
	TweenAnimate(root,{CFrame=root.CFrame*cf(0,500,0)},stats["ASPD"]*25/60,"Sine","Out")
	swait(40)
	CreateSound5(4755396368,char,2,.3)
	CreateSound5(3570968988,char,2,.5)
	-- ~-~ AEROMAD 2 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*180/60,'Quad','InOut',0,false,0
	local eg = TweenAnimate(RootJoint,{C0=cf(0,2,0.2)*angles(math.rad(25),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1,-0.9,0)*angles(math.rad(-25),math.rad(20),math.rad(-10))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(0.9,-0.6,-0.9)*angles(math.rad(-39.9),math.rad(-7.4),math.rad(8.4))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-0.7,0.7,-0.8)*angles(math.rad(58.9),math.rad(54.7),math.rad(119.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(0.7,0.7,-0.8)*angles(math.rad(57.1),math.rad(-58.7),math.rad(-115.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0,0,-0.2)*angles(math.rad(10),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	local col = MAINRUINCOLOR
	for i = 1,24 do
		coroutine.resume(coroutine.create(function()
			swait(stats["ASPD"]*i*5)
			local cfe = root.CFrame*randr()
			local e2 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(500,500,1.5),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,250,.15)
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = 1,
			},{ -- part end properties
				Size=Vector3.new(750,750,1.5),
				CFrame = cfe*cf(0,250,.15)*angles(0,0,math.rad(179))
			},{ -- decal end properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = -1
			},25/60,"Sine","In",nil,nil,nil,nil,true)
			local e = weirdeff("woosh2",{ -- part start properties
				Size=Vector3.new(500,500,1.5),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,250,-.15)
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = 1,
			},{ -- part end properties
				Size=Vector3.new(750,750,1.5),
				CFrame = cfe*cf(0,250,-.15)*angles(0,0,math.rad(-179))
			},{ -- decal end properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = -1
			},25/60,"Sine","In",nil,nil,nil,nil,true)
			swait(25)
			e:Destroy()
			e2:Destroy()
			local e3 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(750,750,1.5),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,250,.15)*angles(0,0,math.rad(179))
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(500,500,1.5),
				CFrame = cfe*cf(0,250,.15)*angles(0,0,math.rad(358))
			},{ -- decal end properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = 1
			},25/60,"Sine","Out")
			local e4 = weirdeff("woosh2",{ -- part start properties
				Size=Vector3.new(750,750,1.5),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,250,.15)*angles(0,0,math.rad(-179))
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(500,500,1.5),
				CFrame = cfe*cf(0,250,.15)*angles(0,0,math.rad(-358))
			},{ -- decal end properties
				Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
				Transparency = 1
			},25/60,"Sine","Out")
		end))
	end
	local e2 = weirdeff("InvertedSphere",{ -- part start properties
		Size=Vector3.new(0,0,0),
		Color=MAINRUINCOLOR2,
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame
	},{ -- decal start properties
	},{ -- part end properties
		Size=Vector3.new(1000,1000,1000),
	},{ -- decal end properties
	},Alpha,"Quint","In",0,true,0)
	local e2 = weirdeff("InvertedSphere",{ -- part start properties
		Size=Vector3.new(0,0,0),
		Color=MAINRUINCOLOR,
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame
	},{ -- decal start properties
	},{ -- part end properties
		Size=Vector3.new(1200,1200,1200),
	},{ -- decal end properties
	},Alpha,"Quad","In",0,true,0)
	TweenAnimate(cc,{Contrast=.1,Brightness=-.05},Alpha,"Quint","In")
	swait(Alpha*60)
	local e2 = weirdeff("sphere",{ -- part start properties
		Size=Vector3.new(500,500,500),
		Color=MAINRUINCOLOR,
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame
	},{ -- decal start properties
	},{ -- part end properties
		Size=Vector3.new(2048,2048,2048),
		Transparency = 1,
	},{ -- decal end properties
	},3,"Sine","Out",0,false,0)
	local e2 = weirdeff("SpikeBall",{ -- part start properties
		Size=Vector3.new(750,750,750),
		Color=MAINRUINCOLOR2,
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame
	},{ -- decal start properties
		Color3=Color3.fromRGB((col.R*100)^1.85,(col.G*100)^1.85,(col.B*100)^1.85),
	},{ -- part end properties
		Size=Vector3.new(2048,2048,2048),
		Transparency = 1,
	},{ -- decal end properties
		Transparency = 1,
	},1,"Elastic","Out",0,false,0)
	cc.Contrast = .5
	cc.Brightness = .25
	cc.TintColor = MAINRUINCOLOR
	cc.Saturation = .25
	for i = 1,15 do
		coroutine.resume(coroutine.create(function()
			swait(i*3)
			local cfe = root.CFrame*randr()
			local e2 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(1500,1500,500),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,500,50)
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*100)^1.85,(col.G*100)^1.85,(col.B*100)^1.85),
				Transparency = 1,
			},{ -- part end properties
				Size=Vector3.new(2048,2048,500),
				CFrame = cfe*cf(0,500,50)*angles(0,0,math.rad(179))
			},{ -- decal end properties
				Transparency = -1
			},20/60,"Sine","In",nil,nil,nil,nil,true)
			local e = weirdeff("woosh2",{ -- part start properties
				Size=Vector3.new(1500,1500,500),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,500,-50)
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*100)^1.85,(col.G*100)^1.85,(col.B*100)^1.85),
				Transparency = 1,
			},{ -- part end properties
				Size=Vector3.new(2048,2048,500),
				CFrame = cfe*cf(0,500,-50)*angles(0,0,math.rad(-179))
			},{ -- decal end properties
				Transparency = -1
			},20/60,"Sine","In",nil,nil,nil,nil,true)
			swait(20)
			e:Destroy()
			e2:Destroy()
			local e3 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(2048,2048,500),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,500,50)*angles(0,0,math.rad(179))
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*100)^1.85,(col.G*100)^1.85,(col.B*100)^1.85),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(1000,1000,500),
				CFrame = cfe*cf(0,500,50)*angles(0,0,math.rad(358))
			},{ -- decal end properties
				Transparency = 1
			},20/60,"Sine","Out")
			local e4 = weirdeff("woosh2",{ -- part start properties
				Size=Vector3.new(2048,2048,500),
				Color=col,
				Transparency = 1,
				Material="Neon",
				CFrame = cfe*cf(0,500,-50)*angles(0,0,math.rad(-179))
			},{ -- decal start properties
				Color3=Color3.fromRGB((col.R*100)^1.85,(col.G*100)^1.85,(col.B*100)^1.85),
				Transparency = -1,
			},{ -- part end properties
				Size=Vector3.new(1000,1000,500),
				CFrame = cfe*cf(0,500,-50)*angles(0,0,math.rad(-358))
			},{ -- decal end properties
				Transparency = 1
			},20/60,"Sine","Out")
		end))
	end
	for i = 1,4 do
		weirdeff("VBLASTMESH2",{ -- part start properties
			Size=Vector3.new(10,0,10),
			Color=MAINRUINCOLOR2,
			Transparency = 1,
			Material="Neon",
			CFrame = root.CFrame*randr()
		},{ -- mesh start properties
			VertexColor = Vector3.new(MAINRUINCOLOR2.R*20,MAINRUINCOLOR2.G*20,MAINRUINCOLOR2.B*20),
			Scale = Vector3.new(1,0,1)*(math.random(75*i,150*i)/3)
		},{ -- part end properties
			Transparency = .05
		},{ -- mesh end properties
			Scale = Vector3.new(0,0,0)
		},math.random(60,140)/150,"Sine","Out",0,false,i/60)
		weirdeff("VBLASTMESH2",{ -- part start properties
			Size=Vector3.new(10,0,10),
			Color=MAINRUINCOLOR2,
			Transparency = .05,
			Material="Neon",
			CFrame = root.CFrame*randr()
		},{ -- mesh start properties
			VertexColor = Vector3.new(MAINRUINCOLOR.R*20,MAINRUINCOLOR.G*20,MAINRUINCOLOR.B*20),
			Scale = Vector3.new(0,0,0),
		},{ -- part end properties
			Transparency = 1
		},{ -- mesh end properties
			Scale = Vector3.new(1,0,1)*(math.random(150*i,300*i)/3)
		},math.random(60,140)/50,"Sine","Out",0,false,i/60)
	end
	CreateSound5(3737497113,char,1,.5)
	CreateSound5(3570970614,char,4,1.5)
	CreateSound5(3404036141,char,5,.7)
	CreateSound5(239000203,char,5,1)
	for i,v in pairs(workspace:GetDescendants()) do
		if v:FindFirstChildOfClass("Humanoid") and v ~= char and v:FindFirstChild("Head") and (v:FindFirstChild("Head").CFrame.p-root.CFrame.p).magnitude <= 2048 then
			v:FindFirstChildOfClass("Humanoid"):BreakJoints()
		end
	end
	swait(2)
	--CreateSound5(1413550336,char,2,1.1)
	cc.Brightness = -.25
	TweenAnimate(cc,{Contrast=0,Brightness=0,TintColor=Color3.new(1,1,1),Saturation=0},1,"Sine","Out")
	-- ~-~ AEROMAD 3 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*30/60,'Quint','Out',0,false,0
	local eg = TweenAnimate(RootJoint,{C0=cf(0,1.8,0.4)*angles(math.rad(50),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1,-1,-0.1)*angles(math.rad(-35.1),math.rad(12.2),math.rad(-17.3))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(0.9,-0.8,-1)*angles(math.rad(-59.9),math.rad(-17.1),math.rad(18.2))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1,0.3,1)*angles(math.rad(115.9),math.rad(-10),math.rad(-84.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(1,0.3,1)*angles(math.rad(115.9),math.rad(10),math.rad(84.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0,0.3,-0.6)*angles(math.rad(35),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	swait(stats["ASPD"]*30)
	-- ~-~ AEROMAD 4 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*30/60,'Sine','In',0,false,0
	local eg = TweenAnimate(RootJoint,{C0=cf(0,0.2,0)*angles(math.rad(135),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1,-1.2,0)*angles(math.rad(42.6),math.rad(10.1),math.rad(-4.4))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(1,-1,0)*angles(math.rad(26.8),math.rad(-7),math.rad(2.6))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1.4,0.6,-0.2)*angles(math.rad(49.9),math.rad(-8.7),math.rad(-12))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(1.4,0.6,-0.1)*angles(math.rad(50.6),math.rad(6.2),math.rad(21.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0,0.4,0.6)*angles(math.rad(-35),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(root,{CFrame=root.CFrame*cf(0,-500,0)},120/60,"Quad","In")
	swait(stats["ASPD"]*120)
	cc:Destroy()
	-- ~-~ AEROMAD 5 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*15/60,'Bounce','Out',0,false,0
	local eg = TweenAnimate(RootJoint,{C0=cf(0,-2.5,0.5)*angles(math.rad(90),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-0.9,-0.9,0.1)*angles(math.rad(0.3),math.rad(10.4),math.rad(-19.6))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(0.7,-0.4,-0.6)*angles(math.rad(-25.8),math.rad(-11.4),math.rad(17.2))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1.5,0.8,0.2)*angles(math.rad(63.2),math.rad(-22.5),math.rad(-106.4))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(1.6,0.6,0.3)*angles(math.rad(63.4),math.rad(-3),math.rad(95.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0,0.1,-0.2)*angles(math.rad(10),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(stats["ASPD"]*480) -- hell for high ASPD values
	attack,idlesoff = false,false
	root.Anchored = false
end

function dash(dir)
	attack,idlesoff = true,true
	local function effect(angle)
		particle(root.CFrame*angles(0,math.rad(angle),0),40*(1+(stats["WS"]/250)),{ZOffset=2,
			Texture="rbxassetid://1053546634",
			LightEmission=1,
			EmissionDirection = "Front",
			Color=CS(Color3.new(1,1,1)),
			Lifetime=NR(20/60,40/60),
			Size=NS({NSK(0,4*((1+(stats["WS"]/25))),0),NSK(0.1,.6*((1+(stats["WS"]/25))),0),NSK(0.2,.4*((1+(stats["WS"]/25))),0),NSK(0.3,.6*((1+(stats["WS"]/25))),0),NSK(0.4,.4*((1+(stats["WS"]/25))),0),NSK(0.5,.6*((1+(stats["WS"]/25))),0),NSK(0.6,.4*((1+(stats["WS"]/25))),0),NSK(1,0,0)}),
			Speed = NR(4*((1+(stats["WS"]/25))),80*((1+(stats["WS"]/25)))),
			SpreadAngle=Vector2.new(20,20),
			Acceleration = Vector3.new(0,-5,0),
			Drag = 5})
		particle(root.CFrame*angles(0,math.rad(angle),0),3,{ZOffset=4,
			Texture="rbxassetid://1053546634",
			LightEmission=1,
			Color=CS(Color3.new(1,1,1)),
			Lifetime=NR(20/60),
			Size=NS({NSK(0,15*((1+(stats["WS"]/25))),0),NSK(0.1,7.5*((1+(stats["WS"]/25))),0),NSK(1,0,0)}),
			RotSpeed=NR(180)})
		weirdeff("VBLASTMESH2",{
			Transparency = 0.05,
			CFrame = root.CFrame*CFrame.Angles(math.rad(90),0,math.rad(180-angle))*CFrame.new(0,.02,0),
		},{
			Scale = Vector3.new(.01,.01,.01)*((1+(stats["WS"]/25))),
			VertexColor = Vector3.new(5,5,5)
		},{
			Transparency = 1,
			CFrame = root.CFrame*CFrame.Angles(math.rad(90),0,math.rad(180-angle))*CFrame.new(0,4.9,0),
		},{
			Scale = Vector3.new(.025,.5,.025)*((1+(stats["WS"]/25))),
			VertexColor = Vector3.new(1,1,1)
		},30/60,"Quint","Out")
		weirdeff("VBLASTMESH2",{
			Transparency = 0.05,
			CFrame = root.CFrame*CFrame.Angles(math.rad(90),0,math.rad(180-angle))*CFrame.new(0,5,0),
		},{
			Scale = Vector3.new(.01,.5,.01)*((1+(stats["WS"]/25))),
			VertexColor = Vector3.new(5,5,5)
		},{
			Transparency = 1,
			CFrame = root.CFrame*CFrame.Angles(math.rad(90),0,math.rad(180-angle))*CFrame.new(0,.5,0),
		},{
			Scale = Vector3.new(.05,.05,.05)*((1+(stats["WS"]/25))),
			VertexColor = Vector3.new(1,1,1)
		},45/60,"Quint","Out")
	end
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 3/60,'Linear','Out',0,false,0
	if dir == "w" then
		GetLocalProperty(Hum,"CameraOffset",Vector3.new(0,0,walkspeed*.5))
		effect(0)
		root.CFrame = root.CFrame*CFrame.new(0,0,-walkspeed*.5)
		local eg = TweenAnimate(RootJoint,{C0=cf(0,-0.1 + (Mode["flying"] or 0),-0.3)*angles(math.rad(-35),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1,-1.2,0.1)*angles(math.rad(-34.9),math.rad(4.1),math.rad(-2.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1,-0.7,-1.1)*angles(math.rad(-45),math.rad(-4.9),math.rad(0.9))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.6,0.6,0.2)*angles(math.rad(-54.6),math.rad(17.9),math.rad(-17.5))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.6,0.6,0.3)*angles(math.rad(-50.1),math.rad(-18.2),math.rad(21.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0,0.1,-0.6)*angles(math.rad(25),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	elseif dir == "s" then
		GetLocalProperty(Hum,"CameraOffset",Vector3.new(0,0,-walkspeed*.5))
		effect(180)
		root.CFrame = root.CFrame*CFrame.new(0,0,walkspeed*.5)
		local eg = TweenAnimate(RootJoint,{C0=cf(0,-0.1 + (Mode["flying"] or 0),0.3)*angles(math.rad(35),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-0.9,-0.5,-0.1)*angles(math.rad(20.9),math.rad(9.8),math.rad(-5.4))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1,-1.3,0)*angles(math.rad(30.4),math.rad(-6.6),math.rad(2.4))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.5,0.4,-0.2)*angles(math.rad(42.4),math.rad(-14),math.rad(-20.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.6,0.3,-0.2)*angles(math.rad(42.4),math.rad(14),math.rad(20.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0,0.2,0.5)*angles(math.rad(-30),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	elseif dir == "a" then
		GetLocalProperty(Hum,"CameraOffset",Vector3.new(walkspeed*.5,0,0))
		effect(90)
		root.CFrame = root.CFrame*CFrame.new(-walkspeed*.5,0,0)
		local eg = TweenAnimate(RootJoint,{C0=cf(-0.2,-0.1 + (Mode["flying"] or 0),-0.1)*angles(math.rad(-33.1),math.rad(21.5),math.rad(32.5))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1.2,-0.9,-0.9)*angles(math.rad(-40.6),math.rad(6.8),math.rad(13.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(0.9,-0.8,0)*angles(math.rad(-8),math.rad(-27.7),math.rad(28.6))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-0.7,0.9,-0.8)*angles(math.rad(125.4),math.rad(-24.6),math.rad(79))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.5,0.3,0.3)*angles(math.rad(4),math.rad(-35.3),math.rad(60.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0.1,0.1,-0.5)*angles(math.rad(22.1),math.rad(-20.4),math.rad(1.6))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	elseif dir == "d" then
		GetLocalProperty(Hum,"CameraOffset",Vector3.new(-walkspeed*.5,0,0))
		effect(-90)
		root.CFrame = root.CFrame*CFrame.new(walkspeed*.5,0,0)
		local eg = TweenAnimate(RootJoint,{C0=cf(.2,-.1 + (Mode["flying"] or 0),-0.1)*angles(math.rad(-33.1),math.rad(-21.5),math.rad(-32.5))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-0.9,-0.8,-0.1)*angles(math.rad(-8),math.rad(27.7),math.rad(-28.6))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1.1,-0.9,-0.9)*angles(math.rad(-40.6),math.rad(-6.8),math.rad(-13.8))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.4,0.3,0.3)*angles(math.rad(4),math.rad(35.3),math.rad(-60.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1,1,-0.7)*angles(math.rad(125.4),math.rad(24.6),math.rad(-79))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(-0.1,0,-0.3)*angles(math.rad(22.1),math.rad(20.4),math.rad(-1.6))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	end
	swait()
	LocalTweenAnimate(Hum,{CameraOffset=Vector3.new(0,0,0)},10/60,"Sine","Out")
	statStuff["Add"]["WS"] = 48
	statStuff["Multiply"]["WS"] = 4
	statThing()
	walkspeed = stats["WS"]
	Hum.WalkSpeed = stats["WS"]
	swait(10)
	attack,idlesoff = false,false
	statStuff["Add"]["WS"] = 18
	statStuff["Multiply"]["WS"] = 2.5
	statThing()
	walkspeed = stats["WS"]
	repeat swait() until attack == true or not (keysDown["w"] or keysDown["s"] or keysDown["a"] or keysDown["d"])
	statStuff["Add"]["WS"] = 0
	statStuff["Multiply"]["WS"] = 1
	statThing()
	if not attack then
		walkspeed = stats["WS"]
	end
end

function doublejump()
	attack,idlesoff = true,true
	particle(root.CFrame*angles(math.rad(90),0,0),40*(1+(stats["WS"]/250)),{ZOffset=2,
		Texture="rbxassetid://1053546634",
		LightEmission=1,
		EmissionDirection = "Front",
		Color=CS(Color3.new(1,1,1)),
		Lifetime=NR(20/60,40/60),
		Size=NS({NSK(0,4*((1+(stats["WS"]/25))),0),NSK(0.1,.6*((1+(stats["WS"]/25))),0),NSK(0.2,.4*((1+(stats["WS"]/25))),0),NSK(0.3,.6*((1+(stats["WS"]/25))),0),NSK(0.4,.4*((1+(stats["WS"]/25))),0),NSK(0.5,.6*((1+(stats["WS"]/25))),0),NSK(0.6,.4*((1+(stats["WS"]/25))),0),NSK(1,0,0)}),
		Speed = NR(4*((1+(stats["WS"]/25))),80*((1+(stats["WS"]/25)))),
		SpreadAngle=Vector2.new(20,20),
		Acceleration = Vector3.new(0,-5,0),
		Drag = 5})
	particle(root.CFrame*angles(math.rad(90),0,0),3,{ZOffset=4,
		Texture="rbxassetid://1053546634",
		LightEmission=1,
		Color=CS(Color3.new(1,1,1)),
		Lifetime=NR(20/60),
		Size=NS({NSK(0,15*((1+(stats["WS"]/25))),0),NSK(0.1,7.5*((1+(stats["WS"]/25))),0),NSK(1,0,0)}),
		RotSpeed=NR(180)})
	weirdeff("VBLASTMESH2",{
		Transparency = 0.05,
		CFrame = root.CFrame*CFrame.Angles(math.rad(0),0,math.rad(0))*CFrame.new(0,.02,0),
	},{
		Scale = Vector3.new(.01,.01,.01)*((1+(stats["WS"]/25))),
		VertexColor = Vector3.new(5,5,5)
	},{
		Transparency = 1,
		CFrame = root.CFrame*CFrame.Angles(math.rad(0),0,math.rad(0))*CFrame.new(0,5,0),
	},{
		Scale = Vector3.new(.025,.5,.025)*((1+(stats["WS"]/25))),
		VertexColor = Vector3.new(1,1,1)
	},30/60,"Quad","Out")
	weirdeff("VBLASTMESH2",{
		Transparency = 0.05,
		CFrame = root.CFrame*CFrame.Angles(math.rad(0),0,math.rad(0))*CFrame.new(0,5,0),
	},{
		Scale = Vector3.new(.01,.5,.01)*((1+(stats["WS"]/25))),
		VertexColor = Vector3.new(5,5,5)
	},{
		Transparency = 1,
		CFrame = root.CFrame*CFrame.Angles(math.rad(0),0,math.rad(0))*CFrame.new(0,.5,0),
	},{
		Scale = Vector3.new(.1,.05,.1)*((1+(stats["WS"]/25))),
		VertexColor = Vector3.new(1,1,1)
	},45/60,"Quad","Out")
	moves["jumped"] = (tonumber(moves["jumped"]) or 0) + 1
	local bv = Instance.new("BodyVelocity")
	bv.Velocity = Vector3.new(root.Velocity.X*2,stats["JP"],root.Velocity.Z*2)
	bv.MaxForce = Vector3.new(1e9,1e10,1e9)
	bv.Parent = root
	for i = 1,5 do
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 2/60,'Linear','Out',0,false,0
		local eg = TweenAnimate(RootJoint,{C0=cf(0,0,0)*angles(math.rad(-sine*20),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-0.9,-0.7,-1)*angles(math.rad(-55.4),math.rad(-5),math.rad(-5))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(0.9,-0.8,-1)*angles(math.rad(-55.4),math.rad(5),math.rad(5))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.3,0,-0.5)*angles(math.rad(106.8),math.rad(16.3),math.rad(53.3))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.2,0.1,-0.6)*angles(math.rad(110),math.rad(-8.6),math.rad(-59.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0,0.2,0.4)*angles(math.rad(-30),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		swait(2)
	end
	bv:Destroy()
	attack,idlesoff = false,false
end

function Chainc1()

end

