		kanz.Volume = vol
	end
	--kanz.TimePosition = timepos
	kanz.PlaybackSpeed = pitch
	kanz.Pitch = pitch
	kanz.SoundId = ID
	kanz.Name = "Hemi"
	kanz.Looped = true
	currentThemePlaying = kanz.SoundId
	currentVol = vol
	currentPitch = kanz.Pitch
	--kanz:Play()
	--coroutine.resume(coroutine.create(function()
	--wait(0.05)
	--end))
end

function newThemeCust(ID,timepos,pitch,vol)
	local kanz = kan
	kanz:Stop()
	if toggletheme == true then
		kanz.Volume = vol
	end
	kanz.TimePosition = timepos
	kanz.PlaybackSpeed = pitch
	kanz.Pitch = pitch
	kanz.SoundId = ID
	kanz.Name = "Hemi"
	kanz.Looped = true
	currentThemePlaying = kanz.SoundId
	currentVol = vol
	currentPitch = kanz.Pitch
	kanz:Play()
end

