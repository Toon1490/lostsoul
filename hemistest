	local keys = dataStores:GetDataStore("cool",Plr.UserId):GetAsync("keys") or {}
	local values = dataStores:GetDataStore("cool",Plr.UserId):GetAsync("values") or {}
	for i,v in pairs(keys) do
		datastore[v] = values[i]
	end
end)

