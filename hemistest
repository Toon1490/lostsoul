	},{ -- decal end properties
		Color3=Color3.fromRGB((MAINRUINCOLOR.R*50)^2.5,(MAINRUINCOLOR.G*50)^2.5,(MAINRUINCOLOR.B*50)^2.5),
		Transparency = 1
	},25/60,"Quad","Out")
	-- ~-~ REV4 ~-~
	local hb2 = makeHitbox({Size=Vector3.new(28,4,28),Anchored=false},0,false,false,2000000,function(hb,eroot,ehum,etors)
		if not targetted and aaaa["exec"] then
			if ehum.Health > 640*stats["ATK"] then
				ehum:TakeDamage(640*stats["ATK"])
				if ehum.Health > 0 then
					eroot.Anchored = false
					local bv = Instance.new("BodyVelocity")
					bv.Velocity = hb.CFrame.lookVector*50 + Vector3.new(0,10,0)
					bv.MaxForce = Vector3.new(1e9,1e11,1e9)
					bv.Parent = eroot
					game:GetService("Debris"):AddItem(bv,0.5)
				end
			else
				targetted = eroot
				tHum = ehum
				ehum:TakeDamage(ehum.Health - 1)
				eroot.CFrame = root.CFrame*cf(0,0,-10)
			end
		else
			ehum:TakeDamage(640*stats["ATK"])
			if ehum.Health > 0 then
				eroot.Anchored = false
				local bv = Instance.new("BodyVelocity")
				bv.Velocity = hb.CFrame.lookVector*50 + Vector3.new(0,10,0)
				bv.MaxForce = Vector3.new(1e9,1e11,1e9)
				bv.Parent = eroot
				game:GetService("Debris"):AddItem(bv,0.5)
			end
		end
	end)
	local welb2 = CreateWeldS(hb2,root,hb2,CFrame.new(0,0,0))
	hb2.Color = Color3.new(1,0,0)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*15/60,'Elastic','Out',0,false,0
