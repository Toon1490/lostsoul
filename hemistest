		TweenAnimate(RW,{C0=cf(1.2,0.4 - .1 * math.cos(sine/30),-0.6)*angles(math.rad(13.2),math.rad(46.1),math.rad(-21))},Alpha,'Linear','Out',0,false,0)
		TweenAnimate(Torso.Neck,{C0 =cf(-0.1,0,0)*angles(math.rad(0),math.rad(-21.7),math.rad(-5.2))*necko},Alpha,'Linear','Out',0,false,0)
	end
	dialog(getName(),modequote2[1],"player",modequotes[2])
	for i = 0,400 do
		if math.random(1,4) == 2 then
			local lepos = math.random(-15,15)
