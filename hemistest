			Scale = Vector3.new(1,2.5,1)
		},{ -- part end properties
			Transparency = 1,
			CFrame = root.CFrame*CFrame.new(0,10*m,-125)*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
		},{ -- mesh end properties
			Scale = Vector3.new(3,1,3)*m
