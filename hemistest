		TweenAnimate(RW,{C0=cf(1.5,0.8,-0.4)*angles(math.rad(7.7),math.rad(49.8),math.rad(51.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0.4,0.3,-0.6)*angles(math.rad(33.4),math.rad(-19.6),math.rad(17.7))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		swait(Alpha*60)
		CreateSound5("2374026404",root,1,1.1,false)
		CreateSound5("1820220449",root,1,1.3,false)
		CreateSound5("1820268208",root,1,1.2,false)
		CreateSound5("4701290769",root,0.75,0.6,false)
	end
	Mode = 0 -- kill effects/colorshift
	Recolor(Color3.new(0,0,0),Color3.new(0,0,0))
	RecolorTextAndRename("???",cl2:lerp(Color3.new(.1,.1,.1),0.8),cl1:lerp(Color3.new(.2,.2,.2),0.8),"GothamBlack")
	Funmy(Player)
	local e = weirdeff("InvertedSphere",{ -- part start properties
		Size=Vector3.new(0,0,0),
		Color=MAINRUINCOLOR,
		Transparency = 0,
		Material="Neon",
		CFrame = root.CFrame*CFrame.new(0,-3,0)
	},{ -- decal start properties
	},{ -- part end properties
		Size=Vector3.new(50,50,50)*(size or 1),
		Transparency = 0.95
	},{ -- decal end properties
	},2,"Sine","Out",nil,true,nil,nil,true)
	weirdeff("sphere",{ -- part start properties
		Size=Vector3.new(0,0,0),
		Color=cl2,
		Transparency = 1,
		Material="ForceField",
		CFrame = root.CFrame*CFrame.new(0,-3,0)
	},{ -- decal start properties
	},{ -- part end properties
		Size=Vector3.new(51,51,51)*(size or 1),
		Transparency = 0
	},{ -- decal end properties
	},2,"Sine","Out",nil,true,nil,nil,true)
	local e2
	if not moves["gliding"] then
		e2 = weirdeff("sphere",{ -- part start properties
			Size=Vector3.new(0,0,0),
			Color=MAINRUINCOLOR2,
			Transparency = 0,
			Material="Neon",
			CFrame = root.CFrame*CFrame.new(0,-3,0)
		},{ -- decal start properties, but sphere has no decals
