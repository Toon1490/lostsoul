	CreateSound5("3404036141",root,.5,0.9)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*15/60,'Quint','Out',0,false,0
	TweenAnimate(root,{CFrame = root.CFrame*CFrame.new(0,0,-1)},Alpha,"Sine","In",Repeat,Reverse,Delay)
	local eg = TweenAnimate(RootJoint,{C0=cf(0,-0.3,0.1)*angles(math.rad(173),math.rad(35.3),math.rad(-179.3))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1,-1.1,0.1)*angles(math.rad(-10.6),math.rad(14.3),math.rad(13.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(0.7,0.2,-0.6)*angles(math.rad(19.6),math.rad(7),math.rad(27))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1.1,0.3,-0.7)*angles(math.rad(53.4),math.rad(71.4),math.rad(58.4))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(0.8,0.8,-0.8)*angles(math.rad(-175.5),math.rad(47.2),math.rad(-89.3))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0.2,0,0.3)*angles(math.rad(-3.7),math.rad(-25.3),math.rad(13.1))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(weldo,{C0=cf(0,0,0)*angles(math.rad(69.9),math.rad(-3.5),math.rad(90+-3.5))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	e:Destroy()
	e2:Destroy()
	e = weirdeff("aur",{ -- part start properties
