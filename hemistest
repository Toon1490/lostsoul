		aura2.Enabled = false
		TweenAnimate(decael,{Transparency=1},0.5,"Quad","In")
		AXE:Destroy()
		parb:Destroy()
		swait(5)
		root.Anchored = false
		attack,idlesoff = false,false
	end
end

function efspin(what,turns)
	return what*angles(0,rad(180*turns),0)
end

vertex1 = Color3.new(MAINRUINCOLOR.R*5,MAINRUINCOLOR.G*5,MAINRUINCOLOR.B*5)
vertex2 = Color3.new(MAINRUINCOLOR2.R*5,MAINRUINCOLOR2.G*5,MAINRUINCOLOR2.B*5)

function infernalcross()
	attack,idlesoff = true,true
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*.4,'Quad','InOut',0,false,0
	TweenAnimate(RootJoint,{C0=cf(0,0,-0)*angles(math.rad(-17.6),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1.1,-1.1,-0.2)*angles(math.rad(17.6),math.rad(4.8),math.rad(-1.3))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(0.9,-1.1,-0.1)*angles(math.rad(18.5),math.rad(-11.3),math.rad(4.8))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1.5,0.3,0.4)*angles(math.rad(54.3),math.rad(2.4),math.rad(-23.4))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(1.5,0.3,0.4)*angles(math.rad(54.3),math.rad(-2.4),math.rad(23.4))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =necko*cf(0,0,0)*angles(math.rad(-11.9),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(60*Alpha)
	local pointa,pointb = root.CFrame*cf(-30,-3,15),root.CFrame*cf(30,-3,15)
	--a
	weirdeff("Ring",{ -- part start properties
		Size=Vector3.new(10,10,2),
		Color=MAINRUINCOLOR2,
		Transparency = 0,
		Material="Neon",CFrame = pointa*angles(rad(90),0,0)},
	{ -- decal start properties
		Color3 = MAINRUINCOLOR2	
	},{ -- part end properties
		Size=Vector3.new(50,50,2),
		Transparency = 1,
		CFrame = pointa*angles(rad(90),0,rad(180))
	},{ -- decal end properties
		Color3 = MAINRUINCOLOR2,
		Transparency = 1
	},1,"Quint","Out",nil,false)

	weirdeff("Wave2",{ -- part start properties
		Size=Vector3.new(10,10,10),
		Color=MAINRUINCOLOR,
		Transparency = 0,
		Material="Neon",CFrame = pointa},
	{ -- decal start properties
		Color3 = Color3.new(MAINRUINCOLOR.R*6,MAINRUINCOLOR.G*6,MAINRUINCOLOR.B*6)
	},{ -- part end properties
		Size=Vector3.new(70,10,70)/1.5,
		Transparency = 1,
		CFrame = pointa*angles(0,0,rad(360))
	},{ -- decal end properties
		Color3 = Color3.new(MAINRUINCOLOR2.R*6,MAINRUINCOLOR2.G*6,MAINRUINCOLOR2.B*6),
		Transparency = 1
	},1,"Quad","InOut",nil,false)
	weirdeff("Explosion2",{ -- part start properties
		Size=Vector3.new(50,.1,50),
		Color=MAINRUINCOLOR,
		Transparency = 0,
		Material="Neon",CFrame = pointa},
	{ -- decal start properties
		Color3 = Color3.new(MAINRUINCOLOR.R*6,MAINRUINCOLOR.G*6,MAINRUINCOLOR.B*6),
	},{ -- part end properties
		Size=Vector3.new(25,50,25),
		Transparency = 1,
		CFrame = pointa*cf(0,25,0)*angles(0,rad(180),0)
	},{ -- decal end properties
		Transparency = 1,
		Color3 = Color3.new(MAINRUINCOLOR2.R*6,MAINRUINCOLOR2.G*6,MAINRUINCOLOR2.B*6)
	},.75,"Linear","Out",nil,false)
	--b
	weirdeff("Ring",{ -- part start properties
		Size=Vector3.new(10,10,2),
		Color=MAINRUINCOLOR2,
		Transparency = 0,
		Material="Neon",CFrame = pointb*angles(rad(90),0,0)},
	{ -- decal start properties
		Color3 = MAINRUINCOLOR2	
	},{ -- part end properties
		Size=Vector3.new(50,50,2),
		Transparency = 1,
		CFrame = pointb*angles(rad(90),0,rad(-180))
	},{ -- decal end properties
		Color3 = MAINRUINCOLOR2,
		Transparency = 1
	},1,"Quint","Out",nil,false)
	weirdeff("Wave2",{ -- part start properties
		Size=Vector3.new(10,10,10),
		Color=MAINRUINCOLOR,
		Transparency = 0,
		Material="Neon",CFrame = pointb},
	{ -- decal start properties
		Color3 = Color3.new(MAINRUINCOLOR.R*6,MAINRUINCOLOR.G*6,MAINRUINCOLOR.B*6)
	},{ -- part end properties
		Size=Vector3.new(70,10,70)/1.5,
		Transparency = 1,
		CFrame = pointb*angles(0,0,rad(360))
	},{ -- decal end properties
		Color3 = Color3.new(MAINRUINCOLOR2.R*5,MAINRUINCOLOR2.G*5,MAINRUINCOLOR2.B*5),
		Transparency = 1},1,"Quad","InOut",nil,false)
	weirdeff("Explosion2",{ -- part start properties
		Size=Vector3.new(50,.1,50),
		Color=MAINRUINCOLOR,
		Transparency = 0,
		Material="Neon",CFrame = pointb},
	{ -- decal start properties
		Color3 = Color3.new(MAINRUINCOLOR.R*5,MAINRUINCOLOR.G*5,MAINRUINCOLOR.B*5),
	},{ -- part end properties
		Size=Vector3.new(25,50,25),
		Transparency = 1,
		CFrame = pointb*cf(0,25,0)*angles(0,rad(180),0)
	},{ -- decal end properties
		Transparency = 1,
		Color3 = Color3.new(MAINRUINCOLOR2.R*6,MAINRUINCOLOR2.G*6,MAINRUINCOLOR2.B*6)},stats["ASPD"]*.35,"Linear","Out",nil,false)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*.2,'Back','Out',0,false,0
	TweenAnimate(RootJoint,{C0=cf(0,-0,0)*angles(math.rad(26.9),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1.1,-1,0)*angles(math.rad(-26.9),math.rad(4.8),math.rad(-1.3))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(0.9,-1,0)*angles(math.rad(-26),math.rad(-11.3),math.rad(4.8))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1.5,0.8,0)*angles(math.rad(125),math.rad(2.4),math.rad(-23.4))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(1.6,0.8,-0.2)*angles(math.rad(32.4),math.rad(64.9),math.rad(106.4))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =necko*cf(0,0,-0)*angles(math.rad(-11.9),math.rad(0),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	local crossa,crossb = pointa*angles(0,rad(-20),0),pointb*angles(0,rad(20),0)
	local lim = 0
	local cfrsA,cfrsB = {},{}
	CreateSound5(243711335,char,4,1.4,false)
	for i = 1,50 do
		swait()
		lim = lim + 2
		if lim >= 25 then
			lim = 1
		end

		local bolla = weirdeff("SpikeBall",{ -- part start properties
			Size=Vector3.new(.2,5+i/(math.random(10,20)/10),1+i/5),
			Color=MAINRUINCOLOR,
			Transparency = 0,
			Material="Neon",CFrame = crossa*cf(0,0,-i*3)},
		{ -- decal start properties
			Color3 = Color3.new(MAINRUINCOLOR.R*5,MAINRUINCOLOR.G*5,MAINRUINCOLOR.B*5),
		},{ -- part end properties
			Size=Vector3.new(.2,5+i,1+i/2.5),
			Transparency = 1
		},{ -- decal end properties
			Color3 = Color3.new(MAINRUINCOLOR2.R*5,MAINRUINCOLOR2.G*5,MAINRUINCOLOR2.B*5),
			Transparency =1
		},.5,"Quint","Out",nil,false,nil,nil,true) if math.random(1,2) == 1 then local hb1 = makeHitbox({CFrame = bolla.CFrame,Size = Vector3.new(45,2,45),Shape = "Ball"},5,false,true) game:GetService("Debris"):AddItem(hb1,.1) end

		local bollb = weirdeff("SpikeBall",{ -- part start properties
			Size=Vector3.new(.2,5+i/(math.random(10,20)/10),1+i/5),
			Color=MAINRUINCOLOR,
			Transparency = 0,
			Material="Neon",CFrame = crossb*cf(0,0,-i*3)},
		{ -- decal start properties
			Color3 = Color3.new(MAINRUINCOLOR2.R*5,MAINRUINCOLOR2.G*5,MAINRUINCOLOR2.B*5),
		},{ -- part end properties
			Size=Vector3.new(.2,5+i,1+i/2.5),
			Transparency = 1
		},{ -- decal end properties
			Color3 = Color3.new(MAINRUINCOLOR.R*5,MAINRUINCOLOR.G*5,MAINRUINCOLOR.B*5),
			Transparency =1},.5,"Quint","Out",nil,false,nil,nil,true)if math.random(1,2) == 1 then local hb1 = makeHitbox({CFrame = bollb.CFrame,Size = Vector3.new(45,2,45),Shape = "Ball"},5,false,true) game:GetService("Debris"):AddItem(hb1,.1) end
		if lim == 1 then
			table.insert(cfrsA,bolla.CFrame)
			table.insert(cfrsB,bollb.CFrame)
		end
	end
	attack,idlesoff = false,false
	swait(stats["ASPD"]*30)
	for _,v in pairs(cfrsA) do

		weirdeff("Explosion2",{ -- part start properties
			Size=Vector3.new(50,.1,50),
			Color=MAINRUINCOLOR,
			Transparency = 0,
			Material="Neon",CFrame = v 
		},
		{ -- decal start properties
			Color3 = Color3.new(MAINRUINCOLOR.R*5,MAINRUINCOLOR.G*5,MAINRUINCOLOR.B*5),
		},{ -- part end properties
			Size=Vector3.new(25,50,25),
			Transparency = 1,
			CFrame = v*cf(0,25,0)*angles(0,rad(180),0)
		},{ -- decal end properties
			Transparency = 1,
			Color3 = Color3.new(MAINRUINCOLOR2.R*6,MAINRUINCOLOR2.G*6,MAINRUINCOLOR2.B*6)},1.25,"Linear","Out",nil,false)
		local funny = weirdeff("sphere",{ -- part start properties
			Size=Vector3.new(40,.1,40),
			Color=MAINRUINCOLOR,
			Transparency = 0,
			Material="Neon",CFrame = v},
		{ -- decal start properties

		},{ -- part end properties
			Size=Vector3.new(60,.5,60),
			Transparency = 1
		},{ -- decal end properties

		},1.75,"Back","InOut",nil,false,nil,nil,true)
		weirdeff("Swirl",{ -- part start properties
			Size=Vector3.new(65,85,65),
			Color=MAINRUINCOLOR,
			Transparency = 0,
			Material="Neon",CFrame = v},
		{ -- decal start properties
			Color3 = vertex1
		},{ -- part end properties
			Size=Vector3.new(90,.1,90),
			Transparency = 1,
			CFrame = efspin(v,3)
		},{ -- decal end properties
			Color3 = vertex2,
			Transparency = 1
		},1.75,"Quad","Out",nil,false)
		CreateSound5(192410084,funny,5,1,false) local hb1 = makeHitbox({CFrame = funny.CFrame,Size = Vector3.new(30,30,30)*2,Shape = "Ball"},750,false,true) game:GetService("Debris"):AddItem(hb1,2)

	end
	for _,v in pairs(cfrsB) do
		weirdeff("Explosion2",{ -- part start properties
			Size=Vector3.new(50,.1,50),
			Color=MAINRUINCOLOR,
			Transparency = 0,
			Material="Neon",CFrame = v 
		},
		{ -- decal start properties
			Color3 = Color3.new(MAINRUINCOLOR.R*5,MAINRUINCOLOR.G*5,MAINRUINCOLOR.B*5),
		},{ -- part end properties
			Size=Vector3.new(1,50,1),
			Transparency = 1,
			CFrame = v*cf(0,25,0)*angles(0,rad(180),0)
		},{ -- decal end properties
			Transparency = 1,
			Color3 = Color3.new(MAINRUINCOLOR2.R*6,MAINRUINCOLOR2.G*6,MAINRUINCOLOR2.B*6)},1.25,"Linear","Out",nil,false)
		local funny = weirdeff("sphere",{ -- part start properties
			Size=Vector3.new(40,.1,40),
			Color=MAINRUINCOLOR,
			Transparency = 0,
			Material="Neon",CFrame = v},
		{ -- decal start properties

		},{ -- part end properties
			Size=Vector3.new(60,.5,60),
			Transparency = 1
		},{ -- decal end properties

		},1,"Back","InOut",nil,false,nil,nil,true)
		weirdeff("Swirl",{ -- part start properties
			Size=Vector3.new(15,85,15),
			Color=MAINRUINCOLOR,
			Transparency = 0,
			Material="Neon",CFrame = v},
		{ -- decal start properties
			Color3 = vertex1
		},{ -- part end properties
			Size=Vector3.new(90,.1,90),
			Transparency = 1,
			CFrame = efspin(v,3)
		},{ -- decal end properties
			Color3 = vertex2,
			Transparency = 1
		},1.75,"Quad","Out",nil,false) local hb1 = makeHitbox({CFrame = funny.CFrame,Size = Vector3.new(30,30,30)*2,Shape = "Ball"},750,false,true) game:GetService("Debris"):AddItem(hb1,2)
	end
end

function pounce()
	idlesoff = true
	attack = true
	local targetted = false
	local tHum = nil
	root.Anchored = true
	local DEATH = true
	local hb1 = makeHitbox({Size=Vector3.new(5,3,11),Anchored=false},0,false,false,2000000,function(hb,eroot,ehum,etors)
		if not targetted then
			targetted = eroot
			tHum = ehum
			Instance.new('CFrameValue',char).Name = 'completed'
		end
	end)
	local h = mouse.KeyUp:connect(function(k)
		if k == "x" then
			DEATH = false
		end
	end)
	local welb = CreateWeldS(root,root,hb1,CFrame.new(0,0,-5))
	-- ~-~ AFFX1 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*10/60,'Quint','InOut',0,false,0
	local eg = TweenAnimate(RootJoint,{C0=cf(0,-0.7,-0.2)*angles(math.rad(-30),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1.1,-0.7,-0.8)*angles(math.rad(-20.8),math.rad(10.2),math.rad(4.5))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(1.1,-1.1,-0.4)*angles(math.rad(-31.4),math.rad(-14.6),math.rad(-6))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1.5,-0.1,0.3)*angles(math.rad(128.7),math.rad(-55.1),math.rad(2.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(1.5,-0.1,0.3)*angles(math.rad(128.7),math.rad(55.1),math.rad(-2.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0,0.1,-0.5)*angles(math.rad(25),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	-- ~-~ AFFX2 ~-~
	hb1.Color = Color3.new(1,0,0)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*5/60,'Quint','Out',0,false,0
	local eg = TweenAnimate(root,{CFrame=root.CFrame*CFrame.new(0,0,-50)},25/60,"Sine","Out",Repeat,Reverse,Delay)
	TweenAnimate(RootJoint,{C0=cf(0,-.4,-0.4)*angles(math.rad(-55),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1,-0.9,-0.5)*angles(math.rad(-29.1),math.rad(9.5),math.rad(-5.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(1,-0.9,-0.5)*angles(math.rad(-29.1),math.rad(-9.5),math.rad(5.9))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(-1.5,0.8,0)*angles(math.rad(152.1),math.rad(-44.8),math.rad(5))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(1.5,0.8,0)*angles(math.rad(152.8),math.rad(49.7),math.rad(-5.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0,0.4,-0.7)*angles(math.rad(45),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	for i = 1,Alpha*60 do
		if targetted then break end
		swait(2)
	end
	eg:Cancel()
	hb1.Color = Color3.new(0,0,1)
	if targetted then
		targetted.Anchored = true
		-- ~-~ AFFX3SUCCEED ~-~
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 10/60,'Quad','InOut',0,false,0
		TweenAnimate(targetted,{CFrame=root.CFrame*CFrame.new(0,-.75,-2.5)*angles(math.rad(-45),math.rad(180),0)},15/60,"Quad","Out",Repeat,false,Delay)
		local eg = TweenAnimate(RootJoint,{C0=cf(0,-0.7,-0.4)*angles(math.rad(-55),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1,-0.6,-0.9)*angles(math.rad(-34.1),math.rad(9.5),math.rad(-5.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1,-0.6,-0.9)*angles(math.rad(-34.1),math.rad(-9.5),math.rad(5.9))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.7,1.3,-0.3)*angles(math.rad(161.3),math.rad(-42.4),math.rad(32.5))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.7,1.3,-0.3)*angles(math.rad(165.2),math.rad(46.5),math.rad(-35.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0,0.4,-0.7)*angles(math.rad(45),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		swait(Alpha*60)
		-- ~-~ AFFX4 ~-~
		affectionHearts(CFrame.new(root.CFrame.p:lerp(targetted.CFrame.p,0.5))*CFrame.new(0,2.5,0),5)
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 20/60,'Quad','Out',0,false,0
		TweenAnimate(targetted,{CFrame=root.CFrame*CFrame.new(0,-2.75,-0.5)*angles(math.rad(-90),math.rad(180),0)},15/60,"Quad","Out",Repeat,false,Delay)
		local eg = TweenAnimate(RootJoint,{C0=cf(-0.1,-1.8,0)*angles(math.rad(-93),math.rad(-10),math.rad(-0.5))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1.3,-0.6,-0.5)*angles(math.rad(5.7),math.rad(19.2),math.rad(-16.5))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1.3,-0.6,-0.9)*angles(math.rad(5.5),math.rad(0.8),math.rad(15.6))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.4,0.4,0.2)*angles(math.rad(-101.9),math.rad(-54.4),math.rad(94.8))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.4,1.6,-0.4)*angles(math.rad(-155.6),math.rad(16.7),math.rad(-37.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(-0.6,0.4,0.4)*angles(math.rad(45.2),math.rad(46.3),math.rad(-65))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		swait(Alpha*90)
		-- ~-~ AFFX4.5 ~-~
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 100/60,'Quad','InOut',1,true,0
		local eg = TweenAnimate(RootJoint,{C0=cf(0,-1.8,0)*angles(math.rad(-88.9),math.rad(-8.2),math.rad(-2))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1.3,-0.7,-0.4)*angles(math.rad(0.9),math.rad(17.4),math.rad(-14.2))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1.3,-0.8,-0.8)*angles(math.rad(1.5),math.rad(-0.9),math.rad(17.9))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.5,0.5,0.1)*angles(math.rad(-138.3),math.rad(-71),math.rad(56.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.3,1.7,-0.5)*angles(math.rad(178.9),math.rad(32.5),math.rad(-31.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(-0.7,0.4,0)*angles(math.rad(30.4),math.rad(28),math.rad(-47.4))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		swait(Alpha*240)
		if DEATH == true then
			CreateSound5(4591842619,root,1,1.2,false)
			CreateSound5(4196111215,root,1.25,1,false)
			local parb = Instance.new("Part")
			parb.Size = Vector3.new(0,0,0)
			parb.CanCollide = false
			parb.Transparency = 1
			local weld = CreateWeldS(larm,larm,parb,cf(0,-1,0)*angles(math.rad(90),math.rad(0),math.rad(90)))
			local AXE = parb:Clone()
