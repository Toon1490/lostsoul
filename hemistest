	hb1.Color = Color3.new(1,0,0)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*15/60,'Quad','In',0,false,0
	TweenAnimate(root,{CFrame = root.CFrame*CFrame.new(0,0,-8)},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	local eg = TweenAnimate(RootJoint,{C0=cf(0.1,-0.2,0)*angles(math.rad(7.4),math.rad(-55.3),math.rad(1))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1.1,-0.4,-0.2)*angles(math.rad(-46.4),math.rad(43.4),math.rad(13.6))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(1.1,-0.8,-0.1)*angles(math.rad(-19.4),math.rad(9.3),math.rad(31.8))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LW,{C0=cf(0.2,0.2,-1.1)*angles(math.rad(19.1),math.rad(48.6),math.rad(101))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RW,{C0=cf(0.8,0.8,-0.3)*angles(math.rad(-26.5),math.rad(65.2),math.rad(122.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(Torso.Neck,{C0 =cf(0.2,0.2,0.2)*angles(math.rad(-20.8),math.rad(47.5),math.rad(21.4))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(weldo,{C0=cf(0,0,0)*angles(math.rad(69.9),math.rad(-3.5),math.rad(90+-3.5))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	swait(Alpha*60)
	hb1.Color = Color3.new(0,0,1)
	welb:Destroy()
	e:Destroy()
	e2:Destroy()
	e = weirdeff("aur",{ -- part start properties
		Size=Vector3.new(30,30,4),
		Color=MAINRUINCOLOR,
		Transparency = 1,
		Material="Neon",
		CFrame = h*CFrame.new(0,-1,0)*angles(0,math.rad(-15),math.rad(150))*cf(0,0,0.3)
	},{ -- decal start properties
		Color3=Color3.fromRGB((MAINRUINCOLOR2.R*50)^1.85,(MAINRUINCOLOR2.G*50)^1.85,(MAINRUINCOLOR2.B*50)^1.85),
		Transparency = -.5,
		Texture = "rbxassetid://4049743792"
	},{ -- part end properties
		Size=Vector3.new(15,15,4),
		Transparency = 1,
		CFrame = h*CFrame.new(0,7,0)*angles(0,math.rad(5),math.rad(-29))*cf(0,0,0.3)
	},{ -- decal end properties
		Color3=Color3.fromRGB((MAINRUINCOLOR.R*50)^2.5,(MAINRUINCOLOR.G*50)^2.5,(MAINRUINCOLOR.B*50)^2.5),
		Transparency = -1.5
	},16/60,"Linear","In",nil,nil,nil,nil,true)
	e2 = weirdeff("aur",{ -- part start properties
		Size=Vector3.new(30,30,4),
