			Transparency = 1,
			Color=MAINRUINCOLOR2,
			CFrame = root.CFrame*CFrame.new(0,0,-125)
		},{ -- decal end properties
			Transparency = 1
		},.5,"Elastic","InOut")
		for i = 1,3 do
			local m = (math.random(25,100)/100)
			weirdeff("VBLASTMESH2",{ -- part start properties
				Size=Vector3.new(10,0,10),
				Color=MAINRUINCOLOR2,
				Transparency = .05,
				Material="Neon",
				CFrame = root.CFrame*CFrame.new(0,100,-125)
			},{ -- mesh start properties
				VertexColor = Vector3.new(MAINRUINCOLOR2.R*5,MAINRUINCOLOR2.G*5,MAINRUINCOLOR2.B*5),
				Scale = Vector3.new(1,10,1)
			},{ -- part end properties
				Transparency = 1,
				CFrame = root.CFrame*CFrame.new(0,50*m,-125)*CFrame.Angles(0,math.rad(math.random(-1,1)*90),0)
			},{ -- mesh end properties
				Scale = Vector3.new(5,5,5)*m
			},math.random(40,160)/80,"Quint","Out")
		end
		swait(5)
		hb1.Color = Color3.new(0,0,1)
	end
	if ISLAN == true then
		swait(stats["ASPD"]*10)
		ISLAN = false
		root.Anchored = false
		idlesoff = false
		attack = false
		return
	end
	swait(stats["ASPD"]*55)
	ISLAN = true
	CreateSound5("4648426102",ISLAND.PrimaryPart,4,0.6)
	local hb2 = makeHitbox({CFrame=root.CFrame*CFrame.new(0,400,-125)*angles(0,0,math.rad(90)),Size=Vector3.new(800,275,275),Shape = "Cylinder"},0,false,true,2000000,function(hb,eroot,ehum,etors)
		local bv = Instance.new("BodyVelocity")
		bv.Velocity = Vector3.new(0,150,0)
		bv.MaxForce = Vector3.new(1e9,1e11,1e9)
		bv.Parent = eroot
		game:GetService("Debris"):AddItem(bv,0.5)
	end)
	local hb3 = makeHitbox({CFrame=root.CFrame*CFrame.new(0,400,-125)*angles(0,0,math.rad(90)),Size=Vector3.new(800,300,300),Shape = "Cylinder"},0,false,false,2000000,function(hb,eroot,ehum,etors)
