local moveList = {
	norm={propulsionBurst,revolutionAxe,infernalcross,affectionHug,vengeanceYeet,RapidSlash,pounce},
	ult={EndCatalyze,PARADISE,AeroSad}
}

function randomMode()
	if math.random(1,4) ~= 4 then
		MAINRUINCOLOR = Color3.fromHSV(math.random(1,360)/360,math.random(1,100)/100,math.random(75,100)/100)
		MAINRUINCOLOR2 = Color3.fromHSV(math.random(1,360)/360,math.random(75,100)/100,math.random(1,100)/100)
	else
		MAINRUINCOLOR2 = Color3.fromHSV(math.random(1,360)/360,math.random(1,100)/100,math.random(75,100)/100)
		MAINRUINCOLOR = Color3.fromHSV(math.random(1,360)/360,math.random(75,100)/100,math.random(1,100)/100)
	end
	local auras = {7,102,21,9,55,6,5,2,1}
	local wanims = {1,7,5,50,27,6,12,16,3,101,-27,12.5}
	local music = musicList[math.random(1,#musicList)]
	Mode = {random=true,idle=math.random(1,15),aura=auras[math.random(1,#auras)],wanim=wanims[math.random(1,#wanims)]}
	RecolorTextAndRename(string.upper(nameList[math.random(1,#nameList)]),MAINRUINCOLOR,MAINRUINCOLOR2,(Enum.Font:GetEnumItems()[math.random(1,#Enum.Font:GetEnumItems())]).Name,nil,music[2])
	Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
	walkspeed = math.random(16,250)
	if math.random(1,10) == 1 then walkspeed = math.random(1,5000) end
	stats["WS"] = walkspeed
	newTheme("rbxassetid://" .. music[1],0,(music[3] or 1)*math.random(80,120)/100,2)
	modequotes = {{},{}}
	for i = 1,math.random(1,3) do
		local eg = gen() .. fromlist({".",".",".","...","!","?","?!","~"})
		if math.random(1,5) == 3 then
			eg = fromlist({fromlist(nouns),fromlist(adj),fromlist(verbos)}) .. fromlist({",",".",".",".","...","!","?","?!","~"})
		end
		eg = string.upper(string.sub(eg,1,1)) .. string.lower(string.sub(eg,2))
		table.insert(modequotes[1],eg)
		table.insert(modequotes[2],0.5 + string.len(eg)/30)
	end
	moves = {}
	moves["z"] = fromlist(moveList["norm"])
	repeat moves["x"] = fromlist(moveList["norm"]) until moves["x"] ~= moves["z"]
	repeat moves["c"] = fromlist(moveList["norm"]) until moves["c"] ~= moves["z"] and moves["c"] ~= moves["x"]
	moves["v"] = fromlist(moveList["ult"])
end

-- end random modes

mouse.Button1Down:connect(function()
	if attack == false then
		c1() -- for now
	end
end)

function round(num,numDecimalPlaces)
	local mult = 10^(numDecimalPlaces or 0)
	return math.floor(num * mult + 0.5) / mult
end

function funkyCompile() -- imagine losing 2 hours of progress to a studio crash
	local flyHeight = math.floor(RootJoint.C0.Y + .25)
	if flyHeight <= 0 then
		flyHeight = "false"
	end
	print([[
	["Q"]={Name="]]..string.upper(getModeName())..[[", Font="]]..modet1B.Font.Name..[[", Idle=]]..Mode..[[, Flying=]]..flyHeight..[[, Aura=]]..Mode..[[, Wings=]]..Mode..[[, Moves={},
		Stats={CATK=1,CSPD=1, ATK=1,ADMG=0,ASPD=1, DEF=0,DR=0, WS=]]..stats["WS"]..[[,JP=75,JUMPS=2},
		Color={M1=Color3.new(]]..round(MAINRUINCOLOR.R,2)..[[,]]..round(MAINRUINCOLOR.G,2)..[[,]]..round(MAINRUINCOLOR.B,2)..[[),M2=Color3.new(]]..round(MAINRUINCOLOR2.R,2)..[[,]]..round(MAINRUINCOLOR2.G,2)..[[,]]..round(MAINRUINCOLOR2.B,2)..[[),CS=]]..Mode..[[}, 
		Music={ID="]]..kan.SoundId..[[",Name="]]..musicval.Value..[[",Pitch=]]..kan.PlaybackSpeed..[[},
		Quotes={{"]]..table.concat(modequotes[1],'","')..[["},{]]..table.concat(modequotes[2],',')..[[}}},]])
end

