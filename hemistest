function OblitTransform()
	attack = true
	idlesoff = true
	walkspeed = 0
	char.Archivable = true -- so cloning works
	local modequote1 = {{"I'm ready..."},{1.5}}
	local modequote2 = {{"TO BREAK YOUR REALITY!","ALL SHALL TURN INTO INSANITY!"},{2,2}}
	dialog(getName(),modequote1[1],"player",modequotes[2])
	swait()
	for i = 0,150 do
		swait(0)
		local Alpha = .1
		TweenAnimate(RootJoint,{C0=cf(0,2 + .5 * math.cos(sine/34),0)*angles(math.rad(0 + 2 * math.cos(sine/34)),math.rad(25.7),math.rad(0))*RootCF},Alpha,'Linear','Out',0,false,0)
		TweenAnimate(LH,{C0=cf(-1,-1 - .1 * math.cos(sine/34),0)*angles(math.rad(-10.9 - 2 * math.cos(sine/30)),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
		TweenAnimate(RH,{C0=cf(1,-0.5 - .1 * math.cos(sine/34),-0.4)*angles(math.rad(-12.1 - 2 * math.cos(sine/30)),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
		TweenAnimate(LW,{C0=cf(-1.1,0.7 - .1 * math.cos(sine/30),0.7)*angles(math.rad(13.1),math.rad(14.4),math.rad(38.3))},Alpha,'Linear','Out',0,false,0)
		TweenAnimate(RW,{C0=cf(1.1,0.5 - .1 * math.cos(sine/30),0.2)*angles(math.rad(-19.5),math.rad(2.1),math.rad(-52.1))},Alpha,'Linear','Out',0,false,0)
		TweenAnimate(Torso.Neck,{C0 =cf(0.1,0.1,0.3)*angles(math.rad(-16.6),math.rad(-29.2),math.rad(6.5))*necko},Alpha,'Linear','Out',0,false,0)
	end
	dialog(getName(),modequote2[1],"player",modequotes[2])
	burst(Color3.new(0,0,0),Color3.new(1,1,1),3)
	idlesoff = false
	attack = false
	walkspeed = stats["WS"]
end

