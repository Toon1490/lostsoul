				Transparency = 1,
				CFrame = h*angles(0,math.rad(180),math.rad(5))*cf(0,0,0.15)
			},{ -- decal end properties
				Color3=Color3.fromRGB((MAINRUINCOLOR.R*50)^1.85,(MAINRUINCOLOR.G*50)^1.85,(MAINRUINCOLOR.B*50)^1.85),
				Transparency = 1
			},10/60,"Sine","Out")
			local e2 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(4,4,2),
				Color=MAINRUINCOLOR,
				Transparency = 1,
				Material="Neon",
				CFrame = h*cf(0,0,0.3)*angles(0,math.rad(0),math.rad(179))
			},{ -- decal start properties
				Color3=Color3.fromRGB((MAINRUINCOLOR2.R*50)^1.85,(MAINRUINCOLOR2.G*50)^1.85,(MAINRUINCOLOR2.B*50)^1.85),
				Transparency = -.5,
			},{ -- part end properties
				Size=Vector3.new(5,5,2),
				Transparency = 1,
				CFrame = h*angles(0,math.rad(0),math.rad(-5))*cf(0,0,0.15)
			},{ -- decal end properties
				Color3=Color3.fromRGB((MAINRUINCOLOR.R*50)^1.85,(MAINRUINCOLOR.G*50)^1.85,(MAINRUINCOLOR.B*50)^1.85),
				Transparency = 1
			},8/60,"Sine","Out")
			TweenAnimate(AXE,{Transparency=0},10/60)
			-- ~-~ AFFEX3 ~-~
			local Alpha,Easing,Direction,Repeat,Reverse,Delay = 15/60,'Sine','Out',0,false,0
			local eg = TweenAnimate(RootJoint,{C0=cf(0.3,-1.2,0.1)*angles(math.rad(-44),math.rad(46.2),math.rad(-11.5))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(LH,{C0=cf(-0.3,-0.1,-1.3)*angles(math.rad(-16),math.rad(-45.5),math.rad(-22.2))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(RH,{C0=cf(1.4,-0.9,0.2)*angles(math.rad(-54.7),math.rad(-48.7),math.rad(-15))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(LW,{C0=cf(-1.5,0.5,-0.2)*angles(math.rad(-139.1),math.rad(21.2),math.rad(-22.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(RW,{C0=cf(0.3,0.6,-1)*angles(math.rad(103.6),math.rad(-9.7),math.rad(-94.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(Torso.Neck,{C0 =cf(0.2,0,-0.1)*angles(math.rad(4.9),math.rad(-38.2),math.rad(9.8))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			swait(Alpha*60)
			CreateSound5(2295409459,root,1,1,false)
			CreateSound5(4002851096,root,1.25,1,false)
			swait(5)
			-- ~-~ AFFEX4 ~-~
			local h11 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(1.613, 0.05, 1.048),
				Color=MAINRUINCOLOR,
				Transparency = 1,
				Material="Neon",
				CFrame = root.CFrame*cf(-1.2,1.1,-2.6)*angles(math.rad(80),math.rad(-75),math.rad(0))
			},{ -- decal start properties
				Color3=Color3.fromRGB((MAINRUINCOLOR2.R*50)^1.85,(MAINRUINCOLOR2.G*50)^1.85,(MAINRUINCOLOR2.B*50)^1.85),
				Transparency = -3,
			},{ -- part end properties
				Size=Vector3.new(4, 0.05, 1.049),
				Transparency = 1,
				CFrame = root.CFrame*cf(-0.5,-1.1,-2.6)*angles(math.rad(80),math.rad(-75),math.rad(0))
			},{ -- decal end properties
				Color3=Color3.fromRGB((MAINRUINCOLOR.R*50)^1.85,(MAINRUINCOLOR.G*50)^1.85,(MAINRUINCOLOR.B*50)^1.85),
				Transparency = 1
			},15/60,"Sine","In",0,false,5/60)
			local h12 = weirdeff("woosh2",{ -- part start properties
				Size=Vector3.new(1.613, 0.05, 1.048),
				Color=MAINRUINCOLOR,
				Transparency = 1,
				Material="Neon",
				CFrame = root.CFrame*cf(-0.9,1.2,-2.6)*angles(math.rad(-100),math.rad(75),math.rad(-180))
			},{ -- decal start properties
				Color3=Color3.fromRGB((MAINRUINCOLOR2.R*50)^1.85,(MAINRUINCOLOR2.G*50)^1.85,(MAINRUINCOLOR2.B*50)^1.85),
				Transparency = -3,
			},{ -- part end properties
				Size=Vector3.new(4, 0.05, 1.049),
				Transparency = 1,
				CFrame = root.CFrame*cf(-0.2,-1,-2.6)*angles(math.rad(-100),math.rad(75),math.rad(-180))
			},{ -- decal end properties
				Color3=Color3.fromRGB((MAINRUINCOLOR.R*50)^1.85,(MAINRUINCOLOR.G*50)^1.85,(MAINRUINCOLOR.B*50)^1.85),
				Transparency = 1
			},15/60,"Sine","In",0,false,5/60)
			local h21 = weirdeff("woosh",{ -- part start properties
				Size=Vector3.new(1.613, 0.05, 1.048),
				Color=MAINRUINCOLOR,
				Transparency = 1,
				Material="Neon",
				CFrame = root.CFrame*cf(-1.1,1.1,-2.8)*angles(math.rad(170),math.rad(0),math.rad(75))
			},{ -- decal start properties
				Color3=Color3.fromRGB((MAINRUINCOLOR2.R*50)^1.85,(MAINRUINCOLOR2.G*50)^1.85,(MAINRUINCOLOR2.B*50)^1.85),
				Transparency = -3,
			},{ -- part end properties
				Size=Vector3.new(4, 0.05, 1.049),
				Transparency = 1,
				CFrame = root.CFrame*cf(-0.3,-1.1,-2.8)*angles(math.rad(170),math.rad(0),math.rad(75))
			},{ -- decal end properties
				Color3=Color3.fromRGB((MAINRUINCOLOR.R*50)^1.85,(MAINRUINCOLOR.G*50)^1.85,(MAINRUINCOLOR.B*50)^1.85),
