		RotSpeed=NR(-150,150),
		Rotation=NR(1,360),
		Transparency=NS({NSK(0,1,0),NSK(0.2,.25,0),NSK(1,0,0)}),
	},Vector3.new(0,-1,0))
	CreateSound5("4836146175",root,1,0.7,false)
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 45/60,'Quad','Out',0,false,0
	if not flying then
		TweenAnimate(RootJoint,{C0=cf(0,-.2,0.1)*angles(math.rad(10),math.rad(-46.1),math.rad(-7))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1.2,-1.2,0.2)*angles(math.rad(-21.4),math.rad(43.7),math.rad(22.2))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1,-0.6,-0.4)*angles(math.rad(-36.7),math.rad(-18.7),math.rad(38.9))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.6,0.4,-0.6)*angles(math.rad(169.5),math.rad(41),math.rad(-15.2))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.4,0.2,0.4)*angles(math.rad(85.7),math.rad(-8.5),math.rad(40.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(-0.2,0,-0.2)*angles(math.rad(17.1),math.rad(44.7),math.rad(-11.4))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	else
		TweenAnimate(RootJoint,{C0=cf(0.1,1.5,0.1)*angles(math.rad(31.1),math.rad(-35.3),math.rad(8.4))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1.1,-1.1,0)*angles(math.rad(-45),math.rad(-7.4),math.rad(20.3))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(0.9,-0.3,-0.8)*angles(math.rad(-31.5),math.rad(-2.8),math.rad(11.8))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.7,0.7,-0.7)*angles(math.rad(155.5),math.rad(53),math.rad(-3.4))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.3,0.3,0.6)*angles(math.rad(70.2),math.rad(-31.2),math.rad(35.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(-0.2,0.1,0.1)*angles(math.rad(5.7),math.rad(44.8),math.rad(-11.9))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	end
	eg.Completed:connect(function()
		chargey:Destroy()
		chargey2:Destroy()
		CreateSound5("4836147040",root,1.5,0.7,false)
		CreateSound5("4701312550",root,0.75,1.5,false)
		weirdeff("sphere",{ -- part start properties
			Size=Vector3.new(30,30,30),
			Color=cl1,
			Transparency = 0,
			Material="Neon",
			CFrame = root.CFrame
		},{ -- decal start properties, but sphere has no decals
		},{ -- part end properties
			Size=Vector3.new(150,150,150)*(size or 1),
			Transparency = 1
		},{ -- decal end properties
		},1,"Quad","Out")
		weirdeff("sphere",{ -- part start properties
			Size=Vector3.new(30,30,30),
			Color=cl2,
			Transparency = 0,
			Material="ForceField",
			CFrame = root.CFrame
		},{ -- decal start properties, but sphere has no decals
		},{ -- part end properties
			Size=Vector3.new(200,200,200)*(size or 1),
			Transparency = 1
		},{ -- decal end properties
		},.25,"Quad","Out",3)
		particle(root,300,{
			Texture="rbxassetid://243728104",
			LightEmission=1,
			Color=CS(cl1),
			Lifetime=NR(.5,2),
			Size=NS({NSK(0,20*(size or 1),10*(size or 1)),NSK(0.3,30*(size or 1),15*(size or 1)),NSK(0.5,40*(size or 1),20*(size or 1)),NSK(0.7,50*(size or 1),25*(size or 1)),NSK(1,60*(size or 1),30*(size or 1))}),
			Transparency=NS({NSK(0,0,0),NSK(0.2,0.7,0.2),NSK(1,1,0)}),
			Speed=NR(100*(size or 1),500*(size or 1)),
			Drag=5,
			SpreadAngle=Vector2.new(360,360),
			RotSpeed=NR(-200,200),
			Rotation=NR(1,360)
		})
		particle(root,5,{ZOffset=5,
			Texture="rbxassetid://1053546634",
			LightEmission=1,
			Color=CS(Color3.new(1,1,1)),
			Lifetime=NR(1),
			Size=NS({NSK(0,200,0),NSK(0.1,100,0),NSK(0.3,50,0),NSK(0.4,10,0),NSK(1,0,0)})
		})
		for i = 1,10 do
			weirdeff("VBLASTMESH2",{ -- part start properties
				Size=Vector3.new(10,0,10)*(size or 1),
				Color=MAINRUINCOLOR,
				Transparency = .05,
				Material="Neon",
				CFrame = root.CFrame*CFrame.new(0,-3,0)*randr()
			},{ -- mesh start properties
				VertexColor = Vector3.new(cl2.R*5,cl2.G*5,cl2.B*5),
				Scale = Vector3.new(0,0,0)
			},{ -- part end properties
				Transparency = 1
			},{ -- mesh end properties
				Scale = Vector3.new(2.5,0,2.5)*(size or 1)*(math.random(30,100)/100)
			},math.random(40,160)/100,"Quad","Out")
		end
		--[[local Alpha,Easing,Direction,Repeat,Reverse,Delay = 10/60,'Quint','Out',0,false,0
		eg = TweenAnimate(RootJoint,{C0=cf(0.1,0,-0.1)*angles(math.rad(-14.8),math.rad(-43.9),math.rad(-6.8))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1.4,-0.8,-0.5)*angles(math.rad(0),math.rad(30),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(0.9,-0.9,0)*angles(math.rad(-9.1),math.rad(-16.9),math.rad(7.7))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.4,0.2,-0.8)*angles(math.rad(180),math.rad(55),math.rad(-20))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.6,-0.1,-0.1)*angles(math.rad(139.3),math.rad(-44.4),math.rad(7.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0.1,0,-0.1)*angles(math.rad(0),math.rad(40),math.rad(5))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		swait(Alpha*60)
		swait(30)]]
		idlesoff = false
		attack = false
	end)
	repeat swait() until attack == false
	pitch:Cancel()
end

loadwings(wings[aaaa["wing"]])
if aaaa["wing"] == "Star" or aaaa["wing"] == "Stratospheric" or aaaa["wing"] == "None" then
	if aaaa["wing"] == "None" then
		prankgui.Enabled = true
	end
	bguilt.Enabled = false
	progui.Enabled = false
end

-- Tween Values. --
function TweenAnimate(object,Aniim,time,easingstyle,easingdirection,repet,reverse,deelay)
	local infos=TweenInfo.new(time or (20/60),Enum.EasingStyle[easingstyle or "Linear"],Enum.EasingDirection[easingdirection or "InOut"],repet or 0,reverse or false,deelay or 0)
	local tween=tweenService:Create(object,infos,Aniim)
	tween:Play()
	return tween
end

function LocalTweenAnimate(object,Aniim,time,easingstyle,easingdirection,repet,reverse,deelay)
	script.LocalTweenyThing:InvokeClient(Player,object,Aniim,time,easingstyle,easingdirection,repet,reverse,deelay)
end

local recoltweens = {}

function CharacterThing(one,two)
	-- rce cytus
	local egg = char:FindFirstChild("RCECytus")
	if egg then
		local matTable = {["0.2"]="Metal",["0.4"]="Fabric",["0.6"]="SmoothPlastic",["0.8"]="Glass",["1"]="Sand"}
		local epik = {}
		epik["h1"],epik["s1"],epik["v1"] = Color3.toHSV(one)
		epik["h2"],epik["s2"],epik["v2"] = Color3.toHSV(two)
		epik["h3"],epik["s3"],epik["v3"] = Color3.toHSV(one:lerp(two,0.5))
		local function recol(index,col)
			local h,s,va = Color3.toHSV(col)
			local transped = false
			for i,v in pairs(rcCol[index]) do
				v.Color = col
				if ((s > 0.9 or s < 0.1) and v.Name ~= "dress" and v.Name ~= "hair") then
					v.Transparency = 1
					if not transped then
						for i,v in pairs(egg:GetDescendants()) do 
							if (v.Name == "misc" or v.Name == "misc2" or v.Name == "neon") then
								v.Transparency = 1
							end
						end
					end
					transped = true
				elseif v.Name ~= "misc2" and v.Name ~= "misc" and v.Name ~= "hair" and v.Name ~= "neon" then
					for h,f in pairs(matTable) do
						if va < tonumber(h) then
							v.Material = f
						end
					end
				end
			end
		end
		for i,v in pairs(egg:GetDescendants()) do
			if v.Parent.Name == "styles" and v.Parent:FindFirstChild("colorBase") and v.ClassName == "Folder" then
				local e = epik[v.Parent.colorBase.Value .. v.Parent.numBase.Value]
				if v.Parent.colorBase.Value == "h" then e = e*360 else e = e*100 end
				if e >= v.min.Value - .1 and e <= v.max.Value + .1 then
					for f,h in pairs(v:GetChildren()) do
						if h:IsA("BasePart") then h.Transparency = 0 end
					end
				else
					for f,h in pairs(v:GetChildren()) do
						if h:IsA("BasePart") then h.Transparency = 1 end
					end
				end
			end
		end
		recol("mc1",MAINRUINCOLOR)
		recol("mc2",MAINRUINCOLOR2)
		recol("lc2",MAINRUINCOLOR:lerp(MAINRUINCOLOR2,0.25))
		recol("lc1",MAINRUINCOLOR2:lerp(MAINRUINCOLOR,0.25))
		recol("acc",MAINRUINCOLOR:lerp(MAINRUINCOLOR2,0.5))
		local closedids = {["45"]="rbxassetid://2801584512",["150"]="rbxassetid://2801584953",["210"]="rbxassetid://2801585179",["330"]="rbxassetid://2801585179",["360"]="rbxassetid://2801584953"}
		local openids = {["45"]="rbxassetid://2801605473",["90"]="rbxassetid://3523957747",["150"]="rbxassetid://2801605712",["265"]="rbxassetid://4823991845",["300"]="rbxassetid://2801594656",["330"]="rbxassetid://2801605712",["360"]="rbxassetid://4824052971"}
		local mouhtids = {["25"]="rbxassetid://2801751305",["45"]="rbxassetid://2801767539",["75"]="rbxassetid://2813766627",["120"]="rbxassetid://2801778269",["165"]="rbxassetid://2801778560",["210"]="rbxassetid://2801732091",["255"]="rbxassetid://2801767831",["285"]="rbxassetid://2801760198",["315"]="rbxassetid://2818152574",["345"]="rbxassetid://2801759774",["360"]="rbxassetid://2801751305"}
		local function facething(dec,list,col)
			local h,s,va = Color3.toHSV(col)
			local last = 0
			local liste = {}
			for i,v in pairs(list) do
				table.insert(liste,{tonumber(i),v})
			end
			table.sort(liste,function(a,b) return a[1] < b[1] end)
			for i,v in pairs(liste) do
				if v[1] > h*360 and tonumber(last) < h*360 then
					dec.Image = v[2]
					break
				end
				last = v[1]
			end
		end
		local face = egg:FindFirstChild("face",true)
		if epik["v1"] <= 0.7 then
			facething(face.face.leye,closedids,MAINRUINCOLOR)
		else
			facething(face.face.leye,openids,MAINRUINCOLOR)
		end
		if epik["v1"] <= 0.4 then
			facething(face.face.reye,closedids,MAINRUINCOLOR)
		else
			facething(face.face.reye,openids,MAINRUINCOLOR)
		end
		if epik["v2"] <= 0.4 then
			facething(face.face.mouth,{["360"]="rbxassetid://2801759987"},MAINRUINCOLOR:lerp(MAINRUINCOLOR2,0.5))
		else
			facething(face.face.mouth,mouhtids,MAINRUINCOLOR:lerp(MAINRUINCOLOR2,0.5))
		end
	end
	-- cytus, vida, nightfall, and emi
	local egg = char:FindFirstChild("Cytus") or char:FindFirstChild("Vida") or char:FindFirstChild("Nightfall") or char:FindFirstChild("Emi")

	if egg then
		for i,v in pairs(egg.mrc1:GetChildren()) do
			v.Color = MAINRUINCOLOR
		end
		for i,v in pairs(egg.mrc2:GetChildren()) do
			v.Color = MAINRUINCOLOR2
		end
		if egg:FindFirstChild("acc") then
			for i,v in pairs(egg.acc:GetChildren()) do
				v.Color = MAINRUINCOLOR:lerp(MAINRUINCOLOR2,0.5)
			end
		end
		if egg:FindFirstChild("hed") then
			if egg.hed:FindFirstChild("it") then
				local eye = egg.hed.it:FindFirstChild("eye")
				local mouf = egg.hed.it:FindFirstChild("Decal")
				local eyeb = egg.hed.it:FindFirstChild("eyeb")
				local extr = egg.hed.it:FindFirstChild("extra")
				if eye and mouf then
					eyeb.Texture = ""
					extr.Texture = ""
					local h,s,v = Color3.toHSV(one:lerp(two,0.5))
					h = h * 360
					if v <= 0.4 then
						eye.Texture = "rbxassetid://2801585179"
					elseif v <= 0.55 then
						eye.Texture = "rbxassetid://2801605473"
					elseif s <= 0.35 then
						eye.Texture = "rbxassetid://2801605712"
					else
						eye.Texture = "rbxassetid://2801594973"
					end
					if s >= 0.35 then
						if h > 345 or h <= 25 then -- red
							--eyeb.Texture = "rbxassetid://2801696705"
							mouf.Texture = "rbxassetid://2801751305"
						elseif h > 25 and h <= 45 then -- orange
							--eyeb.Texture = "rbxassetid://2801716326"
							mouf.Texture = "rbxassetid://2801767539"
						elseif h > 45 and h <= 75 then -- yellow
							mouf.Texture = "rbxassetid://2813766627"
						elseif h > 75 and h <= 120 then -- green 1
							mouf.Texture = "rbxassetid://2801778269"
						elseif h > 120 and h <= 165 then -- green 2
							mouf.Texture = "rbxassetid://2801778560"
						elseif h > 165 and h <= 210 then -- cyan
							mouf.Texture = "rbxassetid://2801732091"
						elseif h > 210 and h <= 255 then -- blue
							mouf.Texture = "rbxassetid://2801767831"
						elseif h > 255 and h <= 285 then -- purple
							--eyeb.Texture = "rbxassetid://2801687922"
							mouf.Texture = "rbxassetid://2801760198"
						elseif h > 285 and h <= 315 then -- magenta/pink
							mouf.Texture = "rbxassetid://2818152574"
						elseif h > 315 and h <= 345 then -- moai's favorite color
							extr.Texture = "rbxassetid://3523974768"
							--eyeb.Texture = "rbxassetid://2801716156"
							mouf.Texture = "rbxassetid://2801759774"
						end
					else
						mouf.Texture = "rbxassetid://2801759987"
					end
				end
			end
		end
	end
	-- moai
	local egg = char:FindFirstChild("apocalypse",true)
	if egg then
		egg.fear.mrc1.ImageColor3 = one:lerp(Color3.new(1,1,1),.25)
		local mouf = egg.fear.mouth
		local h,s,v = Color3.toHSV(one)
		h = h * 360
		egg.fear.closedeye.Image = "rbxassetid://2801585179"
		if s >= 0.1 then
			if h > 345 or h <= 25 then -- red
				egg.fear.closedeye.Image = "rbxassetid://2801584512"
				mouf.Image = "rbxassetid://2801751305"
			elseif h > 25 and h <= 45 then -- orange
				mouf.Image = "rbxassetid://2801767539"
			elseif h > 45 and h <= 75 then -- yellow
				mouf.Image = "rbxassetid://2813766627"
			elseif h > 75 and h <= 120 then -- green 1
				egg.fear.closedeye.Image = "rbxassetid://2801584953"
				mouf.Image = "rbxassetid://2801778269"
			elseif h > 120 and h <= 165 then -- green 2
				egg.fear.closedeye.Image = "rbxassetid://2801584953"
				mouf.Image = "rbxassetid://2801778560"
			elseif h > 165 and h <= 210 then -- cyan
				egg.fear.closedeye.Image = "rbxassetid://2801584953"
				mouf.Image = "rbxassetid://2801732091"
			elseif h > 210 and h <= 255 then -- blue
				mouf.Image = "rbxassetid://2801767831"
			elseif h > 255 and h <= 285 then -- purple
				egg.fear.closedeye.Image = "rbxassetid://2818161064"
				mouf.Image = "rbxassetid://2801760198"
			elseif h > 285 and h <= 315 then -- magenta/pink
				mouf.Image = "rbxassetid://2818152574"
			elseif h > 315 and h <= 345 then -- moai's favorite color
				egg.fear.closedeye.Image = "rbxassetid://2801584953"
				mouf.Image = "rbxassetid://2813765960"
			end
		else
			mouf.Image = "rbxassetid://2801759987"
		end
	end
	-- aero
	egg = char:FindFirstChild("aeroFace",true)
	if egg then
		local mouf = egg
		-- Smile --
		if Mode == 1 or Mode == 21 or Mode == 2 or Mode == 3 or Mode == 4 or Mode == 22 or Mode == 29 or Mode == 14 or Mode == 50 or Mode == 24 or Mode == 103 then
			mouf.Texture = "rbxassetid://45095238"
			-- Frown -- 
		elseif Mode == 995 or Mode == 6 or Mode == 5 or Mode == 58 or Mode == 56 or Mode == 13 or Mode == 15 or Mode == 27 or Mode == 9999 or Mode == 70051 or Mode == 19 or Mode == 102 then
			mouf.Texture = "rbxassetid://3908768038"
			-- Side Frown --
		elseif Mode == 0 or Mode == 55 then
			mouf.Texture = "rbxassetid://4977349829"
			-- Side Smile --
		elseif Mode == 9 or Mode == 12.5 or Mode == 11 or Mode == 12 or Mode == 16 or Mode == 17 or Mode == 18 or Mode == 28 or Mode == -21  or Mode == 101 or Mode == 500 or Mode == 501 then
			mouf.Texture = "rbxassetid://5242446351"
			-- Right Side Smile
		elseif Mode == 100 then
			mouf.Texture = "rbxassetid://5020833462"
			-- Poked Cheek Thing, haha indolent go brrrr --
		elseif Mode ==  78 then
			mouf.Texture = "rbxassetid://2813766627"
			-- Default Aero face --
		else
			mouf.Texture = "rbxassetid://45095238"
		end
	end
end

function Recolor(one,two,three,four,five,exonetran,exone,extwotran,extwo,secondaryenabled,sectrailenabled)
	beans,bean,be = Color3.toHSV(one)
	beans2,bean2,be2 = Color3.toHSV(two)
	local Wings = Mode
	local Aura = Mode
	if typeof(Aura) == typeof({"bruh"}) then
		if Aura["newdata"] == true then
			Aura = Aura["Aura"]
		else
			Aura = Aura["aura"]
		end
	end
	if typeof(Wings) == typeof({"bruh"}) then
		if Wings["newdata"] == true then
			Wings = Wings["Wings"]
		else
			Wings = Wings["wanim"]
		end
	end
	Whatever("wings",Mode,Wings,Aura,sine,handleweld,lwing1weld,lwing2weld,lwing3weld,rwing1weld,rwing2weld,rwing3weld,root,MAINRUINCOLOR,MAINRUINCOLOR2,kan,char,Effects)
	for i,v in pairs(recoltweens) do
		if v.PlaybackState ~= Enum.PlaybackState.Completed then
			v:Cancel()
		end
	end
	recoltweens = {}
	for i, v in pairs(mw2:GetDescendants()) do
		if v:IsA("BasePart") or v:IsA("UnionOperation") then
			v.Color = one
			v.Material = "Neon"
			if v.Name == "Color2" then
				v.Color = two
			end
		elseif v:IsA("Trail") or v:IsA("ParticleEmitter") or v:IsA("Beam") then
			v.Color = ColorSequence.new(two)
		end
	end
	for i, v in pairs(mw1:GetDescendants()) do
		if v:IsA("BasePart") or v:IsA("UnionOperation") then
			v.Color = one
			v.Material = "Neon"
			if v.Name == "Color2" then
				v.Color = two
			end
		elseif v:IsA("Trail") or v:IsA("ParticleEmitter") or v:IsA("Beam") then
			v.Color = ColorSequence.new(two)
		end
	end
	CharacterThing(one,two)
end

function TweenRecolor(one,two,timee,easingstyle,easingdirection)
	for i, v in pairs(mw2:GetDescendants()) do
		if v:IsA("BasePart") or v:IsA("UnionOperation") then
			table.insert(recoltweens,TweenAnimate(v,{Color=one},timee,easingstyle,easingdirection))
			v.Material = "Neon"
			if v.Name == "Color2" then
				table.insert(recoltweens,TweenAnimate(v,{Color=two},timee,easingstyle,easingdirection))
			end
		elseif v:IsA("Trail") or v:IsA("ParticleEmitter") or v:IsA("Beam") then
			v.Color = ColorSequence.new(two)
		end
	end
	for i, v in pairs(mw1:GetDescendants()) do
		if v:IsA("BasePart") or v:IsA("UnionOperation") then
			table.insert(recoltweens,TweenAnimate(v,{Color=one},timee,easingstyle,easingdirection))
			v.Material = "Neon"
			if v.Name == "Color2" then
				table.insert(recoltweens,TweenAnimate(v,{Color=two},timee,easingstyle,easingdirection))
			end
		elseif v:IsA("Trail") or v:IsA("ParticleEmitter") or v:IsA("Beam") then
			v.Color = ColorSequence.new(two)
		end
	end
end

function RecolorQ(one,two,three,four,five,exonetran,exone,extwotran,extwo,secondaryenabled,sectrailenabled)
	for i, v in pairs(mw2:GetDescendants()) do
		if v:IsA("BasePart") or v:IsA("UnionOperation") then
			v.Color = one
			v.Material = "Neon"
			if v.Name == "Color2" then
				v.Color = two
			end
		elseif v:IsA("Trail") or v:IsA("ParticleEmitter") or v:IsA("Beam") then
			v.Color = ColorSequence.new(two)
		end
	end
	for i, v in pairs(mw1:GetDescendants()) do
		if v:IsA("BasePart") or v:IsA("UnionOperation") then
			v.Color = one
			v.Material = "Neon"
			if v.Name == "Color2" then
				v.Color = two
			end
		elseif v:IsA("Trail") or v:IsA("ParticleEmitter") or v:IsA("Beam") then
			v.Color = ColorSequence.new(two)
		end
	end
end

Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)

-- Tween Values. --
local colors = {MAINRUINCOLOR,MAINRUINCOLOR2}

-- Start of Loops --
-- End of Loops --

-- moai's stupid chat function
function dialog(name,texts,image,times,sound,imagestuff,noup)
	local epic = Instance.new("Sound")
	epic.SoundId = sound or "rbxassetid://525200869"
	epic.Volume = .5
	epic.Parent = workspace
	epic:Play()
	for i,v in pairs(players:GetPlayers()) do
		coroutine.resume(coroutine.create(function()
			local chat = resources.chat:Clone()
			chat.Frame.name.Text = name
			chat.Frame.TextLabel.Text = name
			chat.Frame.image.Image = image
			chat.Frame.name.TextColor3 = MAINRUINCOLOR2:lerp(Color3.new(1,1,1),.5)
			chat.Frame.name.TextStrokeColor3 = MAINRUINCOLOR:lerp(Color3.new(0,0,0),.5)
			chat.Frame.name.BorderColor3 = MAINRUINCOLOR
			chat.Frame.name.BackgroundColor3 = MAINRUINCOLOR2:lerp(Color3.new(0,0,0),.5)
			chat.Frame.image.BorderColor3 = MAINRUINCOLOR
			chat.Frame.image.BackgroundColor3 = MAINRUINCOLOR2:lerp(Color3.new(0,0,0),.25)
			chat.Frame.TextLabel.BorderColor3 = MAINRUINCOLOR
			chat.Frame.TextLabel.BackgroundColor3 = MAINRUINCOLOR2:lerp(Color3.new(1,1,1),.125)
			if image == "player" then
				local fdansf = Instance.new("ViewportFrame")
				fdansf.Size = UDim2.new(1,0,1,0)
				fdansf.BackgroundTransparency = 1
				fdansf.Parent = chat.Frame.image
				chat.Frame.image.ImageTransparency = 1
				char.Archivable = true -- so cloning works
				local sss = char:Clone()
				for i,v in pairs(clone) do
					v:Clone().Parent = sss
				end
				for i,v in pairs(sss:GetChildren()) do
					if v:IsA("Script") then v:Destroy() end
					if v:IsA("BasePart") then v.Anchored = true end
				end
				sss:SetPrimaryPartCFrame(CFrame.new(0,0,0))
				sss.Parent = fdansf
				local came = Instance.new("Camera")
				fdansf.CurrentCamera = came
				came.CFrame = sss.Torso.CFrame*CFrame.new(0,1.25,-4)
				came.CFrame = came.CFrame:lerp(CFrame.new(came.CFrame.p,sss.Head.CFrame.p),.95)
				if Mode == 50 then -- nothing about maelstrom is normal
					came.CFrame = came.CFrame*CFrame.Angles(0,0,math.rad(180))
				end
				came.Parent = fdansf
			end
			for d,g in pairs(imagestuff or {}) do
				chat.Frame.image[d] = g
			end
			local upp = 0
			if not noup then
				for i,v in pairs(v.PlayerGui:GetChildren()) do
					if v.Name == "chat" then
						upp = upp - 1
					end
				end
			else
				upp = noup
			end
			chat.Parent = v.PlayerGui
			TweenAnimate(chat.Frame,{AnchorPoint=Vector2.new(0,1),Position = UDim2.new(0,10,1,-10 + 200*upp)},0.5,"Quint","Out",0,false,0)
			swait(10)
			for i,text in pairs(texts) do
				if text == "NOW, FACE MY TRUE POWER!" then
					chat.Frame.TextLabel.actualtext.Position = UDim2.new(0,0,0,11)
					chat.Frame.TextLabel.actualtext.TextColor3 = Color3.new(1,1,1)
					TweenAnimate(chat.Frame.TextLabel.actualtext,{Position=UDim2.new(0,0,0,1)},1/60,"Linear","Out",-1,true)
				end
				for x,v in pairs(string.split(text,"")) do
					chat.Frame.TextLabel.actualtext.Text = chat.Frame.TextLabel.actualtext.Text .. v
					swait(2)
				end
				swait(times[i]*60)
				chat.Frame.TextLabel.actualtext.Text = ""
			end
			chat.Name = "dumpy"
			TweenAnimate(chat.Frame,{AnchorPoint=Vector2.new(0,0),Position = UDim2.new(0,10,1,10)},0.75,"Quint","In",0,false,0)
			wait(0.75)
			chat:Destroy()
			epic:Destroy()
		end))
	end
end

-- Final Functions and variables --
function rayCast(Pos, Dir, Max, Ignore)  -- Origin Position , Direction, MaxDistance , IgnoreDescendants
	return workspace:FindPartOnRay(Ray.new(Pos, Dir.unit * (Max or 999.999)), Ignore) 
end 
