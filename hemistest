}


--[[
	CATK - click attack power
	CSPD - click attack speed
	ATK - damage multiplier
	ADMG - damage addition
	ASPD - attack speed
	DEF - additive damage reduction
	DR - multiplicative damage reduction
	WS - walkspeed
	JP - jump power
	JUMPS - how many times you can jump
]]

