function statThing()
	for i,v in pairs(statStuff["Base"]) do
		stats[i] = (v + (statStuff["Add"][i] or 0)) * (statStuff["Multiply"][i] or 1)
	end
end

local old = Hum.Health
local hdebounce = false

Hum.HealthChanged:connect(function(new)
	hdebounce = true
	swait()
	local change = new - old
	if change < 0 then
		local regen = (math.abs(change*(stats["DR"])) + stats["DEF"])
		print(change)
		print(regen)
		if regen < math.abs(change) then
			print("blocked " .. regen)
			Hum.Health = new + regen
		else
			print("blocked entirely")
			Hum.Health = old
		end
	end
	old = Hum.Health
	hdebounce = false
end)

statThing()

--local plrName = "Zombieza4569" --player.Name
-- We're all Aeros here.

function getMode(list,k)
	if list["Access"] then
		if not list["Access"][Plr.Name] then
			return
		end
	end
	if list["Transform"] and k == "m" then
		list["Transform"]["func"](list["Color"]["M1"],list["Color"]["M2"],list["Transform"]["p"])
	end
	MAINRUINCOLOR = list["Color"]["M1"]
	MAINRUINCOLOR2 = list["Color"]["M2"]
	Mode={newdata=true,Idle=list["Idle"],Aura=list["Aura"],Wings=list["Wings"],CSType=list["Color"]["CS"],flying=list["Flying"],b=list["B"],n=list["N"],m=list["M"]}
	RecolorTextAndRename(list["Name"],MAINRUINCOLOR,MAINRUINCOLOR2,list["Font"],nil,list["Music"]["Name"])
	Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
	statStuff["Base"] = list["Stats"]
	statThing()
	walkspeed = stats["WS"]
	jumppower = stats["JP"]
	newTheme(list["Music"]["ID"],0,list["Music"]["Pitch"],musicvol)
	modequotes = list["Quotes"]
	local glide = moves["gliding"]
	moves = list["Moves"]
	moves["gliding"] = nil
	moves["jumped"] = 0
	if tonumber(Mode["flying"]) == Mode["flying"] and glide then
		moves["gliding"] = glide
		moves["jumped"] = 9999
	elseif glide then
		glide:Destroy()
	end
end

musicvol = 3

keysDown = {}

function keydown(k)
	if k ~= nil then
		keysDown[k] = true

		local keepQuoted = keepQuote
		if keepQuoted then
			keepQuoted = modequotes
		end

		if k == "one" then page = 1 elseif k == "two" then page = 2 elseif k == "three" and Developers[Plr.Name] then page = 3 end

		if page == 3 then
			if k == "rightcontrol" and not attack then
				if keysDown["w"] then
					dash("w")
				elseif keysDown["s"] then
					dash("s")
				elseif keysDown["a"] then
					dash("a")
				elseif keysDown["d"] then
					dash("d")
				elseif (tonumber(Mode["flying"]) or 0) > 0 then
					if not moves["gliding"] then
						local bv = Instance.new("BodyVelocity")
						bv.Velocity = Vector3.new(root.Velocity.X,math.abs(root.Velocity.Y + 100),root.Velocity.Z)*2
						bv.MaxForce = Vector3.new(1e10,1e10,1e10)
						bv.Parent = root
						moves["gliding"] = bv
					else
						moves["gliding"]:Destroy()
						moves["gliding"] = nil
					end
				end
			elseif k == "space" and not attack and ((tonumber(moves["jumped"]) or 0) < ((stats["JUMPS"] - 1) or 0)) and (Anim == "Jump" or Anim == "Fall") then
				doublejump()
			elseif k == "space" and not attack and (Anim == "Jump" or Anim == "Fall" or moves["gliding"]) and (tonumber(Mode["flying"]) or 0) > 0 then
				if not moves["gliding"] then
					local bv = Instance.new("BodyVelocity")
					bv.Velocity = Vector3.new(root.Velocity.X,math.abs(root.Velocity.Y),root.Velocity.Z)*2
					bv.MaxForce = Vector3.new(1e10,1e10,1e10)
					bv.Parent = root
					moves["gliding"] = bv
				else
					moves["gliding"]:Destroy()
					moves["gliding"] = nil
				end
			end
			local eg,bon = pcall(function()
				if Modes[string.upper(k)] then
					getMode(Modes[string.upper(k)],k)
				elseif Modes[Mode[k]] then
					getMode(Modes[Mode[k]],k)
				end
			end)
			if not eg then print(bon) end
		elseif page == 1 then

			if k == "q" and Mode ~= 1 and attack == false then
				MAINRUINCOLOR = BrickColor.new("Pastel light blue").Color
				MAINRUINCOLOR2 = BrickColor.new("Cool yellow").Color
				Mode = 1
				RecolorTextAndRename("EXHALTION",MAINRUINCOLOR,MAINRUINCOLOR2,"Code",nil,"ARForest - Hemisphere")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 70
				stats["WS"] = 70
				newTheme("rbxassetid://4272487115",0,1,musicvol)
			end

			if k == "m" and Mode ~= 100 and Mode == 3 and (Plr.Name == "Zombieza4569") and attack == false then
				MAINRUINCOLOR = Color3.new(0,.5,1) -- .3,.2,0
				MAINRUINCOLOR2 = Color3.new(.3,0,1) -- 1,.5,.3
				Mode = 100
				RecolorTextAndRename("RETALIATION",MAINRUINCOLOR,MAINRUINCOLOR2,"Gotham",nil," - Diastrophism (''Anamorphism'' Long ver.)") -- hi again moai
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 650
				stats["WS"] = 650
				newTheme("rbxassetid://5020792265",0,1,7)
				modequotes = {{"I won't take this anymore from you.", "You're gonna have to try harder."},{1.5,2}}
			end

			if k == "n" and Mode ~= 2 and Mode == 1 and attack == false then
				MAINRUINCOLOR = Color3.new(.45,.6,1)
				MAINRUINCOLOR2 = Color3.new(.65,.45,1)
				Mode = 2
				RecolorTextAndRename("STRATOSPHERIC",MAINRUINCOLOR,MAINRUINCOLOR2,"Code",nil,"Ryoran Kazuki - An")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 70
				stats["WS"] = 70
				newTheme("rbxassetid://3669988686",0,1,musicvol)
				modequotes = {{"Hmm..", "..I think we've met before."},{1,2}}
			end

			if k == "m" and Mode ~= 3 and Mode == 1 and attack == false then -- yes
				MAINRUINCOLOR = Color3.new(.55,.7,1)
				MAINRUINCOLOR2 = Color3.new(1,.65,.5)
				Mode = 3
				RecolorTextAndRename("ASCENDANT",Color3.new(1,1,1),BrickColor.new(.35,.7,2).Color,"SciFi",nil,"[BOFXV]")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,0,MAINRUINCOLOR,0,MAINRUINCOLOR,true,true)
				walkspeed = 250
				stats["WS"] = 250
				newTheme("rbxassetid://4502535701",0,1,1.25)
				modequotes = {{"Let's go beyond what's expected."},{1.5}}
			end

			if k == "e" and Mode ~= 4 and attack == false then
				MAINRUINCOLOR = BrickColor.new("White").Color
				MAINRUINCOLOR2 = Color3.new(0,.5,1)
				Mode = 4
				RecolorTextAndRename("PARADISE",MAINRUINCOLOR,MAINRUINCOLOR2,"Code",nil,"Sound Souler - Paradise")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 70
				stats["WS"] = 70
				newTheme("rbxassetid://4690894100",0,1,musicvol)
				modequotes = {{"I'm not really doing too much.","Don't bother me."},{2,1}}
				moves = {
					v = PARADISE
				}
			end

			if k == "m" and Mode ~= 22 and Mode == 4 and attack == false then
				MAINRUINCOLOR = BrickColor.new(.1,.3,.8).Color
				MAINRUINCOLOR2 = Color3.new(.2,.5,1)
				Mode = 22
				RecolorTextAndRename("EXPEDITION",MAINRUINCOLOR,MAINRUINCOLOR2,"Code",nil,"[BOFXV] No use crying over...")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 125
				stats["WS"] = 125
				newTheme("rbxassetid://4997928185",0,1,musicvol)
				modequotes = {{""},{0}}
			end

			if k == "n" and Mode ~= 29 and Mode == 4 and attack == false then
				MAINRUINCOLOR = BrickColor.new(1,1,1).Color
				MAINRUINCOLOR2 = Color3.new(1,.3,0)
				Mode = 29
				RecolorTextAndRename("INTRIGUEMENT",MAINRUINCOLOR,MAINRUINCOLOR2,"Gotham",nil,"Rebirth - Sound Souler")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 16
				stats["WS"] = 16
				newTheme("rbxassetid://4998146443",0,1,musicvol)
				modequotes = {{"Paradise..","It's quite a fascinating thing, Isn't it?"},{1.5,3}}
			end

			if k == "b" and Mode == 5 and attack == false and Developers[Plr.Name] then
				MAINRUINCOLOR = BrickColor.new("Maroon").Color
				MAINRUINCOLOR2 = BrickColor.new("Dark stone grey").Color
				Mode = "You are pretty useless."
				RecolorTextAndRename("VINDICTIVE",MAINRUINCOLOR,MAINRUINCOLOR2,"Bodoni",nil,"DJ Genki as Gram - Sigmund [t+pazolite Overheat remix]")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 250
				stats["WS"] = 250
				newTheme("rbxassetid://4521541755",0,1,musicvol)
				modequotes = {{"Actions have consequences..."},{1}}
				moves = {z=AeroMad}
			end

			if k == "r" and Mode ~= 5 and attack == false then
				MAINRUINCOLOR = BrickColor.new("Maroon").Color
				MAINRUINCOLOR2 = Color3.new(1,.17,.17)
				Mode = 5
				RecolorTextAndRename("REVOLUTION",MAINRUINCOLOR,MAINRUINCOLOR2,"Code",nil,"Yooh - Dark Matter")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 70
				stats["WS"] = 70
				newTheme("rbxassetid://4684925047",0,1,musicvol)
				modequotes = {{"Chaos is quite nice.","Why not cause a little more of it?"},{1.5,2.25}}
				moves = {z=revolutionAxe}
			end

			if k == "t" and Mode ~= 6 and attack == false then
				MAINRUINCOLOR = Color3.fromRGB(213, 115, 61)
				MAINRUINCOLOR2 = Color3.fromRGB(180, 80, 0)
				Mode = 6
				RecolorTextAndRename("INFERNO",MAINRUINCOLOR,MAINRUINCOLOR2,"Code",nil,"DM Dokuro - Unholy Insurgency")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
