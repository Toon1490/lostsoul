			},{ -- decal start properties
				Color3=Color3.fromRGB((MAINRUINCOLOR2.R*50)^1.85,(MAINRUINCOLOR2.G*50)^1.85,(MAINRUINCOLOR2.B*50)^1.85),
				Transparency = -3,
			},{ -- part end properties
				Size=Vector3.new(4, 0.05, 1.049),
				Transparency = 1,
				CFrame = root.CFrame*angles(math.rad(-10),math.rad(0),math.rad(105))
			},{ -- decal end properties
				Color3=Color3.fromRGB((MAINRUINCOLOR.R*50)^1.85,(MAINRUINCOLOR.G*50)^1.85,(MAINRUINCOLOR.B*50)^1.85),
				Transparency = 1
			},15/60,"Sine","In",0,false,5/60)
			local Alpha,Easing,Direction,Repeat,Reverse,Delay = 25/60,'Elastic','InOut',0,false,0
			local eg = TweenAnimate(RootJoint,{C0=cf(0.3,-1.2,-0.2)*angles(math.rad(-61),math.rad(-9.5),math.rad(-4))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(LH,{C0=cf(-0.8,-0.3,-0.4)*angles(math.rad(-3.3),math.rad(11),math.rad(-28.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(RH,{C0=cf(0.7,-0.3,-1.1)*angles(math.rad(-16.9),math.rad(1.7),math.rad(23.9))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(LW,{C0=cf(-1.5,0.5,-0.6)*angles(math.rad(140.9),math.rad(13.9),math.rad(21.7))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(RW,{C0=cf(1.4,0.3,-0.8)*angles(math.rad(73.5),math.rad(-6.3),math.rad(21.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(Torso.Neck,{C0 =cf(0.1,0,0.1)*angles(math.rad(-4.2),math.rad(21.2),math.rad(8.3))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			swait(Alpha*20)
			particle(targetted.Parent.Head.CFrame,40,{
				Texture="rbxassetid://1391189545",
				LightEmission=.2,
				Color=CS(Color3.new(0.6,0,0),Color3.new(1,0,0)),
				Lifetime=NR(0.25,.75),
				Size=NS({NSK(0,5,2),NSK(0.2,1,.5),NSK(1,0,0)}),
				Transparency=NS({NSK(0,0,0),NSK(0.2,0.25,0.1),NSK(1,1,0)}),
				Speed=NR(10,50),
				Drag=5,
				SpreadAngle=Vector2.new(30,30),
				RotSpeed=NR(-50,50),
				Rotation=NR(1,360),
				EmissionDirection="Front"
			})
			tHum:TakeDamage(999999*stats["ATK"])
			swait(Alpha*50)
			AXE.Anchored = true
			weldo:Destroy()
			parb:Destroy()
			swait(60)
		else
			tHum.Health = tHum.Health + 750
			if tHum.Health >= tHum.MaxHealth then
				tHum.Health = tHum.MaxHealth
			end
		end
		targetted.Anchored = false
	else
		-- ~-~ AFFX3FAIL ~-~
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 20/60,'Bounce','In',0,false,0
		local eg = TweenAnimate(RootJoint,{C0=cf(0,-2.4,-0.5)*angles(math.rad(-100),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1,-0.7,-0.2)*angles(math.rad(10.4),math.rad(5),math.rad(-5))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1,-0.7,-0.2)*angles(math.rad(10),math.rad(-5),math.rad(10))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.3,1.2,0)*angles(math.rad(-174.1),math.rad(-10),math.rad(5.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.4,1.2,0)*angles(math.rad(-174.1),math.rad(10),math.rad(-5.1))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0,0,0)*angles(math.rad(5),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		swait(Alpha*60)
		swait(30)
	end
	root.Anchored = false
	idlesoff = false
	attack = false
end

local scf1,scf2,ssize,stran

function slashything(t,ecf1,ecf2,esize,etran,col,ease,dir)
	ecf1 = root.CFrame * ecf1
	ecf2 = root.CFrame * ecf2
	esize = esize*1.25
	local e2 = weirdeff("woosh",{ -- part start properties
		Size=ssize,
		Color=col,
		Transparency = 1,
		Material="Neon",
		CFrame = scf1
	},{ -- decal start properties
		Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
		Transparency = stran,
	},{ -- part end properties
		Size=esize,
		CFrame = ecf1
	},{ -- decal end properties
		Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
		Transparency = etran
	},t/60,ease,dir,nil,nil,nil,nil,true)
	local e = weirdeff("woosh2",{ -- part start properties
		Size=ssize,
		Color=col,
		Transparency = 1,
		Material="Neon",
		CFrame = scf2
	},{ -- decal start properties
		Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
		Transparency = stran,
	},{ -- part end properties
		Size=esize,
		CFrame = ecf2
	},{ -- decal end properties
		Color3=Color3.fromRGB((col.R*50)^1.85,(col.G*50)^1.85,(col.B*50)^1.85),
		Transparency = etran
	},t/60,ease,dir,nil,nil,nil,nil,true)
	scf1,scf2,ssize,stran = ecf1,ecf2,esize,etran
	return e,e2
end

function RapidSlash()
	attack,idlesoff = true,true
	root.Anchored = true
	local parb = Instance.new("Part")
	parb.Size = Vector3.new(0,0,0)
	parb.CanCollide = false
	parb.Transparency = 1
	local weld = CreateWeldS(rarm,rarm,parb,cf(0,-1,0)*angles(math.rad(-90),math.rad(90),math.rad(0)))
	local AXE = parb:Clone()
	local msh = CreateSpecialMesh(AXE,"rbxassetid://1512524962",0.7, 0.7, 0.7)
	msh.Offset = Vector3.new(1.3, -1.65, 0)
	local decael = Instance.new("Decal",AXE)
	decael.Texture = "rbxassetid://1677620386"
	decael.Color3 = Color3.fromRGB(-2000, 6000, 10000)
	if Mode ~= 5 then
		decael.Color3 = Color3.fromRGB((MAINRUINCOLOR.R*50)^2.5,(MAINRUINCOLOR.G*50)^2.5,(MAINRUINCOLOR.B*50)^2.5)
	end
	decael.Transparency = 1
	TweenAnimate(decael,{Transparency=0},10/60)
	local weldo = CreateWeldS(parb,parb,AXE,angles(math.rad(49.3),math.rad(-23.8),math.rad(101.9)))
	AXE.Parent = workspace
	parb.Parent = char
	local hb2cf = root.CFrame*CFrame.new(0,0,-12)
	local hb3cf = root.CFrame*CFrame.new(0,0,-30)
	local hb4cf = root.CFrame*CFrame.new(0,0,-22.5)
	local hb1 = makeHitbox({CFrame = root.CFrame*CFrame.new(0,0,-6),Size=Vector3.new(15,2.5,7.5)},80,false,false,4,function(hb,eroot,ehum)
		if ehum.Health > 0 then
			eroot.Anchored = true
			eroot.CFrame = hb2cf
		else
			eroot.Anchored = false
		end
	end)
	local hb2 = makeHitbox({CFrame = hb2cf*angles(0,0,math.rad(55)),Size=Vector3.new(15,2.5,7.5)},120,false,false,5,function(hb,eroot,ehum)
		if ehum.Health > 0 then
			eroot.Anchored = true
			eroot.CFrame = hb3cf
		else
			eroot.Anchored = false
		end
	end)
	local hb3 = makeHitbox({CFrame = hb3cf,Size=Vector3.new(20,2,15)},180,false,false,6,function(hb,eroot,ehum)
		if ehum.Health > 0 then
			eroot.Anchored = true
			eroot.CFrame = hb4cf
		else
			eroot.Anchored = false
		end
	end)
