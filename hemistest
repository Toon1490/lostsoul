function rand(min,max)
	return math.random(min,max)
end

function fromlist(lit)
	return lit[rand(1,#lit)]
end

function gen()
	local art1 = fromlist(art)
	local adj1 = ""
	if rand(1,3) == 3 then
		adj1 = fromlist(adj) .. " "
	end
	local noun1 = fromlist(nouns)
	local verb1 = fromlist(verbos)
	local art2 = string.lower(fromlist(art))
	if art1 == "A" and (string.sub(noun1,1,1) == "a" or string.sub(noun1,1,1) == "e" or string.sub(noun1,1,1) == "i" or string.sub(noun1,1,1) == "o" or string.sub(noun1,1,1) == "u") and adj1 == "" then
		art1 = "An"
	elseif art1 == "A" and adj1 ~= "" then
		if (string.sub(adj1,1,1) == "a" or string.sub(adj1,1,1) == "e" or string.sub(adj1,1,1) == "i" or string.sub(adj1,1,1) == "o" or string.sub(adj1,1,1) == "u") then
			art1 = "An"
		end
	end
	local adj2 = ""
	if rand(1,7) == 3 then
		adj2 = fromlist(adj) .. " "
	end
	local noun2 = fromlist(nouns)
	local adv1 = ""
	if rand(1,7) == 3 then
		adv1 = fromlist(adv) .. " "
	end
	if art2 == "a" and (string.sub(noun2,1,1) == "a" or string.sub(noun2,1,1) == "e" or string.sub(noun2,1,1) == "i" or string.sub(noun2,1,1) == "o" or string.sub(noun2,1,1) == "u") and adj2 == "" then
		art2 = "an"
	elseif art2 == "a" and adj2 ~= "" then
		if (string.sub(adj2,1,1) == "a" or string.sub(adj2,1,1) == "e" or string.sub(adj2,1,1) == "i" or string.sub(adj2,1,1) == "o" or string.sub(adj2,1,1) == "u") then
			art2 = "an"
		end
	end
	return art1 .. " " .. adj1 .. noun1 .. " " .. fromlist({"will","shall","must","is going to","has to","shall not","will not","won't","isn't going to","must not","can","cannot","can't"}) .. " " .. adv1 .. verb1 .. " " .. fromlist({art2 .. " " .. adj2 .. noun2,fromlist({"that","you","it","everything","nothing"})})
end

function unknown()
	attack = true
	idlesoff = true
	local formdata = {getModeName(),BrickColor.new(MAINRUINCOLOR),BrickColor.new(MAINRUINCOLOR2),{},stats["WS"],1,1,0,"Hemisphere Hemisphere",kan.SoundId,MAINRUINCOLOR,MAINRUINCOLOR2}
	local eg
	if RootJoint.C0.p.Y > 1 then
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 20/60,'Quad','Out',0,false,0
		eg = TweenAnimate(RootJoint,{C0=cf(0,1.5,0.1)*angles(math.rad(10),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1,-0.5,-0.5)*angles(math.rad(-10),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1,-1,0)*angles(math.rad(-15),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.5,1.1,0)*angles(math.rad(170),math.rad(0),math.rad(5))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.6,1.1,0)*angles(math.rad(170),math.rad(0),math.rad(-5))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0,0.2,0.4)*angles(math.rad(-25),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	else
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 20/60,'Quad','Out',0,false,0
		eg = TweenAnimate(RootJoint,{C0=cf(0,0,0)*angles(math.rad(5),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1,-1,-0.1)*angles(math.rad(-5),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1,-1,-0.1)*angles(math.rad(-5),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-1.5,1.1,-0.1)*angles(math.rad(175),math.rad(0),math.rad(5))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(1.6,1.1,-0.1)*angles(math.rad(175),math.rad(0),math.rad(-5))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0,0,0.3)*angles(math.rad(-10),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	end
	swait(Alpha*60)
	local j = particle(Torso,0,{
		Texture="rbxassetid://243728104",
		LightEmission = 1,
		Color = CS({CSK(0,MAINRUINCOLOR),CSK(0.2,MAINRUINCOLOR2),CSK(1,MAINRUINCOLOR2)}),
		Transparency = NS({NSK(0,0.1,0.1),NSK(0.2,.5,0.3),NSK(1,1,0)}),
		Size = NS({NSK(0,0,0),NSK(.2,5,5),NSK(1,10,7.5)}),
		Lifetime = NR(0.5),
		Rate=75,
		Rotation=NR(1,360),
		RotSpeed=NR(-100,100)
	},Vector3.new(0,5,0))
	particle(Torso,1,{
		Texture="rbxassetid://4409862059",
		LightEmission = 1,
		Color = CS(Color3.new(1,1,1)),
		Transparency = NS({NSK(0,0,0),NSK(0.8,0.25,0),NSK(1,1,0)}),
		Size = NS({NSK(0,0,0),NSK(.3,5,0),NSK(1,15,0)}),
		Lifetime = NR(1.25)
	},Vector3.new(0,5,0))
	particle(Torso,1,{
		Texture="rbxassetid://4409846513",
		LightEmission = 1,
		Color = CS(Color3.new(1,1,1)),
		Transparency = NS({NSK(0,0,0),NSK(0.8,0.25,0),NSK(1,1,0)}),
		Size = NS({NSK(0,0,0),NSK(.3,5,0),NSK(1,15,0)}),
		Lifetime = NR(2.5)
	},Vector3.new(0,5,0))
	local h = true
	local hebb = mouse.KeyUp:connect(function(k) if h == true and k == "p" then Instance.new('CFrameValue',char).Name = 'completed' end end)
	local event = Instance.new("BindableEvent")
	event.Name = "fusionReciever"
	event.Parent = char
	event.Event:connect(function(plre,data)
		if plre:FindFirstChildOfClass("BindableEvent") then
			local e = Instance.new("CFrameValue")
			e.Name = "readyToFuse"
			e.Value = root.CFrame:lerp(plre.Head.CFrame,.5)
			e.Parent = char
			repeat wait() until plre:FindFirstChildOfClass("CFrameValue") and plre:FindFirstChild("readyToFuse")
			if data[8] == "Hemisphere Hemisphere" then
				MAINRUINCOLOR = formdata[11]:lerp(data[12],.35)
				MAINRUINCOLOR2 = data[12]:lerp(formdata[11],.35)
			else
				MAINRUINCOLOR = formdata[2].Color:lerp(data[2].Color,0.35)
				MAINRUINCOLOR2 = data[3].Color:lerp(formdata[3].Color,0.35)
			end
			Instance.new('CFrameValue',char).Name = 'completed'
			RecolorTextAndRename(string.upper(string.sub(formdata[1],2,math.floor(string.len(formdata[1])/1.75)) .. string.sub(data[1],math.floor(string.len(data[1])/1.75))),MAINRUINCOLOR2,MAINRUINCOLOR,modet1.Font)
			Recolor(MAINRUINCOLOR,MAINRUINCOLOR2)
			stats["WS"] = (formdata[5] + data[5])/2
			if typeof(Mode) == typeof(1) then
				Mode = {fusion=true,idle=Mode,aura=Mode,wanim=Mode}
			else
				Mode["fusion"] = true
				Mode["random"] = false
			end
		end
	end)
	local hb1 = makeHitbox({CFrame=root.CFrame,Size=Vector3.new(20,20,20),Anchored=false},0,false,true,2000000,function(hb,eroot,ehum,etors)
		if eroot.Parent:FindFirstChild('fusionReciever') then
			eroot.Parent:FindFirstChild('fusionReciever').Name = "PREPARING"
			eroot.Parent.PREPARING:Fire(char,formdata)
			hb.Parent = nil
		end
	end)
	local e = CreateWeldS(root,hb1,root)
	swait(Alpha*60)
	attack = false
	idlesoff = false
	h = false
	event:Destroy()
	hb1.Color = Color3.new(0,0,1)
	j:Destroy()
end

