		LockedToPart=true,
		Drag=10
	})
	TweenAnimate(decael,{Transparency=0},0.5,"Quad","Out")
	CreateSound5("3355840382",AXE,1,0.9)
	-- ~-~ REV1 ~-~
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = stats["ASPD"]*20/60,'Back','Out',0,false,0
	TweenAnimate(root,{CFrame = root.CFrame*CFrame.new(0,0,1)},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	local eg = TweenAnimate(RootJoint,{C0=cf(0,-0.3,-0.1)*angles(math.rad(-12.7),math.rad(49.7),math.rad(4))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(LH,{C0=cf(-1.1,-1,-0.2)*angles(math.rad(-35),math.rad(25),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
	TweenAnimate(RH,{C0=cf(1.1,-0.5,-0.1)*angles(math.rad(19.3),math.rad(-38.4),math.rad(17.3))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
