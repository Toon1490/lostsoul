				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 20
				stats["WS"] = 20
				newTheme("rbxassetid://4594744373",0,1,musicvol) -- 4550748038 4588373978 1343241846
				modequotes = {{"Where.. am i..?", "I.. can't see..", "..hello?"}, {2,2,1}}
			end

			if k == "m" and Mode ~= 28 and Mode == 19 and attack == false then
				MAINRUINCOLOR = Color3.new(.2,.2,.2)
				MAINRUINCOLOR2 = Color3.new(1,0,0)
				Mode = 28
				RecolorTextAndRename("ENTITY",MAINRUINCOLOR,MAINRUINCOLOR2,"Arcade",nil,'"[Mutual Faith 2] Non-Entity [BGA]"')
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 350
				stats["WS"] = 350
				newTheme("rbxassetid://4810313576",0,1,musicvol) -- 4550748038 4588373978 1343241846
				modequotes = {{"AHAHA!!","YOU REALLY EXPECTED SOMETHING NORMAL?", "PATHETIC."},{1,2,1}}
			end
		end

		if k == "b" and Mode ~= -21 and Mode == 19 and attack == false and (Developers[Plr.Name] or Plr.Name == "Sidetalian") then
			MAINRUINCOLOR = Color3.fromHSV(265/360, 45/100, 35/100) 
			MAINRUINCOLOR2 = Color3.fromHSV(220/360, 35/100, 45/100) 
			Mode = -21
			RecolorTextAndRename("PHANTASMA",MAINRUINCOLOR,MAINRUINCOLOR2,"Fantasy",nil,"ZUN remixed by Noah - Necrofantasia") -- based on nightfall
			Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
			walkspeed = 64
			stats["WS"] = 64
			newTheme("rbxassetid://4801578241",0,0.95,1.25)
			modequotes = {{"All beings have some sort of flaw.","But I'm an exception. My weakness.. is nothing."},{2,2.5}}
		end

		if page == 2 then
			if k == "q" and Mode ~= 101 and attack == false then
				MAINRUINCOLOR = Color3.fromRGB(0, 29, 125)
				MAINRUINCOLOR2 = Color3.fromRGB(165, 115, 44)
				Mode = 101
				RecolorTextAndRename("DIGNIFIED",MAINRUINCOLOR,MAINRUINCOLOR2,"Code",nil,"pilotredsun - Dignified 111")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 40
				stats["WS"] = 40
				newTheme("rbxassetid://3613534839",0,1,musicvol)
			end

			if k == "e" and Mode ~= 102 and attack == false then
				MAINRUINCOLOR = BrickColor.new(1,0,.2).Color
				MAINRUINCOLOR2 = BrickColor.new(.3,0,0).Color
				Mode = 102
				RecolorTextAndRename("EXCESSION",MAINRUINCOLOR,MAINRUINCOLOR2,"Code",nil,"Sakuzyo - Synthesized Angel")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 50
				stats["WS"] = 50
				newTheme("rbxassetid://2482377049",0,1,musicvol)
			end

			if k == "r" and Mode ~= 103 and attack == false then
				MAINRUINCOLOR = BrickColor.new(0,0,0).Color
				MAINRUINCOLOR2 = BrickColor.new(1,1,1).Color
				Mode = 103
				RecolorTextAndRename("ENDATION",MAINRUINCOLOR,MAINRUINCOLOR2,"SourceSansItalic",nil,"Camellia - NIGHTMARE CITY")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 1050
				stats["WS"] = 1050
				newTheme("rbxassetid://4453518004",0,1,musicvol)
				modequotes = {{"Don't be such a fool.","Running isn't an option."},{2,2.5}}
			end

			if k == "t" and Mode ~= 500 and attack == false then
				MAINRUINCOLOR = Color3.fromRGB(87, 134, 191)
				MAINRUINCOLOR2 = Color3.fromRGB(116, 113, 209)
				Mode = 500
				RecolorTextAndRename("AERIAL",MAINRUINCOLOR,MAINRUINCOLOR2,"Gotham",nil,"RMK - YYX")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 100
				stats["WS"] = 100
				newTheme("rbxassetid://3584953190",0,1,musicvol)
			end

			if k == "y" and Mode ~= 501 and attack == false then
				MAINRUINCOLOR = Color3.fromRGB(130, 175, 255)
				MAINRUINCOLOR2 = Color3.fromRGB(185,185,195)
				Mode = 501
				RecolorTextAndRename("CONQUEST",MAINRUINCOLOR,MAINRUINCOLOR2,"Gotham",nil,"??? - Call of Abyss")
				Recolor(MAINRUINCOLOR,MAINRUINCOLOR2,MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,false,false)
				walkspeed = 250
				stats["WS"] = 250
				newTheme("rbxassetid://4390839374",0,1.025,musicvol)
			end
		end

		if k == "four" then
			funkyCompile()
		end

		if k == "semicolon" and Mode ~= 58 and not attack then
			Taunt()
		end

		if k == "semicolon" and Mode == 58 and not attack then 
			RecogTaunt()
		end

		if (k == "z" or k == "x" or k == "c" or k == "v") and not attack then
			if moves[k] then
				root.CFrame = root.CFrame -- yes
				moves[k]()
			end
		end

		if k == "p" and not attack then
			unknown()
		end

		if k == "zero" and disablecolshift == false then
			disablecolshift = true
		elseif k == "zero" and disablecolshift == true then
			disablecolshift = false		
		end

		if k == "nine" and reduceEffect == false then
			reduceEffect = true
		elseif k == "nine" and reduceEffect == true then
			reduceEffect = false		
		end

		if k == "six" and debugmode == false then
			debugmode = true
		elseif k == "six" and debugmode == true then
			debugmode = false		
		end

		if k == "eight" then
			if wingstyle == "Default" then
				loadwings(wings.Old)
			elseif wingstyle == "Old" then
				loadwings(wings.Thin)
			elseif wingstyle == "Thin" then
				loadwings(wings["Tiny Old"])
			elseif wingstyle == "Tiny Old" then
				loadwings(wings["Star"])
				prankgui.Enabled = true
				bguilt.Enabled = false
				progui.Enabled = false
			elseif wingstyle == "Star" then
				loadwings(wings["Stratospheric"])
			elseif wingstyle == "Stratospheric" then
				loadwings(wings["None"])
				prankgui.Enabled = false
			elseif wingstyle == "None" then
				loadwings(wings.Default)
				bguilt.Enabled = true
				progui.Enabled = true
			end
		end

		if k == "l" then
			if toggletheme == true then
				kan.Volume = 0
				toggletheme = false
			else
				kan.Volume = currentVol
				toggletheme = true
			end
		end

		if k == "seven" and not keepQuote then
			keepQuote = true
		elseif keepQuote then
			if k == "seven" then
				keepQuote = false
			else
				modequotes = keepQuoted
			end
		end
	end
end
mouse.KeyDown:connect(keydown)

mouse.KeyUp:connect(function(k)
	keysDown[k] = false
end)

Mode = 0
keydown(aaaa["starter"]) -- start on another side?
keydown("q") -- become exhaltion
keydown(aaaa["starter"]) -- start as another mode?

----- no camera script -----

function weirdsine(h,bean)
	if not bean then
		return CFrame.Angles(math.rad(math.sin(sine/(h or 12))*10),math.rad(math.sin(sine/(h or 12))*10),math.rad(math.sin(sine/(h or 12))*10))
	else
		return CFrame.Angles(math.rad(math.sin(sine/(h or 12))*-10),math.rad(math.sin(sine/(h or 12))*-10),math.rad(math.sin(sine/(h or 12))*-10))
	end
end

rotval = 0
loop = 0
loopval = 5

-- Final Loop
ArtificialHB.Event:connect(function() pcall(function() -- imagine using while true do swait()
		sine = sine+change
		loop = loop + 1
		if loop>=loopval then loop = 0 end
		if Mode ~= 7 then
			blush.Parent = nil
			blush.Transparency = 1
		elseif Mode == 7 then
			blush.Parent = hed
			blush.Transparency = 0
		end	

		larm.Parent = char
		rarm.Parent = char	

		-- moai's stupid colorshift
		if beans and bean and be and beans2 and bean2 and be2 and (sine/20 == math.ceil(sine/20) or ((Mode == 70051 or Mode == 9 or Mode == 50) and sine/5 == math.ceil(sine/5))) then
			local Mode = Mode
			if typeof(Mode) == typeof({"bruh"}) and Mode["newdata"] == true then
				Mode = Mode["CSType"]
			end
			for i,v in pairs(recoltweens) do
				if v.PlaybackState == Enum.PlaybackState.Completed then
					table.remove(recoltweens,i)
				end
			end
			local function re(o,d,h)
				return (o + math.sin(sine/d)*(h or 0.1)) - math.floor(o + math.sin(sine/d)*(h or 0.1))
			end
			if disablecolshift == true then
				if Mode == 8 then -- Spectrum Rainbow Colorshift
					TweenRecolor(Color3.fromHSV((sine/360) - math.floor(sine/360),.55 + math.sin(sine/32)*0.3,1), Color3.fromHSV((sine/360) - math.floor(sine/360),.5 + math.sin(sine/46)*0.4,1))
					RecolorTextAndRename("SPECTRUM",Color3.fromHSV((sine/360) - math.floor(sine/360),.55 + math.sin(sine/32)*0.3,1), Color3.fromHSV((sine/360) - math.floor(sine/360),.85 + math.sin(sine/32)*0.1,1),"Code")

				elseif Mode == 27 then -- Obliteration Rainbow Colorshift
					TweenRecolor(Color3.fromRGB(.2,.2,.25), Color3.fromHSV((sine/360) - math.floor(sine/360),.5 + math.sin(sine/46)*0.4,1))
					RecolorTextAndRename("OBLITERATION",Color3.new(.2,.2,.25), Color3.fromHSV((sine/360) - math.floor(sine/360),.85 + math.sin(sine/32)*0.1,1),"Code")

				elseif Mode == 9 then
					local eg = GetLocalProperty(kan,"PlaybackLoudness")
					MAINRUINCOLOR = Color3.fromHSV((sine/360) - math.floor(sine/360),0.7,eg/750)
					MAINRUINCOLOR2 = Color3.fromHSV(0,0,eg/750)
					RecolorQ(MAINRUINCOLOR2,MAINRUINCOLOR)
					RecolorTextAndRename("RYTHMIC",MAINRUINCOLOR, MAINRUINCOLOR2,"Gotham")

				elseif Mode == 103 then -- Endation Rainbow Colorshift
					TweenRecolor(Color3.fromRGB(.2,.2,.25), Color3.fromHSV((sine/360) - math.floor(sine/360),.5 + math.sin(sine/46)*0.4,1))
					RecolorTextAndRename("ENDATION",Color3.new(.2,.2,.25), Color3.fromHSV((sine/360) - math.floor(sine/360),.85 + math.sin(sine/32)*0.1,1),"SourceSansItalic")

				elseif Mode == 12 then -- amfasdnjkfhb's wacky colorshift
					TweenRecolor(Color3.fromHSV(re(beans,32,0.1),bean,be), Color3.fromHSV(re(beans2,64,0.2),bean2,be2))
					RecolorTextAndRename(modet1.Text,Color3.fromHSV(re(beans,32,0.1),bean,be), Color3.fromHSV(re(beans2,64,0.2),bean2,be2),modet1.Font,true)

				elseif Mode == 12.5 then -- virtue's wackier colorshift
					TweenRecolor(Color3.fromHSV(re(beans,32,0.25),bean,be), Color3.fromHSV(re(beans2,64,0.4),bean2,be2))
					RecolorTextAndRename(modet1.Text,Color3.fromHSV(re(beans,32,0.25),bean,be), Color3.fromHSV(re(beans2,64,0.4),bean2,be2),modet1.Font,true)

				elseif Mode == 15 or Mode == -20 or Mode == 70051 then -- colorshift for requisite and eternal
					TweenRecolor(Color3.fromHSV(beans,re(bean - 0.2,36,0.1),re(be - 0.2,20,0.1)), Color3.fromHSV(beans2,re(bean2 + 0.2,18,0.15),re(be2,10,0.15)))
					RecolorTextAndRename(modet1.Text,Color3.fromHSV(beans,re(bean - 0.2,36,0.1),re(be - 0.2,20,0.1)), Color3.fromHSV(beans2,re(bean2 + 0.2,18,0.15),re(be2,10,0.15)),modet1.Font,true)

				elseif Mode == 6 then -- inferno's wacky colorshift
					TweenRecolor(Color3.fromHSV(re(beans,12,0.05),bean,re(be,24,0.1)), Color3.fromHSV(re(beans2,8,0.05),bean2,re(be2,18,0.1)))
					RecolorTextAndRename(modet1.Text,Color3.fromHSV(re(beans,12,0.05),bean,re(be,24,0.1)), Color3.fromHSV(re(beans2,8,0.05),bean2,re(be2,18,0.1)),modet1.Font,true)

				elseif Mode == 50 then -- maelstrom chaos
					MAINRUINCOLOR,MAINRUINCOLOR2 = Color3.fromHSV(beans,math.random(0,100)/100,math.random(10,70)/100), Color3.fromHSV(beans2,math.random(40,100)/100,math.random(20,80)/100)
					TweenRecolor(Color3.fromHSV(beans,math.random(0,100)/100,math.random(10,70)/100), Color3.fromHSV(beans2,math.random(40,100)/100,math.random(20,80)/100))
					mo = ({"Unknown","Undefined","Null","Who?","???","Cryptid","Unrecognizable","Disturbance","Freak","Monster","Abomination","Malformed","[REDACTED]","Abnormality"})
					mo = ({"MAELSTROM","MAELSTROM","MAELSTROM",string[({"upper","lower"})[math.random(1,2)]](mo[math.random(1,#mo)])})[math.random(1,4)]
					RecolorTextAndRename(mo,Color3.fromHSV(beans,math.random(0,100)/100,math.random(10,70)/100), Color3.fromHSV(beans2,math.random(40,100)/100,math.random(20,80)/100),modet1.Font)

				elseif Mode == 101 then
					TweenRecolor(Color3.fromHSV(.6-.05*math.sin(sine/30),.5+.5*math.cos(sine/25),.5),Color3.fromHSV(.05+.05*math.cos(sine/25),.4+.2*math.sin(sine/30),.6))
					RecolorTextAndRename(modet1.Text,Color3.fromHSV(.6-.05*math.sin(sine/30),.5+.5*math.cos(sine/25),.5),Color3.fromHSV(.05+.05*math.cos(sine/25),.4+.2*math.sin(sine/30),.6),modet1.Font,true)

				elseif Mode == "GHOST-NOVA" then -- vehemence
					TweenRecolor(Color3.fromHSV(re(beans,42,0.5),bean,re(be,32,0.2)), Color3.fromHSV(re(beans2,84,0.7),bean2,re(be2,34,0.2)))
					RecolorTextAndRename(modet1.Text,Color3.fromHSV(re(beans,32,0.25),bean,be), Color3.fromHSV(re(beans2,64,0.4),bean2,be2),modet1.Font,true)

				elseif Mode ~= 3 and Mode ~= 1111 and Mode ~= 29 and Mode ~= 70051 and Mode ~= 78 and Mode ~= 9 and Mode ~= 59 and Mode ~= 58 and Mode ~= 55 and Mode ~= 4 and Mode ~= 13 and Mode ~= 100 and Mode ~= 17 and Mode ~= 18 and Mode ~= 19 and Mode ~= 20  and Mode ~= 21 and Mode ~= 22 and Mode ~= 23 and Mode ~= 25 and Mode ~= 101 and Mode ~= 56 and Mode ~= 57 and Mode ~= 102 then -- colorshift for every other form except those
					TweenRecolor(Color3.fromHSV(re(beans,18),bean,be), Color3.fromHSV(re(beans2,32),bean2,be2))
					RecolorTextAndRename(modet1.Text,Color3.fromHSV(re(beans,18),bean,be), Color3.fromHSV(re(beans2,32),bean2,be2),modet1.Font,true)

				end
			end
		end
		--

		--0.6 1 0.5
		--0.5 0.2 0.5

		--0.1 0.8 0.6
		--0.0 0.4 0.6


		if not attack then
			Hum.WalkSpeed = walkspeed
			Hum.JumpPower = jumppower
		end
		if (sine/5 == math.ceil(sine/5)) then
			local Mode = Mode
			if typeof(Mode) == typeof({"bruh"}) then
				if Mode["newdata"] == true then
					Mode = Mode["Wings"]
				else
					Mode = Mode["wanim"]
				end
			end
			--animThing.wings(Mode,sine,handleweld,lwing1weld,lwing2weld,lwing3weld,rwing1weld,rwing2weld,rwing3weld)
		end

		local torvel=(RootPart.Velocity*Vector3.new(1,0,1)).magnitude
		local velderp=RootPart.Velocity.y
		local hitfloor,posfloor=rayCast(RootPart.Position,(CFrame.new(RootPart.Position,RootPart.Position - Vector3.new(0,1,0))).lookVector,4,char)


		--Footplanting code, no need to modify anything, variable is Vec.X,Y or Z
		local Walking = Hum.MoveDirection.magnitude>0
		local FwdDir = (Walking and Hum.MoveDirection*root.CFrame.lookVector or Vector3.new())
		local RigDir = (Walking and Hum.MoveDirection*root.CFrame.rightVector or Vector3.new())
		local Vec = {
			X=RigDir.X+RigDir.Z,
			Z=FwdDir.X+FwdDir.Z
		};
		local Divide = 1
		if(Vec.Z<0)then
			Divide=math.clamp(-(1.25*Vec.Z),1,2)
		end
		Vec.Z = Vec.Z/Divide
		Vec.X = Vec.X/Divide
		if not idlesoff then
			if moves["gliding"] then
				local bruh = moves["gliding"]
				local eg,_ = pcall(function()
					local ccf = GetLocalProperty("camera","CFrame")
					if keysDown["w"] then bruh.Velocity = bruh.Velocity:lerp(ccf.lookVector*stats["WS"]*2,.05) end
					if keysDown["s"] then bruh.Velocity = bruh.Velocity:lerp(-ccf.lookVector*stats["WS"]*2,.05) end
					if keysDown["a"] then bruh.Velocity = bruh.Velocity:lerp(-ccf.rightVector*stats["WS"]*2,.05) end
					if keysDown["d"] then bruh.Velocity = bruh.Velocity:lerp(ccf.rightVector*stats["WS"]*2,.05) end
					if not keysDown["w"] and not keysDown["s"] and not keysDown["a"] and not keysDown["d"] then
						bruh.Velocity = bruh.Velocity:lerp(Vector3.new(0,0,0),.05)
					end
				end)
				if not eg then moves["gliding"] = nil bruh:Destroy() end
			end
			if RootPart.Velocity.y > 1 and hitfloor==nil and not moves["gliding"] then
				Anim="Jump"
				if getModeName() == "Indolent" then -- indolent
					local Alpha = .1
					TweenAnimate(RootJoint,{C0=cf(0,0 + .1 * math.cos(sine/34),0)*angles(math.rad(0),math.rad(-sine*root.Velocity.magnitude/5),math.rad(0))*RootCF},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LH,{C0=cf(-1,-1 - .1 * math.cos(sine/34),0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RH,{C0=cf(1,-1 - .1 * math.cos(sine/34),0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(LW,{C0=cf(-1.2,1 - .1 * math.cos(sine/30),0)*angles(math.rad(0),math.rad(0),math.rad(-80))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(RW,{C0=cf(1.2,1 - .1 * math.cos(sine/30),0)*angles(math.rad(0),math.rad(0),math.rad(80))},Alpha,'Linear','Out',0,false,0)
					TweenAnimate(Torso.Neck,{C0 =cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(0))*necko},Alpha,'Linear','Out',0,false,0)
				elseif typeof(Mode) == typeof({"bruh"}) and Mode["newdata"] == true and not ((tonumber(moves["jumped"]) or 0) < ((stats["JUMPS"] - 1) or 0)) then
					local Alpha,Easing,Direction,Repeat,Reverse,Delay = 2/60,'Linear','Out',0,false,0
					local eg = TweenAnimate(RootJoint,{C0=cf(0,0,0)*angles(math.rad(-sine*20),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(LH,{C0=cf(-0.9,-0.7,-1)*angles(math.rad(-55.4),math.rad(-5),math.rad(-5))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(RH,{C0=cf(0.9,-0.8,-1)*angles(math.rad(-55.4),math.rad(5),math.rad(5))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(LW,{C0=cf(-1.3,0,-0.5)*angles(math.rad(106.8),math.rad(16.3),math.rad(53.3))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(RW,{C0=cf(1.2,0.1,-0.6)*angles(math.rad(110),math.rad(-8.6),math.rad(-59.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
					TweenAnimate(Torso.Neck,{C0 =cf(0,0.2,0.4)*angles(math.rad(-30),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
				else
					local Alpha = .4
					TweenAnimate(RootJoint,{C0=cf(0,0,0)*angles(math.rad(15),math.rad(0),math.rad(0))*RootCF},Alpha,'Back','Out',0,false,0)
					TweenAnimate(LH,{C0=cf(-1,-1,0)*angles(math.rad(-16.3),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,'Back','Out',0,false,0)
					TweenAnimate(RH,{C0=cf(1,-0.3,-0.8)*angles(math.rad(-29.1),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,'Back','Out',0,false,0)
					TweenAnimate(LW,{C0=cf(-1.5,0.6,0.1)*angles(math.rad(-14.8),math.rad(17.3),math.rad(-10.2))},Alpha,'Back','Out',0,false,0)
					TweenAnimate(RW,{C0=cf(1.4,0.7,0.2)*angles(math.rad(-21.1),math.rad(-27.1),math.rad(9.1))},Alpha,'Back','Out',0,false,0)
					TweenAnimate(Torso.Neck,{C0 =cf(0,0,-0.1)*angles(math.rad(7.7),math.rad(0),math.rad(0))*necko},Alpha,'Back','Out',0,false,0)
				end			
			elseif RootPart.Velocity.y < -1 and hitfloor==nil and not moves["gliding"] then
				Anim="Fall"
				if getModeName() == "Indolent" then -- indolent
					local Alpha = .1
					TweenAnimate(RootJoint,{C0=cf(0,0 + .1 * math.cos(sine/34),0)*angles(math.rad(0),math.rad(sine*root.Velocity.magnitude/5),math.rad(0))*RootCF},Alpha,'Linear','Out',0,false,0)
