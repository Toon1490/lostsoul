	if Aura ~= lastA then
		local sine = sine
		local loop,loopval = 0,5
		local Mode = Aura
		lastA = Mode
		local function WACKYEFFECT(Table)
			return spat.WACKYEFFECT(swait,Table,char,Effects)
		end
		local function weirdeff(...)
			return spat.weirdeff(swait,...)
		end
		local colors = {MAINRUINCOLOR,MAINRUINCOLOR2}
		local VT,MRANDOM,COS,ANGLES,CF,RAD = Vector3.new,math.random,math.cos,CFrame.Angles,CFrame.new,math.rad
		-- AURA EFFECTS	
		coroutine.resume(coroutine.create(function()
			while lastA == Mode do
				local hitfloor,posfloor=rayCast(root.Position,(CFrame.new(root.Position,root.Position - Vector3.new(0,1,0))).lookVector,4,char)
				if hitfloor and not reduceEffect then
					if Mode == 1 then
						WACKYEFFECT({
							Time = 15,
							EffectType = "Wedge",
							Size = VT(0.01,15,15),
							Size2 = VT(0.01, 18, 18),
							Transparency = 0.7,
