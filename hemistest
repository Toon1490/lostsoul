HumB.Parent = serverStorage

idlesoff = false

-- moai's stupid hitbox code

function makeHitbox(hitbProps,dam,multihit,startactive,critchance,hitfunc)
	local exclude = {}
	local e = Instance.new("Part")
	local function check(v)
		if v.Name == "Effect" or v.Name == "hitboxe" or v.Parent == char or v.Parent == nil then return end
		local enem = v.Parent
		if enem:IsA("Model") then
			local ehum = enem:FindFirstChildOfClass("Humanoid")
			local etors = enem:FindFirstChild("Torso")
			if ehum and etors then
				for f,h in pairs(exclude) do
					if h == ehum then
						return
					end
				end
				dam = (dam * stats["ATK"]) + stats["ADMG"]
				if math.random(1,critchance or 10) == 1 then
					ehum:TakeDamage(dam*3) -- critical hit
					CreateSound5("2374032821",etors,.8,1.5,false)
				else
					ehum:TakeDamage(dam)
					CreateSound5("2590761458",etors,.4,1.8,false)
				end
				if not multihit then
					table.insert(exclude,ehum)
				end
				if hitfunc then
					local eroot = enem:FindFirstChild("HumanoidRootPart")
					hitfunc(e,eroot,ehum,etors)
				end
			end
		end
	end
	e.Anchored = true
	e.Transparency = 1
	if debugmode then e.Transparency = .8 end
	e.CanCollide = false
	e.Material = "Neon"
	e.Massless = true
	e.Name = "hitboxe"
	local egg = startactive
	if startactive then
		e.Color = Color3.new(1,0,0)
	else
		e.Color = Color3.new(0,1,0)
	end
	for i,v in pairs(hitbProps) do
		e[i] = v
	end
	e.Parent = char
	e.Changed:connect(function(prop)
		if prop == "Color" then
			if e.Color == Color3.new(1,0,0) then
				egg = true
				for i,v in pairs(e:GetTouchingParts()) do
					check(v)
				end
			elseif e.Color == Color3.new(0,0,1) then
				if egg then
					for i,v in pairs(e:GetTouchingParts()) do
						check(v)
					end
				end
				e:Destroy()
			end
		end
	end)
	local conect = e.Touched:connect(function(h)
		if e.Color == Color3.new(1,0,0) then
			check(h)
		end
	end)
	return e
end

function Test() -- testing hitboxes
	local hb1 = makeHitbox({CFrame = mouse.Hit,Size=Vector3.new(30,30,30),Shape="Cylinder"},30,true)
	swait(30)
	hb1.Color = Color3.new(1,0,0)
	swait(15)
	hb1.Color = Color3.new(0,0,1)
end

function getModeName()
	return string.upper(string.sub(modet1.Text,5,5)) .. string.lower(string.sub(modet1.Text,6,string.len(modet1.Text)-4))
end

function getName()
	local name = char.Name
	if aaaa["chare"] == "Emi" then
		name = "Emi"
	elseif aaaa["chare"] == "RCE Cytus" or aaaa["chare"] == "Cytus" then
		name = "Cytus"
	elseif aaaa["chare"] == "Vida" then
		name = "Vida"
	elseif char.Name == "Zombieza4569" or aaaa["chare"] == "Aero" then
		name = "Aero"
	elseif char.Name == "divineMoai" or char.Name == "doz_koob" or aaaa["chare"] == "Moai" then
		name = "Moai"
	elseif char.Name == "DerpzDeNugget_YT" then
		name = "Derpz" -- because yes
	elseif char.Name == "loplkc" then
		name = "Terminus"
	elseif char.Name == "Helkern" then
		name = "Axiom"
	elseif char.Name == "Birdmaid" then
		name = "Meidri"
	elseif char.Name == "GodzPlaysRB" then
		name = "Cassidy"
	end
	local mo = getModeName() .. " " .. name
	if getModeName() == "Maelstrom" then
		mo = ({"Unknown","Undefined","Null","Who?","???","Cryptid","Unrecognizable","Disturbance","Freak","Monster","Abomination","Malformed","[REDACTED]","Abnormality"})
		mo = string[({"upper","lower"})[math.random(1,2)]](mo[math.random(1,#mo)])
	end
	return mo
end

local modequotes = {{"Hekc","Bean"},{1,2}}

function Taunt()
	attack = true
	idlesoff = true
	walkspeed = 0
	char.Archivable = true -- so cloning works
	dialog(getName(),modequotes[1],"player",modequotes[2])
	swait()
	for i = 0,100 do
		swait()
		local Alpha = .1
		TweenAnimate(RootJoint,{C0=cf(0.1,3.2  - 1 * math.cos(sine/62),-0.7)*angles(math.rad(-4.3 + 5 * math.cos(sine/32)),math.rad(32.9),math.rad(-2.4))*RootCF},Alpha,'Linear','Out',1,false,0)
		TweenAnimate(LH,{C0=cf(-1,-0.5 - .1 * math.cos(sine/32),-0.5)*angles(math.rad(-12.2),math.rad(1.6),math.rad(-6.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,'Linear','Out',1,false,0)
		TweenAnimate(RH,{C0=cf(1,-0.9 - .1 * math.cos(sine/32),0.1)*angles(math.rad(-5.8),math.rad(-35.4),math.rad(8.2))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,'Linear','Out',1,false,0)
		TweenAnimate(LW,{C0=cf(-1.1,0.1 + .1 * math.cos(sine/32),0.5)*angles(math.rad(-46),math.rad(19.7),math.rad(75.3))},Alpha,'Linear','Out',1,false,0)
		TweenAnimate(RW,{C0=cf(1.4,0.2 + .1 * math.cos(sine/32),0.1)*angles(math.rad(144),math.rad(-5.8),math.rad(29.8))},Alpha,'Linear','Out',1,false,0)
		TweenAnimate(Torso.Neck,{C0 =cf(0,0.1,0.4)*angles(math.rad(-23.5),math.rad(-32.3),math.rad(-1.4))*necko},Alpha,'Linear','Out',2,false,0)
	end
	idlesoff = false
	attack = false
	walkspeed = stats["WS"]
end

