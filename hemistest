			TweenAnimate(LW,{C0=cf(-1.2,0.6,-0.9)*angles(math.rad(74.9),math.rad(-8.1),math.rad(34))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(RW,{C0=cf(1.7,0.9,-0.7)*angles(math.rad(90.8),math.rad(23),math.rad(-22.6))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(Torso.Neck,{C0 =cf(0.2,0,-0.1)*angles(math.rad(2.2),math.rad(-23.4),math.rad(8.4))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			swait(Alpha*60)
			e:Destroy()
			e2:Destroy()
			CreateSound5(4575259320,root,.75,1.1,false)
			CreateSound5(2374032821,root,1,1,false)
			weirdeff("SpikeBall",{ -- part start properties
				Size=Vector3.new(4,4,4),
