		},{ -- mesh start properties
			VertexColor = Vector3.new(MAINRUINCOLOR.R*5,MAINRUINCOLOR.G*5,MAINRUINCOLOR.B*5),
			Scale = Vector3.new(0,0,0)
		},{ -- part end properties
			Transparency = 1
		},{ -- mesh end properties
			Scale = Vector3.new(.75,0,.75)*(size or 1)*(math.random(50,100)/100)
		},math.random(60,140)/100,"Quad","Out")
	end
	particle(root,5,{ZOffset=-3,
		Texture="rbxassetid://1053546634",
		LightEmission=1,
		Color=CS(MAINRUINCOLOR,cl1),
		Lifetime=NR(2),
		Size=NS({NSK(0,0,0),NSK(0.3,30,0),NSK(0.5,40,0),NSK(0.7,30,0),NSK(1,0,0)}),
		RotSpeed=NR(-90)
	})
	particle(root,5,{ZOffset=-2.75,
		Texture="rbxassetid://1053546634",
		LightEmission=1,
		Color=CS(MAINRUINCOLOR2,cl2),
		Lifetime=NR(2),
		Size=NS({NSK(0,0,0),NSK(0.3,15,0),NSK(0.5,20,0),NSK(0.7,15,0),NSK(1,0,0)}),
		RotSpeed=NR(90)
	})
	particle(root,100,{
		Texture="rbxassetid://243728104",
		LightEmission=1,
		Color=CS(MAINRUINCOLOR,cl1),
		Lifetime=NR(3),
		Size=NS({NSK(0,0,0),NSK(0.3,10*(size or 1),5),NSK(0.5,12.5*(size or 1),6.25*(size or 1)),NSK(0.7,15*(size or 1),7.55*(size or 1)),NSK(1,17.5*(size or 1),8.75*(size or 1))}),
		Transparency=NS({NSK(0,0,0),NSK(0.2,0.5,0.2),NSK(1,1,0)}),
		Speed=NR(25*(size or 1),100*(size or 1)),
		Drag=3,
		SpreadAngle=Vector2.new(360,360),
		RotSpeed=NR(-100,100),
		Rotation=NR(1,360)
	})
	local eg = TweenAnimate(e,{Color=cl1},4,"Quad","InOut")
	if not moves["gliding"] then
		TweenAnimate(e2,{Color=cl2},4,"Quad","InOut")
	end
	swait(60)
	particle(larm,3,{ZOffset=2,
		Texture="rbxassetid://1053546634",
		LightEmission=1,
		Color=CS(Color3.new(1,1,1)),
		Lifetime=NR(0.75),
		Size=NS({NSK(0,7.5,0),NSK(0.3,3,0),NSK(0.5,2,0),NSK(0.7,1,0),NSK(1,0,0)})
	},Vector3.new(0,-1,0))
	local chargey = particle(larm,0,{ZOffset=2,
		Texture="http://www.roblox.com/asset/?id=1591623238",
		LightEmission=1,
		Color=CS(cl1,Color3.new(1,1,1)),
		Lifetime=NR(0.4),
		Size=NS({NSK(0,40*(size or 1),0),NSK(1,0,0)}),
		Rate=20,
		RotSpeed=NR(300),
		Rotation=NR(1,360),
		Transparency=NS({NSK(0,1,0),NSK(1,0.5,0)}),
	},Vector3.new(0,-1,0))
	local chargey2 = particle(larm,0,{ZOffset=1,
		Texture="http://www.roblox.com/asset/?id=825937029",
		LightEmission=.9,
		Color=CS(cl1,cl2),
		Lifetime=NR(0.1,.6),
		Size=NS({NSK(0,50*(size or 1),0),NSK(0.2,20*(size or 1),0),NSK(1,0,0)}),
