	else
		local eg = TweenAnimate(RootJoint,{C0=cf(-0.1,0,0)*angles(math.rad(5.5),math.rad(-24.9),math.rad(2.3))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-0.9,-0.9,-0.1)*angles(math.rad(-4),math.rad(24.5),math.rad(-6.2))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(0.8,-0.9,0.2)*angles(math.rad(-5),math.rad(-8.5),math.rad(13.2))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
