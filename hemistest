local spat = {}
local cf,angles = CFrame.new, CFrame.Angles

local resources = hemis:WaitForChild("Resources")

task.spawn(function()
local function TweenAnimateC(object,Aniim,time,easingstyle,easingdirection,repet,reverse,deelay)
	local infos=TweenInfo.new(time or (20/60),Enum.EasingStyle[easingstyle or "Linear"],Enum.EasingDirection[easingdirection or "InOut"],repet or 0,reverse or false,deelay or 0)
	local tween=game:GetService("TweenService"):Create(object,infos,Aniim)
	tween:Play()
	return tween
end

local function TweenAnimate(object,Aniim,c0)
	local infos=TweenInfo.new(5/60,Enum.EasingStyle["Linear"],Enum.EasingDirection["InOut"],0,false,0)
	if c0 == true then
		Aniim = {C0=Aniim}
	else
		Aniim = {C1=Aniim}
	end
	local tween=game:GetService("TweenService"):Create(object,infos,Aniim)
	tween:Play()
	return tween
end

local function CreateMesh5(MESH, PARENT, MESHTYPE, MESHID, TEXTUREID, SCALE, OFFSET)
	local NEWMESH = Instance.new(MESH)
	if MESH == "SpecialMesh" then
		NEWMESH.MeshType = MESHTYPE
		if MESHID ~= "nil" and MESHID ~= "" then
			NEWMESH.MeshId = "http://www.roblox.com/asset/?id="..MESHID
		end
		if TEXTUREID ~= "nil" and TEXTUREID ~= "" then
			NEWMESH.TextureId = "http://www.roblox.com/asset/?id="..TEXTUREID
		end
	end
	NEWMESH.Offset = OFFSET or Vector3.new(0, 0, 0)
	NEWMESH.Scale = SCALE
	NEWMESH.Parent = PARENT
	return NEWMESH
end

local function CreatePart5(char, FORMFACTOR, PARENT, MATERIAL, REFLECTANCE, TRANSPARENCY, BRICKCOLOR, NAME, SIZE, ANCHOR)
	local NEWPART = Instance.new("Part")
	NEWPART.formFactor = FORMFACTOR
	NEWPART.Reflectance = REFLECTANCE
	NEWPART.Transparency = TRANSPARENCY
	NEWPART.CanCollide = false
	NEWPART.Locked = true
	NEWPART.Anchored = true
	if ANCHOR == false then
		NEWPART.Anchored = false
	end
	NEWPART.BrickColor = BrickColor.new(tostring(BRICKCOLOR))
	NEWPART.Name = NAME
	NEWPART.Size = SIZE
	NEWPART.Position = char.Torso.Position
	NEWPART.Material = MATERIAL
	NEWPART:BreakJoints()
	NEWPART.Parent = PARENT
	return NEWPART
end

function spat.WACKYEFFECT(swait,Table,char,Effects)
	local TYPE = (Table.EffectType or "Sphere")
	local SIZE = (Table.Size or Vector3.new(1,1,1))
	local ENDSIZE = (Table.Size2 or Vector3.new(0,0,0))
	local TRANSPARENCY = (Table.Transparency or 0)
	local ENDTRANSPARENCY = (Table.Transparency2 or 1)
	local CFRAME = (Table.CFrame or char.Torso.CFrame)
	local MOVEDIRECTION = (Table.MoveToPos or nil)
	local ROTATION1 = (Table.RotationX or 0)
	local ROTATION2 = (Table.RotationY or 0)
	local ROTATION3 = (Table.RotationZ or 0)
	local MATERIAL = (Table.Material or "Neon")
	local COLORLOOP = (Table.ColorLoop or false)
	local COLOR = (Table.Color or Color3.new(1,1,1))
	local TIME = (Table.Time or 45)
	local SOUNDID = (Table.SoundID or nil)
	local SOUNDPITCH = (Table.SoundPitch or nil)
	local SOUNDVOLUME = (Table.SoundVolume or nil)
	local USEBOOMERANGMATH = (Table.UseBoomerangMath or false)
	local BOOMERANG = (Table.Boomerang or 0)
	local SIZEBOOMERANG = (Table.SizeBoomerang or 0)
	local ndjsanfdsa,eueueu = coroutine.resume(coroutine.create(function()
		local PLAYSSOUND = false
		local SOUND = nil
		local EFFECT = CreatePart5(char, 3, Effects, MATERIAL, 0, TRANSPARENCY, BrickColor.new("Pearl"), "Effect", Vector3.new(1,1,1), true)
		--[[if SOUNDID ~= nil and SOUNDPITCH ~= nil and SOUNDVOLUME ~= nil then
			PLAYSSOUND = true
			SOUND = CreateSound5(SOUNDID, EFFECT, SOUNDVOLUME, SOUNDPITCH, false)
		end]]
		EFFECT.Color = COLOR
		local MSH = nil
		if TYPE == "Sphere" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "Sphere", "", "", SIZE, Vector3.new(0,0,0))
		elseif TYPE == "Block" or TYPE == "Box" then
			MSH = Instance.new("BlockMesh",EFFECT)
			MSH.Scale = SIZE
		elseif TYPE == "Wedge" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "Wedge", "", "", SIZE, Vector3.new(0,0,0))	
		elseif TYPE == "Wave" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "FileMesh", "20329976", "", SIZE, Vector3.new(0,0,-SIZE.X/8))
		elseif TYPE == "Ring" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "FileMesh", "559831844", "", Vector3.new(SIZE.X,SIZE.X,0.1), Vector3.new(0,0,0))
		elseif TYPE == "Slash" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "FileMesh", "662586858", "", Vector3.new(SIZE.X/10,0,SIZE.X/10), Vector3.new(0,0,0))
		elseif TYPE == "Round Slash" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "FileMesh", "662585058", "", Vector3.new(SIZE.X/10,0,SIZE.X/10), Vector3.new(0,0,0))
		elseif TYPE == "Swirl" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "FileMesh", "168892432", "", SIZE, Vector3.new(0,0,0))
		elseif TYPE == "Skull" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "FileMesh", "4770583", "", SIZE, Vector3.new(0,0,0))
		elseif TYPE == "Crystal" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "FileMesh", "9756362", "", SIZE, Vector3.new(0,0,0))
		elseif TYPE == "Heart" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "FileMesh", "105992239", "", SIZE, Vector3.new(0,0,0))
		elseif TYPE == "Note" then
			MSH = CreateMesh5("SpecialMesh", EFFECT, "FileMesh", "989516523", "", SIZE, Vector3.new(0,0,0))
		end
		if MSH ~= nil then
			local BOOMR1 = 1+BOOMERANG/50
			local BOOMR2 = 1+SIZEBOOMERANG/50
			local MOVESPEED = nil
			if MOVEDIRECTION ~= nil then
				if USEBOOMERANGMATH == true then
					MOVESPEED = ((CFRAME.p - MOVEDIRECTION).Magnitude/TIME)*BOOMR1
				else
					MOVESPEED = ((CFRAME.p - MOVEDIRECTION).Magnitude/TIME)
				end
			end
			local GROWTH = nil
			if USEBOOMERANGMATH == true then
				GROWTH = (SIZE - ENDSIZE)*(BOOMR2+1)
			else
				GROWTH = (SIZE - ENDSIZE)
			end
			local TRANS = TRANSPARENCY - ENDTRANSPARENCY
			EFFECT.CFrame = CFRAME
			if USEBOOMERANGMATH == true then
				for LOOP = 1, TIME+1 do
					swait()
					if COLORLOOP == true then
						EFFECT.Color = COLOR
					end
					MSH.Scale = MSH.Scale - (Vector3.new((GROWTH.X)*((1 - (LOOP/TIME)*BOOMR2)),(GROWTH.Y)*((1 - (LOOP/TIME)*BOOMR2)),(GROWTH.Z)*((1 - (LOOP/TIME)*BOOMR2)))*BOOMR2)/TIME
					if TYPE == "Wave" then
						MSH.Offset = Vector3.new(0,0,-MSH.Scale.Z/8)
					end
					EFFECT.Transparency = EFFECT.Transparency - TRANS/TIME
					EFFECT.CFrame = EFFECT.CFrame*CFrame.Angles(math.rad(ROTATION1),math.rad(ROTATION2),math.rad(ROTATION3))
					if MOVEDIRECTION ~= nil then
						local ORI = EFFECT.Orientation
						EFFECT.CFrame = CFrame.new(EFFECT.Position,MOVEDIRECTION)*CFrame.new(0,0,-(MOVESPEED)*((1 - (LOOP/TIME)*BOOMR1)))
						EFFECT.Orientation = ORI
					end
				end
			else
				for LOOP = 1, TIME+1 do
					swait()
					if COLORLOOP == true then
						EFFECT.Color = COLOR
					end
					MSH.Scale = MSH.Scale - GROWTH/TIME
					if TYPE == "Wave" then
						MSH.Offset = Vector3.new(0,0,-MSH.Scale.Z/8)
					end
					EFFECT.Transparency = EFFECT.Transparency - TRANS/TIME
					if TYPE == "Block" then
						EFFECT.CFrame = CFRAME*CFrame.Angles(math.rad(math.random(0,360)),math.rad(math.random(0,360)),math.rad(math.random(0,360)))
					else
						EFFECT.CFrame = EFFECT.CFrame*CFrame.Angles(math.rad(ROTATION1),math.rad(ROTATION2),math.rad(ROTATION3))
					end
					if MOVEDIRECTION ~= nil then
						local ORI = EFFECT.Orientation
						EFFECT.CFrame = CFrame.new(EFFECT.Position,MOVEDIRECTION)*CFrame.new(0,0,-MOVESPEED)
						EFFECT.Orientation = ORI
					end
				end
			end
			EFFECT.Transparency = 1
			if PLAYSSOUND == false then
				EFFECT:remove()
			else
				repeat swait() until EFFECT:FindFirstChildOfClass("Sound") == nil
				EFFECT:remove()
			end
		else
			if PLAYSSOUND == false then
				EFFECT:remove()
			else
				repeat swait() until EFFECT:FindFirstChildOfClass("Sound") == nil
				EFFECT:remove()
			end
		end
	end))
	if not ndjsanfdsa then
		print(eueueu)
	end
end

function spat.weirdeff(swait,name,startprop,startprop1,endprop,endprop1,timer,easing,easing2,repet,reverse,del,deb)
	local e = resources.effectstuff[name]:Clone()
	for i,v in pairs(e:GetChildren()) do
		--if v:IsA("Decal") then
		for d,g in pairs(startprop1) do
			v[d] = g
		end
		--end
	end
	for d,g in pairs(startprop) do
		e[d] = g
	end
	e.Anchored = true
	e.CanCollide = false
	e.Massless = true
	e.Name = "Effect"
	e.Parent = workspace
	TweenAnimateC(e,endprop,timer,easing,easing2,repet,reverse,del)
	for i,v in pairs(e:GetChildren()) do
		TweenAnimateC(v,endprop1,timer,easing,easing2,repet,reverse,del)
	end
	if not deb then
		coroutine.resume(coroutine.create(function()
			swait((del or 0)*60)
			swait((timer*((repet or 0)+1))*60)
			if reverse then
				swait((timer*((repet or 0)+1))*60)
			end
			e.Transparency = 1
			swait((1)*60)
			e:Destroy()
		end))
	end
	return e
end

function spat.particle(swait,pos,num,props,attPos)
	local part = pos
	if typeof(pos) == typeof(CFrame.new()) then
		part = Instance.new("Part")
		part.Anchored = true
		part.CFrame = pos
		part.Transparency = 1
		part.CanCollide = false
		part.Parent = workspace
	end
	local at = Instance.new("Attachment",part)
	at.Position = attPos or Vector3.new(0,0,0)
	local eff = Instance.new("ParticleEmitter",at)
	eff.Enabled = false
	eff.Speed = NumberRange.new(0)
	for i,v in pairs(props) do
		eff[i] = v
	end
	if num ~= 0 then
		coroutine.resume(coroutine.create(function()
			eff:Emit(num)
			wait(10)
			eff:Destroy()
		end))
	else
		eff.Enabled = true
		return eff
	end
end

local function lerpything(cf,alpha,cf2)
	return cf:lerp(cf2,alpha)
end

local function rang(x,y,z)
	return CFrame.Angles(math.rad(x),math.rad(y),math.rad(z))
end

local function rayCast(Pos, Dir, Max, Ignore)  -- Origin Position , Direction, MaxDistance , IgnoreDescendants
	return workspace:FindPartOnRay(Ray.new(Pos, Dir.unit * (Max or 999.999)), Ignore) 
end 

local lastW = 0
local lastA = 0

function spat.wings(swait,Mode,Wings,Aura,sine,handleweld,lwing1weld,lwing2weld,lwing3weld,rwing1weld,rwing2weld,rwing3weld,root,MAINRUINCOLOR,MAINRUINCOLOR2,kan,char,Effects)
	if Wings ~= lastW then
		local Mode = Wings
		local sine = sine
		lastW = Mode
		coroutine.resume(coroutine.create(function()
			while Mode == lastW do
				if Mode == 1 or Mode == 4 or Mode == 1111 or Mode == 78 or Mode == 500 or Mode == 9 or Mode == 10 or Mode == 59 or Mode == 11  or Mode == 100  or Mode == 15 or Mode == 18 or Mode == 19 or Mode == 20 or Mode == 22 or Mode == 24 or Mode == 58 or Mode == 102 or Mode == 103 or Mode == 9999 then -- Default wing anims
					handleweld.C0 = lerpything(handleweld.C0,.05,cf(0,0,-0.4)*angles(math.rad(0),math.rad(0),math.rad(0)),true)
					lwing1weld.C1 = lerpything(lwing1weld.C1,.05,cf(0,1.85 - 0.4 * math.sin(sine / 32),0)*angles(math.rad(0 + 2 * math.cos(sine / 32)),math.rad(0 - 2 * math.cos(sine / 36)),math.rad(90 - 0 * math.cos(sine / 32))))
					lwing2weld.C1 = lerpything(lwing2weld.C1,.05,cf(0,1.85 + 0.6 * math.sin(sine / 32),0)*angles(math.rad(0 + 2 * math.cos(sine / 32)),math.rad(0 - 2 * math.cos(sine / 37)),math.rad(130 + 15 * math.cos(sine / 32))))
					lwing3weld.C1 = lerpything(lwing3weld.C1,.05,cf(0,1.85 + 0.6 * math.sin(sine / 32),0)*angles(math.rad(0 + 2 * math.cos(sine / 32)),math.rad(0 - 2 * math.cos(sine / 51)),math.rad(50 - 15 * math.cos(sine / 32))))
					rwing1weld.C1 = lerpything(rwing1weld.C1,.05,cf(0,1.85 - 0.4 * math.sin(sine / 32),0)*angles(math.rad(0 + 2 * math.cos(sine / 32)),math.rad(0 - 2 * math.cos(sine / 36)),math.rad(-90 + 0 * math.cos(sine / 32))))
					rwing2weld.C1 = lerpything(rwing2weld.C1,.05,cf(0,1.85 + 0.6 * math.sin(sine / 32),0)*angles(math.rad(0 + 2 * math.cos(sine / 32)),math.rad(0 - 2 * math.cos(sine / 37)),math.rad(-130 - 15 * math.cos(sine / 32))))
					rwing3weld.C1 = lerpything(rwing3weld.C1,.05,cf(0,1.85 + 0.6 * math.sin(sine / 32),0)*angles(math.rad(0 + 2 * math.cos(sine / 32)),math.rad(0 - 2 * math.cos(sine / 51)),math.rad(-50 + 15 * math.cos(sine / 32))))

				elseif Mode == 7 or Mode == 2  then -- s p i n
					handleweld.C0 = lerpything(handleweld.C0,.05,cf(0,0,-0.4)*angles(math.rad(0),math.rad(0),math.rad(0)),true)
					lwing1weld.C1 = lerpything(lwing1weld.C1,.05,cf(0,1.85,0)*angles(math.rad(0),math.rad(0),math.rad(90 + 7*sine)))
					lwing2weld.C1 = lerpything(lwing2weld.C1,.05,cf(0,1.85,0)*angles(math.rad(0),math.rad(0),math.rad(150 + 7*sine)))
					lwing3weld.C1 = lerpything(lwing3weld.C1,.05,cf(0,1.85,0)*angles(math.rad(0),math.rad(0),math.rad(30 + 7*sine)))
					rwing1weld.C1 = lerpything(rwing1weld.C1,.05,cf(0,1.85,0)*angles(math.rad(0),math.rad(0),math.rad(-90 + 7*sine)))
					rwing2weld.C1 = lerpything(rwing2weld.C1,.05,cf(0,1.85,0)*angles(math.rad(0),math.rad(0),math.rad(-150 + 7*sine)))
					rwing3weld.C1 = lerpything(rwing3weld.C1,.05,cf(0,1.85,0)*angles(math.rad(0),math.rad(0),math.rad(-30 + 7*sine)))

				elseif Mode == 5 then
					handleweld.C0 = lerpything(handleweld.C0,.05,cf(0,0,3)*angles(math.rad(0),math.rad(0),math.rad(0)),true)
					lwing1weld.C1 = lerpything(lwing1weld.C1,.05,angles(math.rad(180),0,0)*cf(0,5 - 0.4 * math.sin(sine / 32),0)*angles(math.rad(0 + 2 * math.cos(sine / 32)),math.rad(0 - 2 * math.cos(sine / 36)),math.rad(90 - 0 * math.cos(sine / 32))))
					lwing2weld.C1 = lerpything(lwing2weld.C1,.05,angles(math.rad(180),0,0)*cf(0,5 + 0.6 * math.sin(sine / 32),0)*angles(math.rad(0 + 2 * math.cos(sine / 32)),math.rad(0 - 2 * math.cos(sine / 37)),math.rad(130 + 15 * math.cos(sine / 32))))
					lwing3weld.C1 = lerpything(lwing3weld.C1,.05,angles(math.rad(180),0,0)*cf(0,5 + 0.6 * math.sin(sine / 32),0)*angles(math.rad(0 + 2 * math.cos(sine / 32)),math.rad(0 - 2 * math.cos(sine / 51)),math.rad(50 - 15 * math.cos(sine / 32))))
