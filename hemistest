			msh.TextureId = "rbxassetid://4676130325"
			local weldo = CreateWeldS(parb,parb,AXE)
			AXE.Parent = targetted.Parent.Head
			parb.Parent = char
			-- ~-~ AFFEX1 ~-~
			local Alpha,Easing,Direction,Repeat,Reverse,Delay = 30/60,'Quad','InOut',0,false,0
			local eg = TweenAnimate(RootJoint,{C0=cf(-0.1,-1.3,-0.1)*angles(math.rad(-49.6),math.rad(-19.9),math.rad(-3))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(LH,{C0=cf(-1.2,-1.1,-0.3)*angles(math.rad(-42.7),math.rad(22.6),math.rad(-0.3))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
			TweenAnimate(RH,{C0=cf(1.1,-0.8,-0.8)*angles(math.rad(-24.6),math.rad(12.3),math.rad(28.6))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
