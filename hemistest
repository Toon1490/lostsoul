local hemis = game:GetObjects("rbxassetid://5090401326")[1].Hemispheric



-- ui edits (sorry)

local tabs = hemis.LocalScript.this.frameo.mainFrame.text1
local div = tabs.divider2:Clone()
local conv = tabs.credits:Clone()
tabs.lore.Size = UDim2.new(.14,0,.125,0)
tabs.credits.Size = UDim2.new(.263,0,.125,0)
tabs.credits.Position = UDim2.new(.47,0,0,0)
for i,v in pairs(tabs:GetChildren()) do
	if v.Name == "divider2" and v.Position == UDim2.fromScale(.66,.025) then
		v.Position = UDim2.new(.47,0,.025,0)
	end
end

conv.Size = UDim2.new(.22,0,.125,0)
conv.Position = UDim2.new(.733,0,0,0)
conv.Text = "Converts"
conv.Name = "conv"
conv.Parent = tabs
div.Position = UDim2.new(.733,0,.025,0)
div.Parent = tabs




-- star wing union to model




local qw = hemis:WaitForChild("Resources").WingModels.Star:FindFirstChildOfClass("Script")													-- retar- moment
qw.Parent = nil
hemis:WaitForChild("Resources").WingModels.Star:Destroy()

