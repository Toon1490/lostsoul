--WACKYEFFECT({EffectType = "", Size = VT(1,1,1), Size2 = VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = CF(), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1), SoundID = nil, SoundPitch = nil, SoundVolume = nil})
function WACKYEFFECT(Table)
	Whatever("WACKYEFFECT",Table,char,Effects)
	--animThing.WACKYEFFECT(Table,char,Effects)
end

function randr()
	return CFrame.Angles(math.rad(math.random(1,360)),math.rad(math.random(1,360)),math.rad(math.random(1,360)))
end

function soundtopos(pos,id,pitch,vol)
	if typeof(pos) == "Vector3" then pos = cf(pos) end
	local spart = CreatePart5(3,workspace,"Plastic",0,1,"White","so",Vector3.new(1,1,1),true)
	spart.CFrame = pos
	CreateSound5(id,spart,vol,pitch,false)
	game:GetService("Debris"):AddItem(spart,.05)
	return spart
end

-- Artificial Heartbeat --
--[[local ArtificialHB = Instance.new("BindableEvent")
ArtificialHB.Name = "Heartbeat"

local tf = 0
local allowframeloss = false
local tossremainder = false
local lastframe = tick()
local frame = 1/60
ArtificialHB:Fire()

runService.Heartbeat:Connect(function(s, p)
	tf = tf + s
	if tf >= frame then
		if allowframeloss then
			ArtificialHB:Fire()
			lastframe = tick()
		else
			for i = 1, math.floor(tf / frame) do
				ArtificialHB:Fire()
			end
			lastframe = tick()
		end
		if tossremainder then
			tf = 0
		else
			tf = tf - frame * math.floor(tf / frame)
		end
	end
end)

function swait(num)
	if num == 0 or num == nil then
		ArtificialHB.Event:wait()
	else
		for i = 0, num do
			ArtificialHB.Event:wait()
		end
	end
end]]
-- End of Artificial Heartbeat --


function CreateSpecialGlowMesh(parent,meshid,x1,y1,z1)
	local mesh = Instance.new("SpecialMesh",parent)
	mesh.MeshType = "FileMesh"
	mesh.MeshId = meshid
	mesh.TextureId = "http://www.roblox.com/asset/?id=269748808"
	mesh.Scale = Vector3.new(x1,y1,z1)
	mesh.VertexColor = Vector3.new(parent.BrickColor.r, parent.BrickColor.g, parent.BrickColor.b)
	return mesh
end
--

local wings = resources.WingModels
wings.Parent = nil

local moves = {}

wingstyle = "f"

function loadwings(wingRes)
	wingstyle = wingRes.Name
	m:ClearAllChildren()
	mw1:ClearAllChildren()
	mw2:ClearAllChildren()	

	handlex = CreateParta(mw1,1,1,"Neon",maincolor)
	CreateMesh(handlex,"Brick",0,0,0)
	handlexweld = CreateWeld(handlex,Torso,handlex,0,0,-0.5,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	handle = CreateParta(m,1,1,"Neon",maincolor)
	CreateMesh(handle,"Brick",0.5,0.5,0.5)
	handleweld = CreateWeld(handle,handlex,handle,0,0,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	lwing1 = CreateParta(m,1,1,"Neon",maincolor)
	CreateMesh(handle,"Brick",0.5,0.5,0.5)
	lwing1weld = CreateWeld(lwing1,handle,lwing1,1,0,0,math.rad(5),math.rad(0),math.rad(12.5),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	local ae = wingRes:Clone()
	ae.Parent = mw1
	for i,v in pairs(ae:GetDescendants())do
		if v:IsA("BasePart")then
			v.Anchored = false
			v.Massless = true
		end
	end
	CreateWeld(ae.Main,lwing1,ae.Main,0,0,-1.75,math.rad(0),math.rad(0),math.rad(90),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	lwing2 = CreateParta(m,1,1,"Neon",maincolor)
	CreateMesh(handle,"Brick",0.5,0.5,0.5)
	lwing2weld = CreateWeld(lwing2,handle,lwing2,1,1,0,math.rad(10),math.rad(0),math.rad(25),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	local ae = wingRes:Clone()
	ae.Parent = mw1
	for i,v in pairs(ae:GetDescendants())do
		if v:IsA("BasePart")then
			v.Anchored = false
			v.Massless = true
		end
	end
	CreateWeld(ae.Main,lwing2,ae.Main,0,0,-1.75,math.rad(0),math.rad(0),math.rad(90),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	lwing3 = CreateParta(m,1,1,"Neon",maincolor)
	CreateMesh(handle,"Brick",0.5,0.5,0.5)
	lwing3weld = CreateWeld(lwing3,handle,lwing3,1,2,0,math.rad(15),math.rad(0),math.rad(37.5),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	local ae = wingRes:Clone()
	ae.Parent = mw1
	for i,v in pairs(ae:GetDescendants())do
		if v:IsA("BasePart")then
			v.Anchored = false
			v.Massless = true
		end
	end
	CreateWeld(ae.Main,lwing3,ae.Main,0,0,-1.75,math.rad(0),math.rad(0),math.rad(90),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	-- Right wing.

	rwing1 = CreateParta(m,1,1,"Neon",maincolor)
	CreateMesh(handle,"Brick",0.5,0.5,0.5)
	rwing1weld = CreateWeld(rwing1,handle,rwing1,-1,0,0,math.rad(5),math.rad(0),math.rad(-12.5),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	local ae = wingRes:Clone()
	ae.Parent = mw2
	for i,v in pairs(ae:GetDescendants())do
		if v:IsA("BasePart")then
			v.Anchored = false
			v.Massless = true
		end
		if aaaa["colroes"] then
			if v.Name == "Color1" then
				v.Name = "Color2"
			elseif v.Name == "Color2" then
				v.Name = "Color1"
			end
		end
	end
	CreateWeld(ae.Main,rwing1,ae.Main,0,0,-1.75,math.rad(0),math.rad(0),math.rad(90),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	rwing2 = CreateParta(m,1,1,"Neon",maincolor)
	CreateMesh(handle,"Brick",0.5,0.5,0.5)
	rwing2weld = CreateWeld(rwing2,handle,rwing2,-1,1,0,math.rad(10),math.rad(0),math.rad(-25),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	local ae = wingRes:Clone()
	ae.Parent = mw2
	for i,v in pairs(ae:GetDescendants())do
		if v:IsA("BasePart")then
			v.Anchored = false
			v.Massless = true
		end
		if aaaa["colroes"] then
			if v.Name == "Color1" then
				v.Name = "Color2"
			elseif v.Name == "Color2" then
				v.Name = "Color1"
			end
		end
	end
	CreateWeld(ae.Main,rwing2,ae.Main,0,0,-1.75,math.rad(0),math.rad(0),math.rad(90),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	rwing3 = CreateParta(m,1,1,"Neon",maincolor)
	CreateMesh(handle,"Brick",0.5,0.5,0.5)
	rwing3weld = CreateWeld(rwing3,handle,rwing3,-1,2,0,math.rad(15),math.rad(0),math.rad(-37.5),0,0,0,math.rad(0),math.rad(0),math.rad(0))

	local ae = wingRes:Clone()
	ae.Parent = mw2
	for i,v in pairs(ae:GetDescendants())do
		if v:IsA("BasePart")then
			v.Anchored = false
			v.Massless = true
		end
		if aaaa["colroes"] then
			if v.Name == "Color1" then
				v.Name = "Color2"
			elseif v.Name == "Color2" then
				v.Name = "Color1"
			end
		end
	end
	CreateWeld(ae.Main,rwing3,ae.Main,0,0,-1.75,math.rad(0),math.rad(0),math.rad(90),0,0,0,math.rad(0),math.rad(0),math.rad(0))
end

function burst(cl1,cl2,size)
	attack = true
	idlesoff = true
	char.Humanoid.WalkSpeed = 0
	local flying = (RootJoint.C0.Y >= 1)
	CreateSound5("2923164079",root,1.5,1.4,false)
	local pitch = TweenAnimate(kan,{PlaybackSpeed=0},210/60,"Sine","In")
	local Alpha,Easing,Direction,Repeat,Reverse,Delay = 30/60,'Quad','Out',0,false,0
	if flying or moves["gliding"] then
		local eg = TweenAnimate(RootJoint,{C0=cf(0,1.8,-0.1)*angles(math.rad(-15),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1.1,-1.1,-0.1)*angles(math.rad(-10),math.rad(3.5),math.rad(6.1))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1.1,-0.3,-0.7)*angles(math.rad(-9.9),math.rad(-2.4),math.rad(-6.6))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LW,{C0=cf(-0.7,0.2,-0.7)*angles(math.rad(115.5),math.rad(0.7),math.rad(45.9))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RW,{C0=cf(0.8,0.6,-0.9)*angles(math.rad(111),math.rad(-18.1),math.rad(-53.5))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(Torso.Neck,{C0 =cf(0,0.1,0.2)*angles(math.rad(-15),math.rad(0),math.rad(0))*necko},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		swait(Alpha*60)
		local Alpha,Easing,Direction,Repeat,Reverse,Delay = 10/60,'Quint','In',0,false,0
		local eg = TweenAnimate(RootJoint,{C0=cf(0,1.5,0)*angles(math.rad(20),math.rad(0),math.rad(0))*RootCF},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(LH,{C0=cf(-1,-0.9,-0.1)*angles(math.rad(-34.3),math.rad(4.3),math.rad(-3.9))*angles(math.rad(0),math.rad(-90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
		TweenAnimate(RH,{C0=cf(1,-0.6,-0.6)*angles(math.rad(-19.7),math.rad(-0.6),math.rad(3.2))*angles(math.rad(0),math.rad(90),math.rad(0))},Alpha,Easing,Direction,Repeat,Reverse,Delay)
