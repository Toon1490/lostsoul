		CFrame = h*CFrame.new(0,7,0)*angles(0,math.rad(185),math.rad(29))*cf(0,0,0.3)
	},{ -- decal start properties
		Color3=Color3.fromRGB((MAINRUINCOLOR2.R*50)^1.85,(MAINRUINCOLOR2.G*50)^1.85,(MAINRUINCOLOR2.B*50)^1.85),
		Transparency = -1.5,
		Texture = "rbxassetid://4049743792"
	},{ -- part end properties
